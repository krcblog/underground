													GetScheduledStart(),				// scheduled start
													GetScheduledDuration(),				// duration
													GetScheduledFinish(),				// scheduled finish
													GetScheduledFinish(),				// Display finish
													GetActualStart(),					// actual start
													GetActualFinish(),					// actual finish
													GetIsPrivate(),						// is private
													true,								// is read
													GetDescription(),					// description element
