					if (GrooveCalendarViewType_Day == ViewType)
						HandleDayViewMode(ViewType, false);
					else if (GrooveCalendarViewType_NormalWeek <= ViewType && ViewType <= GrooveCalendarViewType_WorkWeek)
						HandleWeekViewMode(ViewType, false);
					else if (GrooveCalendarViewType_Month <= ViewType && ViewType <= GrooveCalendarViewType_MonthWithStackedWeekends)
						HandleMonthViewMode(ViewType, false);
					}
				}

