					function PredecessorList_OnListViewCheckChanged(i_Name, i_Index, i_Checked)
					{
						if(true == i_Checked && false == ValidatePredecessorsForTask(-1 == g_TaskID))
						{
							PredecessorList.SetCheckByIndex(i_Index, GrooveCheckState_Unchecked);
							PHF_AlertUser(GPTPROJECT_MB_PREDECESSOR_CAUSES_CYCLE);
