					function UserStartDateValueEdit_OnKillFocus(i_Name)
					{
						if(ValidateStartDate(false) && ValidateDuration())
						{
							RefreshFinishDate();
						}
					}

					function ScheduledDurationValueEdit_OnKillFocus(i_Name)
					{
						ScheduledDurationValueEdit.Text = TrimHead(ScheduledDurationValueEdit.Text);
						if(ValidateDuration() && ValidateStartDate(false))
						{
							RefreshFinishDate();
						}
					}

					function ValidateActualAndFinishDate(i_PopAlert)
					{
						// If the item is not a summary task, the status and actual start fields can
						// be modified
						if(false == g_CurrentItem.IsSummary)
						{
							// Validate actual start/finish
							var Status = MHF_GetStatusEnumFromString(StatusValueEdit.GetSelectionText());

							// Actual start and Actual finish
							if(Status == GrooveProjectToolsetStatusType_Complete ||
							   Status == GrooveProjectToolsetStatusType_InProgress)
							{
								if(isNaN(ParseDayAndDate(ActualStartDateValueEdit.Text,
														 GrooveIntlDateFormatStyle_Short)))
								{
									if (i_PopAlert)
										PHF_AlertUser(GPTPROJECT_MB_ENTER_VALID_ACTUALSTART_DATE);

									EditItemDetailsViewContainer.SetFocus("ActualStartDateValueEdit");
									ActualStartDateValueEdit.SetSelection(0, ActualStartDateValueEdit.Text.length);
									return false;
								}

								if(isNaN(ParseDayAndDate(ActualFinishDateValueEdit.Text,
													     GrooveIntlDateFormatStyle_Short)))
								{
									if (i_PopAlert)
										PHF_AlertUser(GPTPROJECT_MB_ENTER_VALID_ACTUALFINISH_DATE);

									EditItemDetailsViewContainer.SetFocus("ActualFinishDateValueEdit");
									ActualFinishDateValueEdit.SetSelection(0, ActualFinishDateValueEdit.Text.length);
									return false;
								}
							}
						}

						return true;
					}

					function ValidateStartDate(i_PopAlert)
					{
						var UserStart = ParseDayAndDate(UserStartDateValueEdit.Text,
														GrooveIntlDateFormatStyle_Short);

						// Validate start date
						if(isNaN(UserStart))
						{
							if(i_PopAlert)
								PHF_AlertUser(GPTPROJECT_MB_ENTER_VALID_START_DATE);
							UserStartDateValueEdit.SetSelection(0, UserStartDateValueEdit.Text.length);
							return false;
						}

						if((g_ProjectStartTime < 0 || UserStart < g_ProjectStartTime) && ASAPRadioButton.CheckState != GrooveCheckState_Checked)
						{
							if(i_PopAlert)
								PHF_AlertUser(GPTPROJECT_MB_START_DATE_AFTER_PROJECT_DATE);
							return false;
						}

						// move start constraint to a work day if necessary
						var WorkWeekUserStart = g_ProjectDMD.GetStartDateInWorkWeek(UserStart);
						if(WorkWeekUserStart != UserStart)
						{
							var UpdatedUserStart = FormatDayAndDate(WorkWeekUserStart,
																	GrooveIntlDateFormatStyle_Short)
							UserStartDateValueEdit.Text = UpdatedUserStart;
							PHF_AlertUser(GPTPROJECT_MB_START_MOVED_TO_WORKDAY + UpdatedUserStart + GPTPROJECT_MB_DOT);
						}

						return true;
					}

					function ValidateDuration()
					{
						if (isNaN(ScheduledDurationValueEdit.Text) || ScheduledDurationValueEdit.Text == "" ||
						    (ScheduledDurationValueEdit.Text % 1) > 0 || (ScheduledDurationValueEdit.Text >= 10000) || 
							ScheduledDurationValueEdit.Text < 0)
						{
							return false;
						}

						return true;
					}

					function RefreshFinishDate(i)
					{
						var EndDate = MHF_GetDisplayFinishDateMS(GetScheduledDuration(), GetScheduledFinish());
						var FinishText = GPTPROJECT_S_LEFTPAREN +
										 GPTPROJECT_S_SCHEDULED_FINISH + 
										 FormatDayAndDate(EndDate, GrooveIntlDateFormatStyle_Short) +
										 GPTPROJECT_S_RIGHTPAREN;
						ScheduledFinishDateValueStatic.SetText(FinishText); 
					}

					function PredecessorsButton_OnCommand()
					{
						// only make the dialog the first time the button is pressed
						// because it may take a very  long time to get a full list
						// of all tasks in the project
						if(null == g_pIPredecessorWindow)
						{
							g_pIPredecessorWindow = PHF_CreateEditPredecessorDialog(EditPredecessorsSubForm);

							// Grab edit predecessor script dispatch
							var PredecessorDelegate = g_pIPredecessorWindow.OpenDelegateComponent();
							g_EditPredecessorsScriptDispatch = PredecessorDelegate.GetScriptDispatch();


							MHF_ShowWaitCursor(true);

							// Get the list of all tasks (with predecessor information)
							var PredecessorCache;
							var Telespace = PropertyList.OpenProperty(PROPERTY_TELESPACE);
							var Transaction = Telespace.OpenTransaction(true);
							try
							{
								PredecessorCache = PHF_GetTaskArray(g_ProjectDMD);

								Transaction.Commit();
							}
							catch(e)
							{
								MHF_ShowWaitCursor(false);
								Transaction.Abort();
								GrooveDebugFunctions.DisplayAssert(0, "Error refreshing predecessor list");
								return;
							}


							MHF_ShowWaitCursor(false);

							// Set the project data model delegate
							g_EditPredecessorsScriptDispatch.SetProjectDataModelDelegate(g_ProjectDMD);

							// Set the task
							g_EditPredecessorsScriptDispatch.InitializeDialogData
														(
															g_CurrentItem.ID,
															g_CurrentItem.Name,
															g_CurrentItem.OpenPredecessorEnum(),
															PredecessorCache
														);

							// Reset cached state, since we are dealing with a different task
							g_PredecessorSelectionState = null;
							g_PredecessorOriginalSelectionState = g_EditPredecessorsScriptDispatch.GetSelectionState();

							g_NewPredecessorsArray = null;
							g_RemovePredecessorsArray = null;
						}

						// Set selection state
						if(null != g_PredecessorSelectionState)
						{
							g_EditPredecessorsScriptDispatch.SetSelectionState(g_PredecessorSelectionState);
						}

						// Show modal dialog
						var nResult =  g_pIPredecessorWindow.DoModal();
						if(GrooveDialogBoxResultCode_OK == nResult)
						{
							// Remember user selection
							g_PredecessorSelectionState = g_EditPredecessorsScriptDispatch.GetSelectionState();
							g_PredecessorOriginalSelectionState = g_PredecessorSelectionState;

							// Remember the predecessor arrays incase we need them
							g_NewPredecessorsArray =  GetNewPredecessorsArray();
							g_RemovePredecessorsArray =  GetRemovePredecessorsArray();

							// Update UI
							PredecessorsButton.Label = PHF_GetPredecessorLabel(g_EditPredecessorsScriptDispatch.GetSelectionCount())
						}

						// User clicked [x]; window is destroyed
						else  if (-1 == nResult)
						{
							g_pIPredecessorWindow = null;
							g_EditPredecessorsScriptDispatch = null;
						}

						// User canceled; reset selection
						else
						{
							g_EditPredecessorsScriptDispatch.SetSelectionState(g_PredecessorOriginalSelectionState);
						}
					}

