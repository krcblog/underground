		CopyFolderContent(CopyFromFolder, LeftBack(XMLFilePath, "\\"));
	}

	function DoneButton_OnCommand(i_UICommand)
	{
		var Window = UIPropertyList.OpenProperty(UIPROPERTY_TOP_LEVEL_WINDOW);
		Window.Close(GrooveWindowResult_Cancel);
	}

	function RunReportButton_OnCommand(i_UICommand)
	{
		// Display selected report
		NavigateToNewReport();
	}

