//************************************************************
// Initialization & Termination
//************************************************************

	function Initialize()
	{
		SaveButton.Enabled = false;
		PrintButton.Enabled = false;
		g_FileServices = GrooveScriptFunctions.CreateNewObject("Groove.Win32FileSystemServices");
	}

	function Terminate()
	{
		g_CurXMLRootElem = null;
		if(g_FilterSubFormHost)
			DisconnectObject(g_FilterSubFormHost);
		DeleteXMLFile();
	}

	function OnSetUIProperties()
	{
		// Populate combo box with available report types
		PopulateReportTypeCombobox(g_DefaultReportFolder);

		// Select default report type, the first item in list.
		if (0 < ReportTypeComboBox.Count)
			ReportTypeComboBox.SetSelectionByIndex(0);

		// Clear display view
		PrintCtrl.Navigate("about:blank");
	}

	function OnViewContainerShow()
	{
	}

	function OnViewContainerHide()
	{
	}

