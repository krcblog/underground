                }

                function Print ()
                {
					var pCOS = null;
					try
					{
						// Format strings for printing
						var Footer = PropertyList.OpenProperty(PROPERTY_TOOL_DISPLAY_NAME) + "-" + PropertyList.OpenProperty(PROPERTY_TELESPACE_DISPLAY_NAME);

						// Setup print information and print
						pCOS = PrintObject.Create();
						PrintObject.WriteBody(pCOS, TextView);
						PrintObject.WriteFooter(pCOS, Footer, GroovePrintStyle_Report);
						PrintObject.IGroovePrintHelper.PrintFile(pCOS);
					}
					catch (error)
					{
						if (pCOS)
							pCOS.Close();
					}
                }

