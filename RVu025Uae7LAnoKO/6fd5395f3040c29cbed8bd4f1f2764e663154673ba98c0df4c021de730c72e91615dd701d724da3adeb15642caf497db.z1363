				function OnTranslateAccelerator (i_Message, i_KeyCode, i_KeyState, i_KeyModifiers)
				{
					// Setup key states
					var ShiftKeyDown = i_KeyModifiers & GROOVE_KEYMOD_SHIFT;
					var ControlKeyDown = i_KeyModifiers & GROOVE_KEYMOD_CONTROL;
					var ShiftKeyOnlyDown = ShiftKeyDown && !ControlKeyDown;
					var ControlKeyOnlyDown = !ShiftKeyDown && ControlKeyDown;

					if (i_Message == GROOVE_WM_KEYDOWN)
					{
						switch (i_KeyCode)
						{
							case GROOVE_VK_C:
							case GROOVE_VK_X:
							case GROOVE_VK_V:
							{
								if (HaveFocus && ControlKeyOnlyDown)
								{
			                        TextView.SubmitText (String.fromCharCode (i_KeyCode & 0x1F));
									return true;
								}
								break;
							}
						}
					}

					// Didn't handle it
					return false;
				}
                

                function OnConnectToExternalObject (i_Object, i_ConnectionID)
                {
                    if (i_ConnectionID == 0)
                    {
						GlobalUICommandContainer = i_Object.UICommandContainer3;
						GlobalUICommandContainerCookie = GlobalUICommandContainer.MergeUICommandContainer(
							NotepadMenu.UICommandContainer3, 
							ScriptHostComponent.IGrooveUICommandListener,
							GrooveScriptFunctions.CreateNewObject("")
							);
                        ShowMenuCommands();
                    }
                    else if (i_ConnectionID == 1)
                    {
                        ToolUICommandContainer = i_Object;
                        ConnectObject (ToolUICommandContainer, "ToolUICommandContainer");
                        ShowTextUICommands (ToolUICommandContainer);

                        StopTimer ();
                        ForcedUnread = false;
                        if (UnreadMarkEngine.IsItemUnread (TEXT_ROOT_ITEM_ID))
                            StartTimer ();
                    }
                }

                function OnUnconnectFromExternalObjects ()
                {
                    if (ToolUICommandContainer != null)
                    {
                        StopTimer ();
                        HideTextUICommands (ToolUICommandContainer);
						DisconnectObject (ToolUICommandContainer);
                        ToolUICommandContainer = null;
                    }
                    
					if (GlobalUICommandContainer != null)
                    
                    {
                        HideMenuCommands();
 						if (GlobalUICommandContainerCookie != 0)
							GlobalUICommandContainer.RemoveMergedUICommandContainer(GlobalUICommandContainerCookie);
						GlobalUICommandContainer = null;
                    }
                }

