					//
					// basically swiped from calendar
					//
					function GetScheduledDateFromPopup()
					{
						// Open window for subform
						var pIWindow = MHF_CreateDialogBox(MonthPopupSubForm, "", 210,280, false);
						if (!pIWindow.DoesDelegateComponentExist())
							return;

						var MonthPopupHost = pIWindow.OpenDelegateComponent();
						var MonthPopupCode = MonthPopupHost.GetScriptDispatch();
						var DefaultDateMS = ParseDayAndDate(UserStartDateValueEdit.Text,
															GrooveIntlDateFormatStyle_Short);

						if(isNaN(DefaultDateMS))
						{
							DefaultDateMS = new Date().valueOf();
						}

						MonthPopupCode.SetSelection(DefaultDateMS);

						if (GrooveDialogBoxResultCode_OK == pIWindow.DoModal())
						{
							var StartDate = MonthPopupCode.GetStartDate();

						    // Make sure stat date is proper relative to project date.
							if(g_ProjectStartTime > 0 && StartDate >= g_ProjectStartTime)
							{
								UserStartDateValueEdit.Text = FormatDayAndDate(StartDate,
																			   GrooveIntlDateFormatStyle_Short);
							}
							else
							{
								PHF_AlertUser(GPTPROJECT_MB_START_DATE_AFTER_PROJECT_DATE);
								return;
							}

							// Calculate duration
							var EndDate = MonthPopupCode.GetEndDate();
							if (null != EndDate && StartDate != EndDate)
							{
								ScheduledDurationValueEdit.Text =  g_ProjectDMD.GetWorkWeekDuration(StartDate, EndDate)/MSPERDAY + 1;
							}
						}
					}

