	function SetDefaultReportFolder(i_DefaultReportFolder)
	{
		g_DefaultReportFolder = i_DefaultReportFolder;
	}

	function SetXMLDocument(i_XMLDocument, i_ReportPath, i_XSLFileName)
	{
		g_XSLFileName = i_XSLFileName;
		SaveButton.Enabl␀ð ␀␀␀␀␀␀␀␀␀ed = false;
		PrintButton.Enabled = false;
		g_bCleanUp = false;

		try
		{
			if(i_XMLDocument == null && g_CachedXMLDoc != null)
				i_XMLDocument = g_CachedXMLDoc;

			if(i_XMLDocument != null && i_ReportPath != "" && g_XSLFileName != "")
			{
				g_CurXMLRootElem = i_XMLDocument.OpenRootElement();
				g_NavigateToURL = i_ReportPath + "\\" + g_CurXMLRootElem.OpenName() + ".xml";

				g_CachedXMLDoc = i_XMLDocument;
				SaveElement2File(g_CurXMLRootElem, g_NavigateToURL);
				var pTemporaryFileServices = GrooveScriptFunctions.CreateNewObject("Groove.TemporaryFileServices");
				pTemporaryFileServices.TemporaryFileServices2.DeleteTemporaryPath(g_NavigateToURL, GrooveTemporaryFileDeleteFlag_TimerDelay, PropertyList.OpenProperty(PROPERTY_ACCOUNT_GUID));
				PrintCtrl.Navigate(g_NavigateToURL);
				g_bCleanUp = true;
			}
		}
		catch(error)
		{
			GrooveDebugFunctions.DisplayAssert(0, error.description);
		}
	}

	function SetFilterLayout(i_FilterSubForm)
	{
		g_FilterSubFormHost = i_FilterSubForm.OpenDelegateComponent();
		g_ConfigurationSubFormCode = g_FilterSubFormHost.GetScriptDispatch();

		var pForm = PropertyList.OpenProperty(PROPERTY_FORM);
		var pComponentContainer = pForm.ComponentContainer;
		var ParentConfigurationLayout = pComponentContainer.OpenComponentByName("CustomView").Layout;

		var HTMLTableSize = GrooveScriptFunctions.CreateNewObject("Groove.HTMLTableLayoutControlInfo");
		var HTMLTableCell = HTMLTableSize.Cell;
		HTMLTableCell.SetCell(0,0);
		ParentConfigurationLayout.RemoveCellContents(HTMLTableCell);
		ParentConfigurationLayout.AddControlByComponent("CustomConfigurationPane", i_FilterSubForm, HTMLTableCell);

		ConnectObject(g_FilterSubFormHost, "PrintFilterSubForm_");
	}

