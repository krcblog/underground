						//
						// When this function is called, we are always guaranteed
						// to be inside of a read transaction
						//
						function OnRecordOpened
							(
							i_pIRecord,
							i_CurrentView,
							i_CurrentSort,
							i_RowType,
							i_SynthType
							)
						{
							try
							{
								// Determine the record type so we can treat the project record differently
								var RecordType = "urn:groove.net:ProjectTaskRecord";


								if(true == RecordSetEngine.HasRecord(i_pIRecord.RecordID))
								{
									var pIActualRecord = RecordSetEngine.OpenRecord(i_pIRecord.RecordID);
									RecordType = pIActualRecord.Type;
								}

								// If the task is a leaf node show the assigned-to and priority fields.
								if("urn:groove.net:ProjectTaskRecord" == RecordType &&		// Task
									false == i_pIRecord.OpenField("_HasChildren"))			// Leaf node

								{
									var ContactURL = i_pIRecord.OpenField("AssignedToContactURL");

									// Resolve contact
									if("" == ContactURL || null == ContactURL)
									{
										i_pIRecord.SetField("AssignedToContactURL", GrooveRunTimeData.GetItem("GPT_S_UNASSIGNED"));
									}
									else if (null == ContactProvider ||
											 (null != ContactProvider && false == ContactProvider.DoesContactExist(ContactURL)))
									{
										i_pIRecord.SetField("AssignedToContactURL", GrooveRunTimeData.GetItem("GPT_S_MISSINGCONTACT"));
									}
									else
									{
										var Contact = ContactProvider.OpenContact(ContactURL);
										i_pIRecord.SetField("AssignedToContactURL", CHF_GetDisplayNameForContact(Contact));
									}

									// Resolve priority
									i_pIRecord.SetField("Priority", DHF_GetPriorityStringFromEnum(i_pIRecord.OpenField("Priority")));
								}
								else // Otherwise, blank them out
								{
									i_pIRecord.SetField("AssignedToContactURL", "");
									i_pIRecord.SetField("Priority", "");
								}

								// Adjust project description so that user will know what kind of task this is.
								if ("urn:groove.net:ProjectProjectRecord" == RecordType)
								{
									var Description = i_pIRecord.OpenField("Name") + " " + GPT_S_PROJECTNAME_TAG;
									i_pIRecord.SetField("Name", Description);	
								}

								// Resolve start date
								i_pIRecord.SetField("ScheduledStart", FormatDayAndDate(i_pIRecord.OpenField("ScheduledStart"),
																					   GrooveIntlDateFormatStyle_Short)
													);

								// Set display date
								i_pIRecord.SetField("DisplayFinish", FormatDayAndDate(i_pIRecord.OpenField("DisplayFinish"),
																					  GrooveIntlDateFormatStyle_Short)
													);

								// Resolve status
								i_pIRecord.SetField("Status", DHF_GetStatusStringFromEnum(i_pIRecord.OpenField("Status")));
							}
							catch(e)
							{
								PDHF_SetError(PROJECTTRACKING_E_OPERATION_FAILED, "Error updating item");
							}
						}
