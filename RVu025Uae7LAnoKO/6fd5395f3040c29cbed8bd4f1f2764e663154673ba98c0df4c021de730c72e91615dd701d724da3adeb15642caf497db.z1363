					function LayoutActualDatesForTask(i_Status)
					{
						// verify that the edit controls have been sited
						SiteControl(EditItemDetailsViewLayout, "ActualStartDateValuePlaceholder", "ActualStartDateValueEdit");
						SiteControl(EditItemDetailsViewLayout, "ActualStartPopupButtonPlaceholder", "ActualStartPopupButton");
						SiteControl(EditItemDetailsViewLayout, "ActualFinishDateValuePlaceholder", "ActualFinishDateValueEdit");
						SiteControl(EditItemDetailsViewLayout, "ActualFinishPopupButtonPlaceholder", "ActualFinishPopupButton");

						switch(i_Status)
						{
							case GrooveProjectToolsetStatusType_Complete:
								ActualStartLabel.Visible = true;
								ActualStartDateValueEdit.Visible = true;
								ActualStartPopupButton.Visible = true;

								ActualFinishLabel.Visible = true;
								ActualFinishDateValueEdit.Visible = true;
								ActualFinishPopupButton.Visible = true;
								break;
							case GrooveProjectToolsetStatusType_InProgress:
								ActualStartLabel.Visible = true;
								ActualStartDateValueEdit.Visible = true;
								ActualStartPopupButton.Visible = true;

								ActualFinishLabel.Visible = false;
								ActualFinishDateValueEdit.Visible = false;
								ActualFinishPopupButton.Visible = false;
								break;
							default:
								ActualStartLabel.Visible = false;
								ActualStartDateValueEdit.Visible = false;
								ActualStartPopupButton.Visible = false;

								ActualFinishLabel.Visible = false;
								ActualFinishDateValueEdit.Visible = false;
								ActualFinishPopupButton.Visible = false;
								break;
						}
					}


					function LayoutActualDatesForSummaryTask(i_Status)
					{
						// verify that only the static controls have been sited
						SiteControl(EditItemDetailsViewLayout, "ActualStartDateValuePlaceholder", "ActualStartDateValueStatic");
						RemoveControl(EditItemDetailsViewLayout, "ActualStartPopupButton");
						SiteControl(EditItemDetailsViewLayout, "ActualFinishDateValuePlaceholder", "ActualFinishDateValueStatic");
						RemoveControl(EditItemDetailsViewLayout, "ActualFinishPopupButton");

						switch(i_Status)
						{
							case GrooveProjectToolsetStatusType_Complete:
								ActualStartLabel.Visible = true;
								ActualStartDateValueStatic.Visible = true;

								ActualFinishLabel.Visible = true;
								ActualFinishDateValueStatic.Visible = true;
								break;
							case GrooveProjectToolsetStatusType_InProgress:
								ActualStartLabel.Visible = true;
								ActualStartDateValueStatic.Visible = true;

								ActualFinishLabel.Visible = false;
								ActualFinishDateValueStatic.Visible = false;
								break;
							default:
								ActualStartLabel.Visible = false;
								ActualStartDateValueStatic.Visible = false;

								ActualFinishLabel.Visible = false;
								ActualFinishDateValueStatic.Visible = false;
								break;
						}
					}

					function StatusValueEdit_OnSelectionChange(i_Name, i_Item)
					{
						var NewStatus = MHF_GetStatusEnumFromString(i_Item);
						var CurrentDate = new Date();

						// if this is the first time the progress has been advanced (since saved),
						// generate a new default date
						switch(NewStatus)
						{
							case GrooveProjectToolsetStatusType_Complete:
								// set finish date
								if(GrooveProjectToolsetStatusType_InProgress == g_CurrentItem.Status ||
								   GrooveProjectToolsetStatusType_NotStarted == g_CurrentItem.Status)
								{
									ActualFinishDateValueEdit.Text = FormatDayAndDate(CurrentDate.valueOf(),
																					  GrooveIntlDateFormatStyle_Short);
								}

								// set start date if we had not been started yet
								if(GrooveProjectToolsetStatusType_NotStarted == g_CurrentItem.Status)
								{
									ActualStartDateValueEdit.Text = FormatDayAndDate(CurrentDate.valueOf(),
																					 GrooveIntlDateFormatStyle_Short);
								}
								break;
							case GrooveProjectToolsetStatusType_InProgress:
								if(GrooveProjectToolsetStatusType_NotStarted == g_CurrentItem.Status)
								{
									ActualStartDateValueEdit.Text = FormatDayAndDate(CurrentDate.valueOf(),
																					 GrooveIntlDateFormatStyle_Short);
								}
								break;
							default:
								break;
						}

						if(i_Item.IsSummary)
						{
							LayoutActualDatesForSummaryTask(NewStatus);
						}
						else
						{
							LayoutActualDatesForTask(NewStatus);
						}
					}

