				// String to localize
				var g_cSTR_MESSAGE_BOX_CAPTION = "Project";
				var g_cSTR_IMPORT_CAPTION = "Import from Microsoft Project";
				var g_cSTR_EXPORT_CAPTION = "Export to Microsoft Project";
				var g_cSTR_EXPORT_TO_CAPTION = "Export Groove Project To...";
				var g_cSTR_IMPORT_FROM_CAPTION = "Import Microsoft Project File From...";
				var g_cSTR_FILTER_STRING = "Microsoft Project Files (*.mpp)|*.mpp||";

