				function ExportToRTF()
				{
				// Find the directory to use
				var Directory;
				if (PropertyList.PropertyExists(g_LAST_DIRECTORY))
					Directory = PropertyList.OpenProperty(g_LAST_DIRECTORY);
				else
					{
					var pWin32FileSystemServices = GrooveScriptFunctions.CreateNewObject("Groove.Win32FileSystemServices");
					Directory = pWin32FileSystemServices.OpenPersonalFilesFolderPath();
					}

				// Prompt user for file
				var strFilter = "Rich Text Format (*.rtf)|*.rtf";
				var FileResult = App.SaveFileDialog("Export to File...", strFilter, Directory, g_INITIAL_FILE, pAccount, 1, false);
				if (GrooveDialogBoxResultCode_OK == FileResult.Result)
					{
					var pWindowsManager = GrooveScriptFunctions.CreateNewObject("Groove.WindowManager");
					pWindowsManager.ShowWaitCursor();
					
					try
						{
						// Remember directory for next time.
						PropertyList.SetProperty(g_LAST_DIRECTORY, FileResult.FileDialogResult.Directory);

						// Create the file
						var pBOS = GROOVE_GLOBAL_MISC_POINTER.StreamFactory.CreateByteOutputStreamFromFile(FileResult.Data, false, false);
						var pCOS = pBOS.OpenCharOutputStream(GrooveCharEncodingType_UTF8);

						// Write contents to file
						pCOS.WriteString(TextView.RTFContent);

						// Flush and close the file
						pCOS.Flush();
						pCOS.Close();

						// Dismiss the wait cursor
						pWindowsManager.ShowWaitCursor(false);
						}
					catch (error)
						{
						// Dismiss the wait cursor
						pWindowsManager.ShowWaitCursor(false);

						// Display the error message
						var pWindow = UIPropertyList.OpenProperty(UIPROPERTY_TOP_LEVEL_WINDOW);
						App.GrooveMessageBox(pWindow, pAccount, error.message);
						}
					}
				}

				function GetPlainText (i_TextView)
				{
					// prepare for direct TOM access
					i_TextView.Freeze (true);
					i_TextView.HideDecorations (true);

					// retrieve TOM document
					var TextCache = i_TextView.TextCache;
					var Document = TextCache.Document;

					// retrieve plain text
					var Range = Document.Range (0, tomForward); // see "tom" typelib
					var PlainText = Range.Text;

					// we're done accessing the TOM directly
					i_TextView.HideDecorations (false);
					TextCache.Synchronize (GrooveTextCacheSyncType_Cursors);
					i_TextView.Freeze (false);
					
					// When we turn this on, we need to replace all carriage returns with
					// carriage return + line feed.

					return PlainText;
				}

