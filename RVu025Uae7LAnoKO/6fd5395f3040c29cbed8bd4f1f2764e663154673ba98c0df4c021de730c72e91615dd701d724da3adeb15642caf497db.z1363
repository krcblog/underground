		function MSProjectPumpDataDelegate_OnPropertyChanged(i_Key, i_Value, i_LocallyGenerated)
		{
			if(false == i_LocallyGenerated)
				return;

			switch (i_Key)
			{
				case "DMD_BigProject":
				{
					if(GrooveMessageBoxResultCode_No == 
					UltimateMessageBox(g_cSTR_BIG_PROJECT, g_cSTR_DIALOG_TITLE_WARNING, GrooveMessageBoxStyle_YesNo, GrooveMessageBoxIcon_Exclamation, g_pWindow ))
					{
						MSProjectPumpDataDelegate.IGrooveProjectMSProjectPump.SetProperty("UI_Cancel", 0);
						TeardownWindow();
					}

					break;
				}

				case "DMD_NoPredecessors":
				{
					if(GrooveMessageBoxResultCode_No == 
					UltimateMessageBox(g_cSTR_NO_PREDECESSORS, g_cSTR_DIALOG_TITLE_WARNING, GrooveMessageBoxStyle_YesNo, GrooveMessageBoxIcon_Exclamation, g_pWindow ))
					{
						MSProjectPumpDataDelegate.IGrooveProjectMSProjectPump.SetProperty("UI_Cancel", 0);
						TeardownWindow();
					}

					break;
				}

				case "DMD_Message":
				{
					g_MessageText = i_Value;
					StatusValue.SetText(i_Value);
					break;
				}

				case "DMD_SetLayoutForImport":
				{
					HideOptionsDisplayProgressBar()
					break;
				} 

				case "DMD_SetRange":
				{
					g_Range = i_Value;
					ProgressBar.SetRange(0, i_Value);
					ProgressBar.Value = 0;
					break;
				}

				case "DMD_UpdateProgress":
				{
					if(0 != g_Range)
					{
						ProgressBar.Value++;	
						var StatusText = g_MessageText + ": " + ProgressBar.Value.toString() + " of " + g_Range.toString();
						StatusValue.SetText(StatusText);
					}
					else
					{
						ProgressBar.SetRange(0, 1);
						ProgressBar.Value = 1;
						StatusValue.SetText("");
					}		

					break;
				}

				case "DMD_EndTaskImport":
				{
					CancelButton.Enabled = false;
					break;
				}

				case "DMD_StartTimerProgress":
				{
					CancelButton.Enabled = false;
					ProgressBar.Value = ProgressBar.Min;
					Timer.Interval = i_Value;
					Timer.Start();
					break;
				}

				case "DMD_EndTimerProgress":
				{
					Timer.Stop()
					ProgressBar.Value = ProgressBar.Max;
					CancelButton.Enabled = true;
					break;
				}

				case "DMD_Done":
				{
					CancelButton.Label = "Close";
					CancelButton.Enabled = true;
					break;
				}
		
				case "DMD_Abort":
				{

					ShowMSProjectError();
					TeardownWindow();
					break;
				}
			}
		}

		function Timer_OnTimer()
		{
			if(ProgressBar.Value == ProgressBar.Max)
				ProgressBar.Value = ProgressBar.Min;

			ProgressBar.Value += 1; 	
		}

		function HideOptionsDisplayProgressBar()
		{
			var TableSizeObj = GrooveScriptFunctions.CreateNewObject("Groove.MultiCellLayoutControlInfo");
			var TableCellObj = TableSizeObj.Cell();	
			
			TableCellObj.Name = "DontImportReconcile";
			MSPumpProgressLayout.RemoveCellContents(TableCellObj);
			
			TableSizeObj.SetTopCellPadding(6, GrooveLayoutSizeType_Pixels);
			TableSizeObj.SetBottomCellPadding(6, GrooveLayoutSizeType_Pixels);
			MSPumpProgressLayout.AdjustCellLayout(TableCellObj, TableSizeObj);

			MSPumpProgressLayout.AddControlByName("ProgressBar",TableCellObj);

			DontImportDontReconcileButton.Visible = false;
			DontImportReconcileButton.Visible = false;
			ImportAndReconcileButton.Visible = false;
			GoAheadButton.Visible = false;
		}

