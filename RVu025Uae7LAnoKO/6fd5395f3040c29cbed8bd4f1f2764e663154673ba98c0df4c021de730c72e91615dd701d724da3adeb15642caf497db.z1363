                //----------------------------------
                // unread mark engine event handlers
                //----------------------------------

                function UnreadMarkEngine_OnUnreadMarksChanged (i_Enum, i_Unread, i_Triggered)
                {
                    if (!EnumContainsItem (i_Enum, TEXT_ROOT_ITEM_ID))
                        return;

                    if (!i_Unread)
                        ForcedUnread = false;
                    else
                    {
                        StopTimer ();
                        if (!i_Triggered)
                            ForcedUnread = true;
                        else if (!ForcedUnread)
                            StartTimer ();
                    }
                }

                function UnreadMarkEngine_OnMarkReadConflict (i_Enum)
                {
                    if (EnumContainsItem (i_Enum, TEXT_ROOT_ITEM_ID))
                    {
                        ForcedUnread = false;
                        StartTimer ();
                    }
                }

