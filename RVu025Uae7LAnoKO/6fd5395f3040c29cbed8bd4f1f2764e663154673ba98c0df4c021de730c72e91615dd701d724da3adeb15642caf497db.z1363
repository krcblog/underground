					function AddPredecessors(i_TaskID, i_pPredecessorIDEnum, i_pIProgress)
					{
						// add this listener
						PDHF_AddListener(i_pIProgress);

						// AMLATER - CHECK ACL

						try
						{
						//__SZLATER PDHF_IsCalculationDeferred() check is added for bulk processing. 
