				function AddRandomEvent()
				{
					var CalendarRecord = CalendarEngine.RecordFactory.CreateRecord("urn:groove.net:CalendarEntry");
					CalendarRecord.SetField("Subject", GenRandomSubject());
					var StartDate = GenRandomStartDate();
					CalendarRecord.SetField("Start", StartDate.getTime());
					var EndDate = GenRandomEndDate(StartDate);
					CalendarRecord.SetField("End", EndDate.getTime());
					CalendarRecord.SetField("IsAllDay", false);
					CalendarRecord.SetField("IsRecurrent", false);
					CalendarRecord.SetField("IconPath", "");
					CalendarRecord.SetField("CreatorURL", "TESTER");
					CalendarRecord.SetField("Description", "");
					CalendarEngine.AddRecord(CalendarRecord);
				}

				function GenRandomStartDate()
				{
					var Start = new Date(CalendarView.SelectionStart);
					var Hours = RandInt(9, 15);
					var Mins = RandInt(0, 59);
					Start.setHours(Hours, Mins);
					return Start;
				}

				function GenRandomEndDate(i_Start)
				{
					var StartMillis = i_Start.getTime();
					var MillisInOneMinute = 1000 * 60;
					// one to five hours long
					StartMillis += (RandInt(60, 300) * MillisInOneMinute);
					return new Date(StartMillis);
				}

				function GenRandomSubject()
				{
					var NumChars = RandInt(4, 10);
					var RetStr = "";
					for (var i = 0; i < NumChars; i++)
						RetStr += String.fromCharCode(RandInt(65,90));
					return RetStr;
				}

				var GROOVE_DATE_FORMAT_LONG = 0;
				var GROOVE_DATE_FORMAT_SHORT = 2;

				// Returns a date string formatted with the given style
				function FormatDate(i_DateObj, i_FormatStyle)
				{
					// returns a string in the format: "Wed Aug 12, 2000"
					if (i_FormatStyle == GROOVE_DATE_FORMAT_LONG)
					{
						var DateStr = i_DateObj.toString();
						var re = /^([A-Za-z\s]+)\s(\d{1,2})\W+.*(\d{4})$/;
						var ResultAry = re.exec(DateStr);
						return RegExp.$1 + " " + RegExp.$2 + ", " + RegExp.$3;
					}

					// returns a string in the format: "8/12/2000"
					else if (i_FormatStyle == GROOVE_DATE_FORMAT_SHORT)
					{
						var RetStr;
						RetStr = (i_DateObj.getMonth() + 1) + "/";
						RetStr += i_DateObj.getDate() + "/";
						RetStr += i_DateObj.getYear();
						return RetStr;
					}
				}

				// This is a temporary hack until IGrooveDate!!
				// Returns a time string formatted with the given style
				function FormatTime(i_DateObj)
				{
					if (i_DateObj.getHours() < 12)
						var AMPMStr = "AM";
					else
						var AMPMStr = "PM";

					var FriendlyHour = ((i_DateObj.getHours() - 1) % 12) + 1;
					if (FriendlyHour == 0)
						FriendlyHour = 12;

					return FriendlyHour + ":" + FormatDigits(i_DateObj.getMinutes(), 2) + " " + AMPMStr;
				}

				function FormatDigits(i_Num, i_MinDigits)
				{
