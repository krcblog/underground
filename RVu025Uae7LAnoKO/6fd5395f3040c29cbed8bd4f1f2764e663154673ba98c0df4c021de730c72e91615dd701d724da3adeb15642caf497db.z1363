								GrooveDebugFunctions.DisplayAssert("urn:groove.net:ProjectTaskRecord" == TaskRecord.Type, "Invalid Task ID");

								var DurationMS = TaskRecord.OpenField("ScheduledDuration");
								var ScheduledFinishMS = TaskRecord.OpenField("ScheduledFinish");
								ScheduledFinishMS = MHF_GetDisplayFinishDateMS(DurationMS, ScheduledFinishMS);

								Item = pIPendingItemDetailsFactory.CreatePendingItem
											(	i_ID,
												TaskRecord.OpenField("Name"),
												GroovePendingItemType_Task,		// Type
												TaskRecord.OpenField("AssignedToContactURL"),
