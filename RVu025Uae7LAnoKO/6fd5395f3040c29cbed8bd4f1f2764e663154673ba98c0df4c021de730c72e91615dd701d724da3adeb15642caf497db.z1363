					// Verify that selected predecessors are not parents/ancestors of the task
					// Return false if cycle is caused.
					function ValidatePredecessorsForTask(i_bIsNewTask)
					{
						try
						{
							var pICheckedEnum = PredecessorList.OpenCheckedEnum();
							var pIPredecessorIDEnum = CreateDoubleEnum();
							var NoCycle = true;
							var taskID = g_TaskID;

							var Telespace = PropertyList.OpenProperty(PROPERTY_TELESPACE);
							var Transaction = Telespace.OpenTransaction(true);
							try
							{
								// Special case for new tasks.
								if (i_bIsNewTask)
								{
									//-----
									//- If new task will be a sub-task of another, then the current selection
									//- is the task to check circular dependecies on. If the new task will be
									//- a sibling, then use the siblling's parent to check for circular dependecies.
									//-----
									if (g_bSubTask && g_SelectionID != -1)
										taskID = g_SelectionID;

									else if (g_ParentID != -1)
										taskID = g_ParentID;
								}

								// if the item has been deleted, don't check for cycles
								// and we'll inform the user later
								if(g_pIProjectDMD.DoesItemExist(taskID))
								{
									while(pICheckedEnum.HasMore())
									{
										var Index = pICheckedEnum.OpenNext();
										var PredecessorInfo = PredecessorList.GetItemData(Index);

										// Handle edge condition. If new task will be a sub-task of another,
										// the parent task may not be a predecessor.
										if (i_bIsNewTask && g_bSubTask && g_SelectionID == PredecessorInfo.ID)
										{
											NoCycle = false;
											break;
										}

										// if the item has been deleted, ignore it and we'll 
										// inform the user later that some predecessors were skipped
										if(g_pIProjectDMD.DoesItemExist(PredecessorInfo.ID))
										{
											// Add to the enum list
											pIPredecessorIDEnum.Add(PredecessorInfo.ID);
										}
									}
									
									pIPredecessorIDEnum = pIPredecessorIDEnum.DoubleEnum;

									if(NoCycle &&
									   g_pIProjectDMD.DoesAddPredecessorsCreateCycle(taskID, pIPredecessorIDEnum))
									{
										NoCycle = false;
									}
								}

								Transaction.Commit();
							}
							catch(e)
							{
								Transaction.Abort();
								GrooveDebugFunctions.DisplayAssert(0, "Error checking for cycle");
							}
						}
						catch(e)
						{
							GrooveDebugFunctions.DisplayAssert(0, "Error checking for cycle");
						}

						return NoCycle;
					}

