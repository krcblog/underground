				function PopulateStatusCombobox(i_StatusArray)
				{
					if (i_StatusArray.length != StatusComboBox.Count)
					{
						StatusComboBox.ResetContent();
						for (var i = 0; i < i_StatusArray.length; ++i)
						{
							var index = StatusComboBox.AddItem(i_StatusArray[i]);
						}
					}

					if(StatusComboBox.Count > 0)
						StatusComboBox.SetSelectionByIndex(0);
				}

				function FillContactsCB(i_ParticipantStrEnum)
				{
					ParticipantComboBox.ResetContent();
					for(var i=0; i<i_ParticipantStrEnum.GetCount(); i++)
					{
						var Index = ParticipantComboBox.AddItem(i_ParticipantStrEnum.Map[i].Name);
						ParticipantComboBox.SetItemData(Index, i_ParticipantStrEnum.Map[i].ContactURL);
					}
					if(ParticipantComboBox.Count > 0)
						ParticipantComboBox.SetSelectionByIndex(0);
				}

				function SetParticipantComboboxCurrentSelection(i_SelectionText)
				{
					if(GrooveIndex_NotFound != ParticipantComboBox.FindStringExact(0, i_SelectionText))
						ParticipantComboBox.SetSelectionByText(i_SelectionText);
				}

				function SetStatusComboboxCurrentSelection(i_SelectionText)
				{
					if(GrooveIndex_NotFound != StatusComboBox.FindStringExact(0, i_SelectionText))
						StatusComboBox.SetSelectionByText(i_SelectionText);
				}

				function StatusComboBox_OnSelectionChange(i_Name, i_Item)
				{
					FireOnChangeEvent();
				}

				function ParticipantComboBox_OnSelectionChange(i_Name, i_Item)
				{
					FireOnChangeEvent();
				}

				function FireOnChangeEvent()
				{
					FireEvent("", "OnSelectionChanged");
				}

				function IssueFilteringRequest(i_XSLPath, i_bGenerateNewXML)
				{
					FireEvent(
								"",
								"OnFilter",
								i_XSLPath,
								StatusComboBox.GetSelectionText(),
								ParticipantComboBox.GetItemData(ParticipantComboBox.GetSelectionIndex()),
								i_bGenerateNewXML
