												TaskRecord.OpenField("ActualStart"),
												TaskRecord.OpenField("ActualFinish"),
												TaskRecord.OpenField("_IsRead")
											);
							}

							return Item;
						}
						catch(e)
						{
							PDHF_SetError(PROJECTTRACKING_E_OPERATION_FAILED, "Error retrieving task");
