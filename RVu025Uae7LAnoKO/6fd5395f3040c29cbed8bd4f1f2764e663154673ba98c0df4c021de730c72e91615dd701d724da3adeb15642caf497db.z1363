					function HasChanged()
					{
						// This only compares the fields that we are actually using
						return g_CurrentItem.Name != GetName() ||
							   g_CurrentItem.AssignedToContactURL != GetAssignedToContactURL() ||
							   g_CurrentItem.Status != GetStatus() ||
							   g_CurrentItem.Priority != GetPriority() ||
							   g_CurrentItem.UserStartConstraint != GetUserStartConstraint() ||
							   g_CurrentItem.ConstrainStartDate != GetConstrainStartFlag() ||
							   g_CurrentItem.ScheduledDuration != GetScheduledDuration() ||
							   g_CurrentItem.ActualStart != GetActualStart() ||
							   g_CurrentItem.ActualFinish != GetActualFinish() ||
							   false == g_CurrentItem.Description.Equals(GetDescription()) ||
							   0 != GetNewPredecessorsArray().length ||
							   0 != GetRemovePredecessorsArray().length ||
							   true == g_ParentSubForm.Attachments.HasChanged();
					}

					function ValidateInput()
					{
						var UserStart = ParseDayAndDate(UserStartDateValueEdit.Text,
														GrooveIntlDateFormatStyle_Short);

						// Validate actual start and finish dates
						if (!ValidateActualAndFinishDate(true))
							return false;

						// If the user has sufficient privs check the name and dates (which 
						// can only be modified when the task is not a summary task)
						if(g_CanEditScheduleInfo)
						{
							if(false == g_CurrentItem.IsSummary)
							{
								if(false == ValidateStartDate(true))
								{
									// Reset start date field to default if there is no data.
									if ("" == UserStartDateValueEdit.Text)
										UserStartDateValueEdit.Text =
													 FormatDayAndDate(g_CurrentItem.UserStartConstraint,
																	  GrooveIntlDateFormatStyle_Short);

									// Set focus and selection to the invalid item.
									ProjectDetailsHeaderViewContainer.SetFocus("UserStartDateValueEdit");
									UserStartDateValueEdit.SetSelection(0, UserStartDateValueEdit.Text.length);
									return false;
								}

								if(false == ValidateDuration())
								{
									PHF_AlertUser(GPTPROJECT_MB_ENTER_VALID_DURATION_DATE);

									// Reset duration field to default if there is no data.
									if ("" == ScheduledDurationValueEdit.Text)
										ScheduledDurationValueEdit.Text = g_CurrentItem.ScheduledDuration / MSPERDAY;

									// Set focus and selection to the invalid item.
									ProjectDetailsHeaderViewContainer.SetFocus("ScheduledDurationValueEdit");
									ScheduledDurationValueEdit.SetSelection(0, ScheduledDurationValueEdit.Text.length);


									return false;
								}
							}

							// Validate name
							if("" == TrimHead(ItemNameEdit.Text))
							{
								PHF_AlertUser(GPTPROJECT_MB_NAME_REQUIRED);
								ItemNameEdit.Text = g_CurrentItem.Name;

								// Set focus and selection to the invalid item.
								ItemNameEdit.SetSelection(0, ItemNameEdit.Text.length);
								ProjectDetailsHeaderViewContainer.SetFocus("ItemNameEdit");
								return false;
							}
						}

						return true;
					}

