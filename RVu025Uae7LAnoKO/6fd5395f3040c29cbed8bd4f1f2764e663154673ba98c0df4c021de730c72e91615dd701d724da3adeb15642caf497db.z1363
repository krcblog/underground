                function EnumContainsItem (i_Enum, i_Item)
                {
                    while (i_Enum.HasMore())
                        if (i_Enum.OpenNext() == i_Item)
                            return true;

                    return false;
                }

                function StartTimer ()
                {
                    StopTimer ();
                    if (ToolUICommandContainer != null)
                        Timer.Start ();
                }

                function StopTimer ()
                {
                    Timer.Stop ();
                }

