					function DemoteItem(i_ID, i_pIProgress)
					{
						if(GrooveRunTimeData.GetItem("PROJECT_ID") == i_ID)
						{
							GrooveDebugFunctions.DisplayAssert(0, "Attempt to demote project task");
							return;
						}

						// Forward to DemoteMultiple
						var pIIDEnum = CreateDoubleEnum();
						pIIDEnum.Add(i_ID);
						DemoteMultipleItems(pIIDEnum.DoubleEnum, i_pIProgress)
					}

					function DemoteMultipleItems(i_pIItemIDEnum, i_pIProgress)
					{
						i_pIItemIDEnum.Reset();
						if (!i_pIItemIDEnum.HasMore())
							return;

						// Add this listener
						PDHF_AddListener(i_pIProgress);

						// Trigger execution
						var DynamicsMgr = PropertyList.OpenProperty(PROPERTY_DYNAMICS_MANAGER);
						var pIDelta = DynamicsMgr.CreateDelta();
						try
						{
							// Generate the demote pseudo-command
							var pIDemoteCmdRecord = PDHF_BuildDemoteCmdRecord(i_pIItemIDEnum);
							RecordSetEngine.ReplaceRecord(pIDemoteCmdRecord.RecordID, pIDemoteCmdRecord);

							pIDelta.Commit();
						}
						catch(e)
						{
							pIDelta.Abort();
/* xxx BBLATER

							if (PROJECTTRACKING_E_CYCLE_CHECK_FAILED == e.number)
							{
								// Demote failed cycle check. Alert user.
								//xxx Would be nice to display e.message to user
							}
							else
*/
							{
								PDHF_SetError(PROJECTTRACKING_E_OPERATION_FAILED, "Demoting tasks");
							}
						}

						// Remove this listener
						PDHF_RemoveListener(i_pIProgress);
					}

					function PromoteItem(i_ID, i_pIProgress)
					{
						// add this listener
						PDHF_AddListener(i_pIProgress);

						try
						{
							// forward to PromoteMultiple
							var pIIDEnum = CreateDoubleEnum();
							pIIDEnum.Add(i_ID);
							PromoteMultipleItems(pIIDEnum.DoubleEnum, i_pIProgress);
						}
						catch(e)
						{
							PDHF_SetError(PROJECTTRACKING_E_OPERATION_FAILED, "Error promoting task");
						}

						// remove this listener
						PDHF_RemoveListener(i_pIProgress);
					}



					function PromoteMultipleItems(i_pIItemIDEnum, i_pIProgress)
					{
						// add this listener
						PDHF_AddListener(i_pIProgress);

						try
						{
							// validate operation
							i_pIItemIDEnum.Reset();
							while(i_pIItemIDEnum.HasMore())
							{
								var PromoteID = i_pIItemIDEnum.OpenNext();
								if(false == CanPromoteItem(PromoteID))
								{
									GrooveErrorFunctions.SetCurrentError(PROJECTTRACKING_E_INVALID_OPERATION, "Cannot promote task.");
									return;
								}
								
							}
							i_pIItemIDEnum.Reset();


							// trigger execution
							var DynamicsMgr = PropertyList.OpenProperty(PROPERTY_DYNAMICS_MANAGER);
							var pIDelta = DynamicsMgr.CreateDelta();
							try
							{
								var pIPromoteCmdRecord = PDHF_BuildPromoteCmdRecord(i_pIItemIDEnum);
								RecordSetEngine.ReplaceRecord(pIPromoteCmdRecord.RecordID, pIPromoteCmdRecord);
								pIDelta.Commit();
							}
							catch(e)
							{
								pIDelta.Abort();
								throw e;
							}
						}
						catch(e)
						{
							PDHF_SetError(PROJECTTRACKING_E_OPERATION_FAILED, "Error promoting tasks");
						}

						// add this listener
						PDHF_RemoveListener(i_pIProgress);
					}


