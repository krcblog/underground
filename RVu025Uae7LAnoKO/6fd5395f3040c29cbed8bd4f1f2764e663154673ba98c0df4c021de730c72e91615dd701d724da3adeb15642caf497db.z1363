				function DisplayProgressWindow(i_FilePath, i_pParentWindow, i_pProjectDataDelegate, i_bImport)
				{
					g_pProgressCode = MSPumpProgressSubform.OpenDelegateComponent().GetScriptDispatch();
					g_pProgressCode.SetFilePath(i_FilePath);
					g_pProgressCode.SetOperation(i_bImport);

					g_pProgressCode.SetProjectDataDelegate(i_pProjectDataDelegate);

					if(i_pParentWindow)
						g_pWindowProperties.ParentWindow = i_pParentWindow;
							
					g_pWindowProperties.Style = "PopupWindow";
					g_pWindowProperties.Resizable = false;
					g_pWindowProperties.CaptionVisible = true;

					if(true == i_bImport)
						g_pWindowProperties.Caption = g_cSTR_IMPORT_CAPTION;
					else
						g_pWindowProperties.Caption = g_cSTR_EXPORT_CAPTION;

					g_pWindowProperties.Visible = false;
					g_pWindowProperties.InitialWidth = 500;
					g_pWindowProperties.InitialHeight = 230;
					g_pWindowProperties.CloseButton = true;

		␀ð ␀␀␀␀␀€␀␀␀			g_pProgressWindow = g_pWindowManager.CreateGrooveWindowForForm
																	(	g_pAccount,
																		"",
																		g_pWindowProperties,
																		MSPumpProgressSubform
																	);

					g_pProgressCode.SetWindow(g_pProgressWindow);

					ConnectObject(g_pProgressWindow, "ProgressWindow_", true);
					var Result = g_pProgressWindow.DoModal();
					DisconnectObject(g_pProgressWindow);
				}

				function ProgressWindow_OnWindowShow(i_pIWindow, i_ShowCommand)
				{
					if(i_pIWindow == g_pProgressWindow && GrooveWindowShowCommand_ShowNormal == i_ShowCommand)
					{
						if(false == g_pProgressCode.g_bImport)
							g_pProgressCode.GoAheadButton.DoClick();
					}
				}

				function ProgressWindow_OnWindowClosing(i_pIWindow)
				{
					if(i_pIWindow == g_pProgressWindow)
					{
						g_pProgressCode.CancelButton.DoClick();
					}

					return true; 
				}


