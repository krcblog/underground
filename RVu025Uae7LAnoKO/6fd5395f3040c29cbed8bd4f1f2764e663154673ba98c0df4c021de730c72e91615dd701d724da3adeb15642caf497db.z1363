		function UltimateMessageBox()
		{
			var Message = "Error";
			var Title = "Groove Message Box";
			var Style = GrooveMessageBoxStyle_OK;
			var Icon = GrooveMessageBoxIcon_Exclamation;
			var Window;
			var Account;
			var Result = GrooveMessageBoxResultCode_Ignore;

			switch(arguments.length)
			{
				case 6:
					Account = arguments[ 5 ];
				case 5:
					Window = arguments[ 4 ];
				case 4:
					Icon = arguments[ 3 ];
				case 3:
					Style = arguments[ 2 ];
				case 2:
					Title = arguments[ 1 ];
				case 1:
					Message = arguments[ 0 ];
				default:
					break;
			}

			if ("undefined" == typeof Window || !Window)
			{
				try
				{
					Window = UIPropertyList.OpenProperty(UIPROPERTY_TOP_LEVEL_WINDOW);
				}
				catch(e)
				{
					Window = GrooveScriptFunctions.CreateNewObject("");
				}
			}

			if ("undefined" == typeof Account || !Account)
			{
				try
				{
					Account = UIPropertyList.OpenProperty(UIPROPERTY_WINDOW_ACCOUNT);
				}
				catch(e)
				{
					try
					{
						Account = PropertyList.OpenProperty(PROPERTY_ACCOUNT);
					}
					catch(e)
					{
						Account = GrooveScriptFunctions.CreateNewObject("");
					}
				}
			}

			try
			{
				Result = App.Application2.GrooveMessageBox2(Window, Account, Message, Title, Style, Icon);
			}
			catch(error)
			{
				if (Message.Range == null)
					Result = GrooveScriptFunctions.DoMessageBox(Message, Title, Style);
				else
					Result = GrooveScriptFunctions.DoMessageBox(Message.Text, Title, Style);
			}

