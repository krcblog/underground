				var m_Start = 0, m_End = 0;

				function OnViewContainerShow()
				{
					CalendarView.ShowGridHeader = false;
					UpdateTitle();
				}

				function SetSelection(i_Date)
				{
					CalendarView.SetSelection(i_Date, i_Date);
					CalendarView.ScrollToSelectionStart();
				}

				function CalendarView_OnDblClickCell(i_Date, i_ClickedHeader)
				{
					CloseWindow();
				}

				function CalendarView_OnGridExtentsChanged(Start, End)
				{
					UpdateTitle();
				}

				function UpdateTitle()
				{
					MonthTitle.SetText(FormatMonthAndYear(CalendarView));
				}

				function CalendarView_OnCalendarCommand(i_Command)
				{
					if (i_Command == GrooveCalendarCommand_NewEntry)
						CloseWindow();
				}
	
				function OKButton_OnCommand(URL, Data)
				{
					CloseWindow();
				}

				function CancelButton_OnCommand(URL, Data)
				{
					var Window = UIPropertyList.OpenProperty(UIPROPERTY_TOP_LEVEL_WINDOW);
					Window.Close(GrooveMessageBoxResultCode_Cancel);
					Window.Destroy(GrooveMessageBoxResultCode_Cancel);
				}

				function CloseWindow()
				{
					// save the selections..
					m_Start = CalendarView.SelectionStart;
					m_End = CalendarView.SelectionEnd;

					var Window = UIPropertyList.OpenProperty(UIPROPERTY_TOP_LEVEL_WINDOW);
					Window.Close(GrooveMessageBoxResultCode_OK);
					Window.Destroy(GrooveMessageBoxResultCode_OK);
				}

				function Prev_OnCommand(URL, Data)
				{
					CalendarView.ScrollPrev();
				}

				function Next_OnCommand(URL, Data)
				{
					CalendarView.ScrollNext();
				}

				function GetStartDate()
				{
					return m_Start;
				}

				function GetEndDate()
				{
					return m_End;
				}

				function GetSelectionSpansDays()
				{
