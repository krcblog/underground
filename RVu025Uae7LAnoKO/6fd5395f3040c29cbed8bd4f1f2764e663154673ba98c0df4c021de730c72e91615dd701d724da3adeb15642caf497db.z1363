				function GetCreatorURLFromRecordID(i_RecordID)
				{
					var CreatorURL = "";
					// Begin read-only Transaction
					var Transaction = Telespace.OpenTransaction(true);
					try
					{
						if (CalendarEngine.HasRecord(i_RecordID))
						{
							var CalendarRecord = CalendarEngine.OpenRecord(i_RecordID);
							CreatorURL = CalendarRecord.OpenField("CreatorURL");
							Transaction.Commit();
						}
						else
						{
							Transaction.Abort();
						}
					}
					catch (e)
					{
						Transaction.Abort();
					}

					return CreatorURL;
				}

				function DeleteSelectedEvent()
				{
					if (!CalendarView.IsSelectionADate)
					{
						if (!ConfirmDeleteSelectedEntry())
							return;

						var RecordID = CalendarView.SelectedEntry;

						// Begin the read/write transaction
						var Transaction = Telespace.OpenTransaction(false);
						try
						{
							if (CalendarEngine.HasRecord(RecordID))
							{
								CalendarEngine.RemoveRecord(RecordID);
								Transaction.Commit();
							}
							else
							{
								Transaction.Abort();
							}
						}
						catch(e)
						{
							Transaction.Abort();
						}
					}
				}

				function ConfirmDeleteSelectedEntry()
				{
					var EntrySubject = GetSelectedEntrySubject();
					var ConfirmStr = CALENDARFORM_TEXT_COMFIRM_DELETE_1 " + EntrySubject + "\"?  ";
					ConfirmStr += CALENDARFORM_TEXT_COMFIRM_DELETE_2;
					var Window = UIPropertyList.OpenProperty(UIPROPERTY_TOP_LEVEL_WINDOW);
					var Account = OpenAccountFromPropertyList(PropertyList);
					var RetVal = App.GrooveMessageBox(Window, Account, ConfirmStr, CALENDARFORM_MESSAGEBOX_TITLE_DELETE_RECIPE, GrooveMessageBoxStyle_YesNo, 256);
					if (RetVal == GrooveMessageBoxResultCode_Yes)
						return true;
					else
						return false;
				}

				function GetSelectedEntrySubject()
				{
					if (CalendarView.IsSelectionADate)
						return "";

					var SubjectStr = "";
					var RecordID = CalendarView.SelectedEntry;

					// Begin read-only Transaction
					var Transaction = Telespace.OpenTransaction(true);
					try
					{
						if (CalendarEngine.HasRecord(RecordID))
						{
							var CalendarRecord = CalendarEngine.OpenRecord(RecordID);
							SubjectStr = CalendarRecord.OpenField("Subject");
							Transaction.Commit();
						}
						else
						{
							Transaction.Abort();
						}
					}
					catch (e)
					{
						Transaction.Abort();
					}
					return SubjectStr;
				}

				// Opens a dialog prompting the user to fill out calendar entry info.
				// Fills a calendar record with that info, and adds it to the engine.
				function AddCalendarEvent()
				{
					var WindowAccount = UIPropertyList.OpenProperty(UIPROPERTY_WINDOW_ACCOUNT);

					// set the window properties
					var WindowProperties = GrooveScriptFunctions.CreateNewObject("Groove.WindowProperties");
					WindowProperties.Style = "PopupWindow";
					WindowProperties.ParentWindow = UIPropertyList.OpenProperty(UIPROPERTY_TOP_LEVEL_WINDOW);
					WindowProperties.Resizable = false;
					WindowProperties.Caption = CALENDARFORM_WINDOWPROPERTIES_CAPTION_CALENDAR;
					WindowProperties.CaptionVisible = true;
					WindowProperties.Visible = true;
					WindowProperties.CloseButton = false;
					WindowProperties.BorderVisible = false;

					// open the window
					var WindowMgr = GrooveScriptFunctions.CreateNewObject("Groove.WindowManager");
					var Window = WindowMgr.CreateGrooveWindowForForm(WindowAccount, "", WindowProperties, CalendarAddEntryDialogSubForm);

					// get the dispatch
					var AddEntryHost = Window.OpenDelegateComponent();
					var AddEntryCode = AddEntryHost.GetScriptDispatch();

					// prefill the add event form
					if (CalendarView.IsSelectionADate)
					{
						AddEntryCode.PrefillStartDate(CalendarView.SelectionStart, true);
						AddEntryCode.PrefillEndDate(CalendarView.SelectionEnd, true);
					}
					else
					{
						var RecordID = CalendarView.SelectedEntry;
						var SelectedEntryRecord = CalendarEngine.OpenRecord(RecordID);
						var Start = SelectedEntryRecord.OpenField("Start");
						var End = SelectedEntryRecord.OpenField("End");
						AddEntryCode.PrefillStartDate(Start, false);
						AddEntryCode.PrefillEndDate(End, false);
					}

					AddEntryCode.CalculateEventLength();

					if (GrooveMessageBoxResultCode_OK == Window.DoModal())
					{
						// Begin the read/write transaction
						var Transaction = Telespace.OpenTransaction(false);
						try
						{
							var CalendarRecord = CalendarEngine.RecordFactory.CreateRecord("urn:groove.net:CalendarEntry");
							AddEntryCode.FillCalendarRecord(CalendarRecord);
							CalendarEngine.AddRecord(CalendarRecord);
							Transaction.Commit();
						}
						catch (e)
						{
							Transaction.Abort();
						}
					}
				}

				// Opens a dialog prompting the user to fill out calendar entry info.
				// Fills a calendar record with that info, and adds it to the engine.
				function EditCalendarEvent(i_RecordID)
				{
					var WindowAccount = UIPropertyList.OpenProperty(UIPROPERTY_WINDOW_ACCOUNT);

					// set the window properties
					var WindowProperties = GrooveScriptFunctions.CreateNewObject("Groove.WindowProperties");
					WindowProperties.Style = "PopupWindow";
					WindowProperties.ParentWindow = UIPropertyList.OpenProperty(UIPROPERTY_TOP_LEVEL_WINDOW);
					WindowProperties.Resizable = false;
					WindowProperties.Caption = CALENDARFORM_WINDOWPROPERTIES_CAPTION_ADD_EVENT;
					WindowProperties.CaptionVisible = true;
					WindowProperties.Visible = true;
					WindowProperties.CloseButton = false;
					WindowProperties.BorderVisible = false;

					// open the window
					var WindowMgr = GrooveScriptFunctions.CreateNewObject("Groove.WindowManager");
					var Window = WindowMgr.CreateGrooveWindowForForm(WindowAccount, "", WindowProperties, CalendarAddEntryDialogSubForm);

					// get the dispatch
					var AddEntryHost = Window.OpenDelegateComponent();
					var AddEntryCode = AddEntryHost.GetScriptDispatch();

					var CreatorURL = "";
					var Failed = false;
					var ClonedRecord = null;

					// Begin the read-only transaction
					var Transaction = Telespace.OpenTransaction(true);
					try
					{
						if (CalendarEngine.HasRecord(i_RecordID))
						{
							var CalendarRecord = CalendarEngine.OpenRecord(i_RecordID);
							ClonedRecord = CalendarRecord.Clone();
							Transaction.Commit();
						}
						else
						{
							Transaction.Abort();
							Failed = true;
						}
					}
					catch (e)
					{
						Transaction.Abort();
						Failed = true;
					}
					
					if (Failed)
					{
						// Asynchronously destroy the window
						Window.Destroy(GrooveWindowResult_Cancel, true);
						return;
					}

					AddEntryCode.PrefillFormFromRecord(ClonedRecord);
					CreatorURL = ClonedRecord.OpenField("CreatorURL");

					// Find out if we're editing or viewing
					var Editing;
					if (m_UserURL == CreatorURL)
					{
						Editing = true;
						AddEntryCode.EnableAll();
					}
					else
					{
						Editing = false;
						AddEntryCode.DisableAll();
					}

					if (GrooveMessageBoxResultCode_OK == Window.DoModal())
					{
						if (Editing)
						{
							AddEntryCode.FillCalendarRecord(ClonedRecord);

							// Begin the read/write transaction
							var Transaction = Telespace.OpenTransaction(false);
							try
							{
								// If the record is still around, replace it with the clone
								if (CalendarEngine.HasRecord(i_RecordID))
									CalendarEngine.ReplaceRecord(i_RecordID, ClonedRecord);

								// If the record is gone, add it
								else
									CalendarEngine.AddRecord(ClonedRecord);
									
								Transaction.Commit();
							}
							catch (e)
							{
								Transaction.Abort();
							}
						}
					}
				}

				// Swaps the list view in or out of layout
				function SwapListView()
				{
					// remove the current contents
					var TableSizeObj = GrooveScriptFunctions.CreateNewObject("Groove.HTMLTableLayoutControlInfo");
					var TableCellObj = TableSizeObj.Cell();
					TableCellObj.SetCell(1,0);
					var Layout = CalendarFormView.Layout;
					Layout.RemoveCellContents(TableCellObj);
					
					// add the list view
					if (m_CalendarGridShowing)
					{
						Layout.AddControlByName("CalendarListViewSubForm", TableCellObj);
						var CalendarListViewHost = CalendarListViewSubForm.OpenDelegateComponent();
						var CalendarListViewCode = CalendarListViewHost.GetScriptDispatch();
						var RecordEnum = CalendarEngine.OpenRecordEnum();
						CalendarListViewCode.FillList(RecordEnum);
					}

					// add the calendar view
					else
					{
						Layout.AddControlByName("CalendarSubForm", TableCellObj);
					}

					// flip the global flag
					m_CalendarGridShowing = !m_CalendarGridShowing;
				}

				// Updates the text title of the calendar. i.e. "July 2000"
				function UpdateCalendarTitle()
				{	
					var View = CalendarView.ViewType;
					var TitleType;
					
					switch (CalendarView.ViewType)
					{
					case GrooveCalendarViewType_Day:
						// show single date
						var ViewDate = new Date(CalendarView.StartDate);
						var TitleText = FormatDate(ViewDate, GROOVE_DATE_FORMAT_LONG);
						CalendarTitle.SetText(TitleText);
						break;

					case GrooveCalendarViewType_Month:
					case GrooveCalendarViewType_MonthWithStackedWeekends:
						// show month and year
						CalendarTitle.SetText(CalendarView.GridMonthName + " " + CalendarView.GridYearNum);
						break;

					case GrooveCalendarViewType_TwoDay:
					case GrooveCalendarViewType_NormalWeek:
					case GrooveCalendarViewType_FamilyWeek:
					case GrooveCalendarViewType_WideWeek:
					case GrooveCalendarViewType_TwoRowWorkWeek:
					case GrooveCalendarViewType_WorkWeek:
					default:
						// show date range
						var StartDate = new Date(CalendarView.StartDate);
						var EndDate = new Date(CalendarView.EndDate);
						var TitleText = FormatDate(StartDate, GROOVE_DATE_FORMAT_SHORT);
						TitleText += " - " + FormatDate(EndDate, GROOVE_DATE_FORMAT_SHORT);
						CalendarTitle.SetText(TitleText);
						break;
					}
				}
				
