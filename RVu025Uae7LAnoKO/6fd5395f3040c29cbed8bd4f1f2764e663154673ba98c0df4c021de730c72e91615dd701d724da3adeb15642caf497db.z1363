							if(false == PDHF_IsCalculationDeferred() && true == DoesAddPredecessorsCreateCycle(i_TaskID, i_pPredecessorIDEnum))
							{
								GrooveDebugFunctions.DisplayAssert(0, "Adding predecessor creates a dependency cycle");
								GrooveErrorFunctions.SetCurrentError(INVALID_OPERATION, "Adding predecessor creates a dependency cycle");
								return;
