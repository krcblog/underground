	function PrintButton_OnCommand(i_UICommand)
	{
		try
		{
			PrintCtrl.ExecWB(6/*OLECMDID_PRINT*/, 1/*OLECMDEXECOPT_PROMPTUSER*/, null, null);
		}
		catch(e)
		{
			//This hack is for Win2K bug throwing an exception when the user cancels printing - we ignore it
			if(e.number != -2147221248)
			{
				var Window = UIPropertyList.OpenProperty(UIPROPERTY_TOP_LEVEL_WINDOW);
				var Account = OpenAccountFromPropertyList(PropertyList);
				g_App.GrooveMessageBox(Window, Account, "Printing Failed");
			}
		}
	}

