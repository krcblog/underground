				function Initialize()
				{
					PrintObject = GrooveScriptFunctions.CreateNewObject("Groove.MiscHelperObject").IGroovePrintDocument;

					if (PropertyList.PropertyExists(TIE_UI_COMMAND_TO_FOCUS_PROPERTY))
						TieUICommandToFocus = PropertyList.OpenPropertyAsBool(TIE_UI_COMMAND_TO_FOCUS_PROPERTY);

					// Set the unread time interval
					var Account = OpenAccountFromPropertyList (PropertyList);
					Timer.Interval = GrooveScriptFunctions.CreateNewObject("Groove.MiscHelperObject").IGroovePreferencesHelper.UnreadTimeInterval(Account);
				}
				
				//----------------------------------
