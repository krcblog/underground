					//
					// strings
					//
					var pBundle = PropertyList.OpenProperty(PROPERTY_RESOURCE_BUNDLE);

					var DIALOG_TITLE_STRING = GetString(pBundle,"PrjWiz_S_PrjWiz");
					var WELCOME_STRING = GetString(pBundle,"PrjWiz_S_Wlcm");

					//
					// constants
					//
					var DATE_STYLE = GrooveIntlDateFormatStyle_Short;

					//
					// globals
					// 
					var g_CurrentProject = null;
					var g_PeopleProvider = null;
					var g_ContactNameURLMap = null;
					var g_ProjectDMD = null;

					function Initialize()
					{
					}

					function SetProjectDataModelDelegate(i_ProjectDMD)
					{
						g_ProjectDMD = i_ProjectDMD;
					}

					function Terminate()
					{
						g_CurrentProject = null;
						g_PeopleProvider = null;
					}

					//
					// Caller must call SetContactList() before this function is called
					// (via DoModal())
					//
					function OnViewContainerShow()
					{
						var ScheduledStart = new Date();

						ProjectLabel.SetText(DIALOG_TITLE_STRING);
						ProjectHelpLabel.SetText(WELCOME_STRING);

						// project name
						ProjectNameLabel.SetText(GPTPROJECT_S_PROJECTNAME);
						ProjectNameValue.Text = g_CurrentProject.Name;
						ProjectNameValue.SetSelection(0, g_CurrentProject.Name.length);

						// team leader
						TeamLeaderLabel.SetText(GPTPROJECT_S_LEADER);

						// Populate contact list
						RefreshContactList();

						// select current leader in combo, default to current user
						// if they're in the list of available contacts
						if("" == g_CurrentProject.ProjectLeaderContactURL &&
						   g_ContactNameURLMap.URLExists(CHF_GetUserURL()))
						{
							g_CurrentProject.ProjectLeaderContactURL = CHF_GetUserURL();
						}

						if("" != g_CurrentProject.ProjectLeaderContactURL)
						{
							var Name = g_ContactNameURLMap.FindByURL(g_CurrentProject.ProjectLeaderContactURL);
							var LeaderIndex = TeamLeaderValue.FindStringExact(0, Name);
							if(GrooveIndex_NotFound != LeaderIndex)
							{
								TeamLeaderValue.SetSelectionByIndex(LeaderIndex);
							}
						}
						
						// scheduled start
						ScheduledStartLabel.SetText(GPTPROJECT_S_SCHEDULED_START);
						ScheduledStartDateValue.Text = FormatDayAndDate(g_CurrentProject.ScheduledStart, DATE_STYLE);
											
						// objective
						ObjectiveLabel.SetText(GPTPROJECT_S_OBJECTIVE);

						var pIObjectiveElement = g_CurrentProject.Objective;
						if(0 != pIObjectiveElement.GetContentCount())
						{
							ObjectiveView.ComplexCellContent.ReadContentFromElement(pIObjectiveElement);
						}
					}

					//
					// requires transaction
					//
					function SetContactList(i_ContactNameURLMap)
					{
						// only use contacts that have role of manager
						g_ContactNameURLMap = new ContactNameURLMap();
						
						for(var i = 0; i < i_ContactNameURLMap.GetCount(); ++i)
						{
							var CurrContact = i_ContactNameURLMap.GetAt(i);
							if(CHF_IsManager(CurrContact.ContactURL))
							{
								g_ContactNameURLMap.Add(CurrContact.Name, CurrContact.ContactURL);
							}
						}
					}

				␀ð ␀␀␀␀␀␀␀␀␀	function RefreshContactList()
					{
						// reset UI
						TeamLeaderValue.ResetContent();

						// populate cache and combo box
						for(var i = 0; i < g_ContactNameURLMap.GetCount(); ++i)
						{
							var CurrentContactNameURLTuple = g_ContactNameURLMap.GetAt(i);
							var FullName = CurrentContactNameURLTuple.Name;

							TeamLeaderValue.AddItem(FullName);
						}
					}

					function SetProject(i_Project)
					{
						g_CurrentProject = i_Project;
					}

					function GetProject()
					{
						return g_CurrentProject;
					}

					function OKButton_OnCommand(i_pICommand)
					{
						GrooveDebugFunctions.DisplayAssert(null != g_CurrentProject, "Missing project item");

						// must have a valid project name
						if(0 == ProjectNameValue.Text.length)
						{
							PHF_AlertUser(GPTPROJECT_MB_PROJECT_NAME_REQUIRED);
							ProjectNameValue.Text = g_CurrentProject.Name;
							ConfigureInfoViewContainer.SetFocus("ProjectNameValue");
							ProjectNameValue.SetSelection(0, g_CurrentProject.Name.length);
							return;
						}

						var ScheduledStart = ParseDayAndDate(ScheduledStartDateValue.Text, DATE_STYLE);

						// validate start date
						if(isNaN(ScheduledStart))
						{
							PHF_AlertUser(GPTPROJECT_MB_ENTER_VALID_START_DATE);
							ConfigureInfoViewContainer.SetFocus("ScheduledStartDateValue");
							ScheduledStartDateValue.SetSelection(0, ScheduledStartDateValue.Text.length);
							return;
						}

						// move start date to a work day if necessary
						var WorkWeekScheduledStart = g_ProjectDMD.GetStartDateInWorkWeek(ScheduledStart);
						if(WorkWeekScheduledStart != ScheduledStart)
						{
							var UpdatedScheduledStart = FormatDayAndDate(WorkWeekScheduledStart, DATE_STYLE)
							ScheduledStartDateValue.Text = UpdatedScheduledStart;
							PHF_AlertUser(GPTPROJECT_MB_START_MOVED_TO_WORKDAY + UpdatedScheduledStart + GPTPROJECT_MB_DOT);
						}

						// update our project item
						var ObjectiveElement = CreateNewElement("urn:groove.net:RichText");
						ObjectiveView.ComplexCellContent.WriteContentToElement(ObjectiveElement);

						// translate contact name to URL
						var ContactURL = "";
						if("" != TeamLeaderValue.GetSelectionText())
						{
							ContactURL = g_ContactNameURLMap.FindByName(TeamLeaderValue.GetSelectionText());
						}

						var cNull = GrooveScriptFunctions.CreateNewObject("");

						var pIProjectItemDetailsFactory = GrooveScriptFunctions.CreateNewObject("Groove.ProjectItemDetailsFactory");
						g_CurrentProject = pIProjectItemDetailsFactory.CreateProjectItem
										(
											g_CurrentProject.ID,				// project id
											ProjectNameValue.Text,				// project name
											ContactURL,							// project leader contact url
											g_CurrentProject.Status,			// project status
