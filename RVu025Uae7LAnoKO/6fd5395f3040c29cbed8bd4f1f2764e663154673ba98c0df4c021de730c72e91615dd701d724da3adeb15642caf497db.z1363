					function GetDateFromPopup()
					{
						// Open window for subform
						var Window = MHF_CreateDialogBox(MonthPopupSubForm, "", 210,220, false);
						if (!Window.DoesDelegateComponentExist())
							return;

						var MonthPopupHost = Window.OpenDelegateComponent();
						var MonthPopupCode = MonthPopupHost.GetScriptDispatch();

						// If we do not have valid project time, use current time.
						if (isNaN(g_ProjectStartTime))
							g_ProjectStartTime = new Date().valueOf();

						// Get default date for popup display.
						var DefaultDateMS = ParseDayAndDate(StartDateEdit.Text,
															GrooveIntlDateFormatStyle_Short);
						if(isNaN(DefaultDateMS))
							DefaultDateMS = g_ProjectStartTime;

						// Populate the popup calendar and display.
						MonthPopupCode.SetSelection(DefaultDateMS);
						if(GrooveMessageBoxResultCode_OK == Window.DoModal())
						{
							// Get Start and end dates from popup
							g_TaskStartTime = MonthPopupCode.GetStartDate();
							var EndDate = MonthPopupCode.GetEndDate();

						    // Make sure stat date is proper relative to project date.
							if(g_ProjectStartTime > 0 && g_TaskStartTime >= g_ProjectStartTime)
								PutStartDate(g_TaskStartTime);
							else
							{
								PHF_AlertUser(GPTPROJECT_MB_START_DATE_AFTER_PROJECT_DATE);
								return;
							}

							// Move start constraint to a work day if necessary
							var WorkWeekUserStart = g_ProjectDMD.GetStartDateInWorkWeek(g_TaskStartTime);
							if(WorkWeekUserStart != g_TaskStartTime)
							{
								g_TaskStartTime = WorkWeekUserStart;
								PutStartDate(g_TaskStartTime);
								PHF_AlertUser(GPTPROJECT_MB_START_MOVED_TO_WORKDAY + StartDateEdit.Text + GPTPROJECT_MB_DOT);
							}

							// Calculate duration
							if (null != EndDate && g_TaskStartTime != EndDate && EndDate >= g_TaskStartTime)
							{
								DurationEdit.Text =  g_ProjectDMD.GetWorkWeekDuration(g_TaskStartTime, EndDate)/MSPERDAY + 1
							}
						}
					}

