					function CanDeleteItem(i_ID, i_CheckChildren)
					{
						if(GrooveRunTimeData.GetItem("PROJECT_ID") == i_ID)
						{
							return false;
						}

						if (false == i_CheckChildren)
						{
							return PDHF_CanUserRemoveTask(CHF_GetUserURL(), i_ID);
						}
						else
						{
							return PDHF_CanUserRemoveTaskAndChildren(CHF_GetUserURL(), i_ID);
						}
					}

					function CanMoveItemUp(i_ID)
					{
						if(GrooveRunTimeData.GetItem("PROJECT_ID") == i_ID)
						{
							return false;
						}

						// can't move up past the first child in the parent
						var PreviousSiblingID = PDHF_GetPreviousSiblingID(i_ID);
						if(-1 == PreviousSiblingID)
						{
							return false;
						}

						// ACL check
						return PDHF_CanUserMoveItem(CHF_GetUserURL(), i_ID);
					}

					function CanMoveItemDown(i_ID)
					{
						if(GrooveRunTimeData.GetItem("PROJECT_ID") == i_ID)
						{
							return false;
						}

						// can't move item past the end of the current parent
						var NextSiblingID = Collection.OpenCollection().OpenRecordID(i_ID, GrooveCollectionNavigationOp_NextPeer);
						if(-1 == NextSiblingID)
						{
							return false;
						}

						// ACL check
						return PDHF_CanUserMoveItem(CHF_GetUserURL(), i_ID);
					}

					// move the item closer to the root
					function CanPromoteItem(i_ID)
					{
						// can never promote the project item
						if(GrooveRunTimeData.GetItem("PROJECT_ID") == i_ID)
						{
							return false;
						}

						// all items are children of the project item
						var pIRecord = RecordSetEngine.OpenRecord(i_ID);
						if(pIRecord.OpenField("_ParentID") == GrooveRunTimeData.GetItem("PROJECT_ID"))
						{
							return false;
						}

						// ACL check
						return PDHF_CanUserPromoteItem(CHF_GetUserURL(), i_ID);
					}

					// move the item further from the root
					function CanDemoteItem(i_ID)
					{
						// can never demote the project item
						if(GrooveRunTimeData.GetItem("PROJECT_ID") == i_ID)
						{
							return false;
						}

						// can't demote when it's the first item in the list
						var PreviousSiblingID = PDHF_GetPreviousSiblingID(i_ID);
						if(-1 == PreviousSiblingID)
						{
							return false;
						}

						// ACL check
						return PDHF_CanUserDemoteItem(CHF_GetUserURL(), i_ID);
					}

					function DoesDemoteCreateCycle(i_ID, i_ParentID)
					{
						// can never demote the project item
						if(GrooveRunTimeData.GetItem("PROJECT_ID") == i_ID)
						{
							GrooveErrorFunctions.SetCurrentError(PROJECTTRACKING_E_INVALID_OPERATION, "Cannot demote project item");
							return true;
						}

						if (null == i_ParentID)
							i_ParentID = PDHF_GetPreviousSiblingID(i_ID);

						return PDHF_DoesDemoteCreateCycle(i_ID, i_ParentID);
					}

					function SaveAsTemplate(i_pIParentElement, i_pIItemIDEnum, i_IncludeChildren, i_Flags)
					{
						try
						{
							var NSP = "urn:groove.net:";
							var pITool = PropertyList.OpenProperty(PROPERTY_TOOL);
							var pITemplateDescriptor = pITool.TemplateDescriptor;
							var pITemplateElmnt = i_pIParentElement.CreateElement(NSP + "Template", i_pIParentElement);
							pITemplateElmnt.SetAttribute("URL", pITemplateDescriptor.ComponentResourceURL);
							pITemplateElmnt.SetAttribute("Category", pITemplateDescriptor.Category);
							pITemplateElmnt.SetAttribute("DisplayType", pITemplateDescriptor.DisplayType);
							pITemplateElmnt.SetAttribute("Name", pITemplateDescriptor.Name);
							pITemplateElmnt.SetAttribute("DisplayName", pITemplateDescriptor.DisplayName);

							var pIInitComponentElmnt = pITemplateElmnt.CreateElement(NSP + "InitializationComponent", pITemplateElmnt);
							pIInitComponentElmnt.SetAttribute("Name", "ProjectDataModel");

							var pIPropertyListElmnt = pIInitComponentElmnt.CreateElement(NSP + "PropertyList", pIInitComponentElmnt);
							var pIPropertyElmnt = pIPropertyListElmnt.CreateElement(NSP + "Property", pIPropertyListElmnt);
							pIPropertyElmnt.SetAttribute("Name", "ProjectData");
							var pIPropertyValueElmnt = pIPropertyElmnt.CreateElement(NSP + "PropertyValue", pIPropertyElmnt);

							var pIProjectData = PDHF_SerializeItemsToElement(i_pIItemIDEnum, i_IncludeChildren, i_Flags);
							pIPropertyValueElmnt.CreateElementCopy(pIProjectData, pIPropertyValueElmnt, false, GrooveXLinkCopyOption_Default);
							
							return pITemplateElmnt;
						}
						catch(e)
						{
							PDHF_SetError(PROJECTTRACKING_E_OPERATION_FAILED, "Error saving as template");
						}
					}


					function SerializeItemsToStream(i_pIItemIDEnum, i_IncludeChildren, i_pByteOutputStream, i_Flags)
					{
						try
						{
							if(GrooveProjectTrackingSerializeFlags_Predecessors & i_Flags)
							{
								GrooveErrorFunctions.SetCurrentError(PROJECTTRACKING_E_INVALID_OPERATION, "Serializing predecessors is not supported.");
								return;
							}

							// Include attachments? Or use auto-format (currently MIME) to save binary content.
							var SerializeFormat = (0 != (GrooveProjectTrackingSerializeFlags_Attachments & i_Flags))
											    ? GrooveSerializeType_Auto
												: GrooveSerializeType_XML;
							
							var pIElement = PDHF_SerializeItemsToElement(i_pIItemIDEnum, i_IncludeChildren, i_Flags);
							pIElement.SerializeToStream(i_pByteOutputStream, SerializeFormat,
														GrooveCharEncodingType_UTF8,
														GrooveSerializeOption_EnableXlinkCompression);
						}
						catch(e)
						{
							PDHF_SetError(PROJECTTRACKING_E_OPERATION_FAILED, "Error serializing");
						}
					}

					function ImportItemsFromStream(i_pByteInputStream, i_pIProgress)
					{
						if(true == PDHF_IsCalculationDeferred())
						{
							GrooveErrorFunctions.SetCurrentError(PROJECTTRACKING_E_INVALID_OPERATION, "Cannot import with deferred calculation.");
							return;
						}

						// add this listener
						PDHF_AddListener(i_pIProgress);

						var DynamicsMgr = PropertyList.OpenProperty(PROPERTY_DYNAMICS_MANAGER);
						var pIDelta = DynamicsMgr.CreateDelta();
						try
						{
							var pIHeaderElement = PDHF_GetHeaderElementFromStream(i_pByteInputStream);
							
							// import to the root
							var pINewItemEnum = PDHF_ImportFromHeaderElement(GrooveRunTimeData.GetItem("PROJECT_ID"), -1 /* prev sibling */, pIHeaderElement);

							// trigger recalc
							if(null != pINewItemEnum)
							{
								var pIRecalculateCmdRecord = PDHF_BuildRecalculateMultipleCmdRecord(pINewItemEnum);
								RecordSetEngine.ReplaceRecord(pIRecalculateCmdRecord.RecordID, pIRecalculateCmdRecord);
							}

							pIDelta.Commit();
						}
						catch(e)
						{
							pIDelta.Abort();
							PDHF_SetError(PROJECTTRACKING_E_OPERATION_FAILED, "Error importing");
						}

						// remove this listener
						PDHF_RemoveListener(i_pIProgress);
					}



					function InsertItemsFromStream(i_ParentID, i_PreviousSiblingID, i_pByteInputStream, i_pIProgress)
					{
						if(GrooveRunTimeData.GetItem("PROJECT_ID") == i_PreviousSiblingID)
						{
							GrooveErrorFunctions.SetCurrentError(PROJECTTRACKING_E_INVALID_OPERATION, "Cannot create siblings of the project item.");
							return;
						}

						if(true == PDHF_IsCalculationDeferred())
						{
							GrooveErrorFunctions.SetCurrentError(PROJECTTRACKING_E_INVALID_OPERATION, "Cannot import with deferred calculation.");
							return;
						}

						// Add this listener
						PDHF_AddListener(i_pIProgress);

						var DynamicsMgr = PropertyList.OpenProperty(PROPERTY_DYNAMICS_MANAGER);
						var pIDelta = DynamicsMgr.CreateDelta();
						try
						{
							// Import to specified location
							var pIHeaderElement = PDHF_GetHeaderElementFromStream(i_pByteInputStream);
							var pINewItemEnum = PDHF_ImportFromHeaderElement(i_ParentID, i_PreviousSiblingID, pIHeaderElement);

							// Trigger recalc
							if(null != pINewItemEnum)
							{
								var pIRecalculateCmdRecord = PDHF_BuildRecalculateMultipleCmdRecord(pINewItemEnum);
								RecordSetEngine.ReplaceRecord(pIRecalculateCmdRecord.RecordID, pIRecalculateCmdRecord);
							}
							
							pIDelta.Commit();
						}
						catch(e)
						{
							pIDelta.Abort();
							PDHF_SetError(PROJECTTRACKING_E_OPERATION_FAILED, "Error importing");
						}

						// Remove this listener
						PDHF_RemoveListener(i_pIProgress);
					}


