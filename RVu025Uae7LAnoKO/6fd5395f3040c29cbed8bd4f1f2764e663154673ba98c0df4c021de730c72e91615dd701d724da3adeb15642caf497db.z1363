					//
					// Internal helper routines
					//
					//

					function ScheduledStartPopupButton_OnCommand(i_pICommand)
					{
						GetDateFromPopup(ScheduledStartDateValue);
					}

					function GetDateFromPopup(i_EditBox)
					{
						var pIWindow = MHF_CreateDialogBox(MonthPopupSubForm, "", 210,280, false);
						var MonthPopupHost = pIWindow.OpenDelegateComponent();
						var MonthPopupCode = MonthPopupHost.GetScriptDispatch();

						var DefaultDateMS = ParseDayAndDate(i_EditBox.Text,
															GrooveIntlDateFormatStyle_Short);

						if(isNaN(DefaultDateMS))
						{
							DefaultDateMS = new Date().valueOf();
						}

						MonthPopupCode.SetSelection(DefaultDateMS);

						if(GrooveDialogBoxResultCode_OK == pIWindow.DoModal())
						{
							i_EditBox.Text = FormatDayAndDate(MonthPopupCode.GetStartDate(),
															  GrooveIntlDateFormatStyle_Short);
						}
					}

					function RTFInsertTime()
					{
						var curDate = new Date();
						var curDateString = FormatDateTime(curDate.valueOf(), 3, 3);
						ObjectiveView.SubmitText(curDateString);
					}

					function CutToClipboard()
					{
						SendClipboardCommand(GROOVE_VK_X);
					}

					function CopyToClipboard()
					{
						SendClipboardCommand(GROOVE_VK_C);
					}

					function PasteFromClipboard()
					{
						SendClipboardCommand(GROOVE_VK_V);
					}

					function CanCutToClipboard()
					{
						return CanCopyToClipboard();
					}

					function CanCopyToClipboard()
					{
						if(DoesDescriptionHaveFocus())
						{
							return ObjectiveView.CanCopy;
						}

						return false;
					}

					function CanUndo()
					{
						if(DoesDescriptionHaveFocus())
						{
							return ObjectiveView.CanUndo;
						}

						return false;
					}

					function CanRedo()
					{
						if(DoesDescriptionHaveFocus())
						{
							return ObjectiveView.CanRedo;
						}

						return false;
					}

					function CanPasteFromClipboard()
					{
						if(DoesDescriptionHaveFocus())
						{
							return ObjectiveView.CanPaste;
						}
						
						return false;
					}

					function SendClipboardCommand(i_CommandChar)
					{
						if(DoesDescriptionHaveFocus())
						{
							if (GROOVE_VK_DELETE == i_CommandChar)
							{
								ObjectiveView.SubmitKeyPress(GrooveTextKey_Delete, false, false);
							}
							else
							{
								var ControlCmd = String.fromCharCode(i_CommandChar & 0x1F)
								ObjectiveView.SubmitText(ControlCmd);
							}
						}
					}


					function GetItem()
					{
						var pIProjectItemDetailsFactory = GrooveScriptFunctions.CreateNewObject("Groove.ProjectItemDetailsFactory");
						return pIProjectItemDetailsFactory.CreateProjectItem
										(
											g_CurrentProject.ID,			// project id
											GetName(),						// project name
											GetAssignedToContactURL(),		// project leader contact url
											GetStatus(),					// project status
											g_CurrentProject.CreationTime,	// project creation time
											GetScheduledStart(),			// scheduled start
											GetScheduledDuration(),			// scheduled duration
											GetScheduledFinish(),			// scheduled finish
											GetScheduledFinish(),			// Display finish
											GetActualStart(),				// actual start
											GetActualFinish(),				// actual finish
											GetDescription(),				// objective element
											true,							// is read
											g_CurrentProject.OpenAttachmentDetailsEnum(), // attachments
											g_CurrentProject.ProjectCreatorContactURL
										);
					}

					function GetName()
					{
						return ProjectName.Text;
					}

					function GetID()
					{
						return g_CurrentProject.ID;
					}

					function GetParentID()
					{
						return -1;  // project item is the root
					}

					function GetPreviousSiblingID()
					{
						return -1; // project item is the root
					}

					function GetAssignedToContactURL()
					{
						var ContactURL = "";
						if("" != TeamLeaderValue.GetSelectionText())
						{
							ContactURL = g_ContactNameURLMap.FindByName(TeamLeaderValue.GetSelectionText());
						}

						return ContactURL;
					}

					function GetStatus()
					{
						// this field is always calculated
						return g_CurrentProject.Status;
					}

					function GetPriority()
					{
						return ""; // project does not have priority
					}

					//
					// The project does not have a start constraint
					//
					function GetUserStartConstraint()
					{
						return 0;
					}

					//
					// The project does not have a start constraint
					//
					function GetConstrainStartFlag()
					{
						return false;
					}

					function GetScheduledStart()
					{
						return ParseDayAndDate(ScheduledStartDateValue.Text,
											   GrooveIntlDateFormatStyle_Short);
					}

					function GetScheduledDuration()
					{
						// this field is always calculated
						return g_CurrentProject.ScheduledDuration;
					}

					function GetScheduledFinish()
					{
						// this field is always calculated
						return g_CurrentProject.ScheduledFinish; 
					}

					function GetActualStart()
					{
						// this field is always calculated
						return g_CurrentProject.ActualStart;
					}

					function GetActualFinish()
					{
						// this field is always calculated
						return g_CurrentProject.ActualFinish;
					}

					function GetIsPrivate()
					{
						return false; // project cannot be private
					}

					function GetDescription()
					{
						var ObjectiveElement = CreateNewElement("urn:groove.net:RichText");
						ObjectiveView.ComplexCellContent.WriteContentToElement(ObjectiveElement);

						return ObjectiveElement;
					}

					//
					// The project item cannot have predecessors
					//
					function GetNewPredecessorsArray()
					{
						return new Array();
					}

					//
					// The project item cannot have predecessors
					//
					function GetRemovePredecessorsArray()
					{
						return new Array();
					}


					function DoesDescriptionHaveFocus()
					{
						if (UIPropertyList.PropertyExists(UI_PROPERTY_UIACTIVE_COMPONENT))
						{
							var pIFocusComponent = UIPropertyList.OpenProperty(UI_PROPERTY_UIACTIVE_COMPONENT);
							if("ObjectiveView" == pIFocusComponent.Component.OpenName())
								return true;
						}

						return false;
					}

					function LayoutActualDatesForStatus(i_Status)
					{
						switch(i_Status)
						{
							case GrooveProjectToolsetStatusType_Complete:
								ActualStartLabel.Visible = true;
								ActualStartDateValue.Visible = true;
								ActualStartPopupButton.Visible = true;

								ActualFinishLabel.Visible = true;
								ActualFinishDateValue.Visible = true;
								ActualFinishPopupButton.Visible = true;
								break;
							case GrooveProjectToolsetStatusType_InProgress:
								ActualStartLabel.Visible = true;
								ActualStartDateValue.Visible = true;
								ActualStartPopupButton.Visible = true;

								ActualFinishLabel.Visible = false;
								ActualFinishDateValue.Visible = false;
								ActualFinishPopupButton.Visible = false;
								break;
							default:
								ActualStartLabel.Visible = false;
								ActualStartDateValue.Visible = false;
								ActualStartPopupButton.Visible = false;

								ActualFinishLabel.Visible = false;
								ActualFinishDateValue.Visible = false;
								ActualFinishPopupButton.Visible = false;
								break;
						}
					}

					function SaveAndCloseButton_OnCommand(i_pICommand)
					{
						// Validate the data
						if(true == ValidateInput())
						{
							g_ParentSubForm.OnCommand(i_pICommand.UICommand);
						}
					}

					function CloseButton_OnCommand(i_pICommand)
					{
						g_ParentSubForm.OnCommand(i_pICommand.UICommand)
					}

					function AttachmentsMenu_OnCommand(i_pICommand)
					{
						var bAttachmentsChanged = g_ParentSubForm.ProcessAttachmentsMenuCommand(i_pICommand, g_CurrentProject.ID);
						if (bAttachmentsChanged)
						{
							g_AttachmentCount = g_ParentSubForm.RefreshAttachmentsMenu
														(g_CurrentProject.ID,
														 AttachmentsMenu.UICommandContainer,
														 AttachmentButton,
														 true); // Edit mode
						}
					}

					function AttachmentButton_OnUpdateCommandState(i_pICommand)
					{
						i_pICommand.Label = g_ParentSubForm.Attachments.GenerateLabel(g_AttachmentCount);
