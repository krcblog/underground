				function Initialize()
				{
					g_pAccount = OpenAccountFromPropertyList(PropertyList);
					g_pWindowManager = GrooveScriptFunctions.CreateNewObject("Groove.WindowManager");
					g_pWindowProperties = GrooveScriptFunctions.CreateNewObject("Groove.WindowProperties");
				}

				function Terminate()
				{
				}

				function StartImport(i_pParentWindow, i_pProjectDataDelegate)
				{
					var FilePath = ChooseMSProjectFileToImportFrom();
					if("" == FilePath)
						return;

					DisplayProgressWindow(FilePath, i_pParentWindow, i_pProjectDataDelegate, true);
					return;
				}

				function StartExport(i_pParentWindow, i_pProjectDataDelegate)
				{
					var	FilePath = ChooseMSProjectFileToExportTo(i_pParentWindow);
					if("" == FilePath)
						return;

					DisplayProgressWindow(FilePath, i_pParentWindow, i_pProjectDataDelegate, false);
					return;
				}

				function ChooseMSProjectFileToExportTo(i_pParentWindow)
				{
					while(1)
					{
						FilePath = "";
						var DefaultDirectory;
						if(true == PropertyList.PropertyExists("DefaultDirectoryForExport"))
							DefaultDirectory = PropertyList.OpenPropertyAsString("DefaultDirectoryForExport");
						else
							DefaultDirectory = "";

						var DefaultFileName;
						if(true == PropertyList.PropertyExists("DefaultFileNameForExport"))
							DefaultFileName = PropertyList.OpenPropertyAsString("DefaultFileNameForExport");
						else
							DefaultFileName = "";

						var FileResult = App.SaveFileDialog(g_cSTR_EXPORT_TO_CAPTION, g_cSTR_FILTER_STRING, DefaultDirectory, DefaultFileName, g_pAccount);

						if (GrooveDialogBoxResultCode_OK == FileResult.Result)
							FilePath = FileResult.Data;
                        else
						{
							FilePath = "";
							break;
						}

						var fso = GrooveScriptFunctions.CreateNewObject("Groove.Win32FileSystemServices");

						if("" != FilePath)
						{ 	// ensure that file has mpp extension
							var FileExtension = fso.OpenFileExtension(FilePath);
							if(FileExtension.toLowerCase() != "mpp")
							{
								var BaseFileName = fso.OpenFileBaseName(FilePath);
								var ParentFolder = fso.OpenFileFolderPath(FilePath);
								var FileName = BaseFileName + ".mpp";␀ð ␀␀␀␀␀␀␀␀␀

								FilePath = ParentFolder + FileName;

								// Let us check that file does not exist
								if(true == fso.DoesFileExist(FilePath))
								{
									var Message = "File " + FilePath + " already exists. Do you want to replace it?"
									if(GrooveMessageBoxResultCode_No == UltimateMessageBox(Message, g_cSTR_EXPORT_TO_CAPTION, GrooveMessageBoxStyle_YesNo, GrooveMessageBoxIcon_Exclamation, i_pParentWindow ))
									{

										var ParentFolder = fso.OpenFileFolderPath(FilePath);
										var FileName = fso.OpenFileFullName(FilePath);

										PropertyList.SetPropertyAsString("DefaultDirectoryForExport", ParentFolder,  GroovePropertyFlag_Private | GroovePropertyFlag_IdentitySerialize);
										PropertyList.SetPropertyAsString("DefaultFileNameForExport", FileName,  GroovePropertyFlag_Private | GroovePropertyFlag_IdentitySerialize);
										continue;
									}
								}
							}

							var ParentFolder = fso.OpenFileFolderPath(FilePath);
							var FileName = fso.OpenFileFullName(FilePath);

							PropertyList.SetPropertyAsString("DefaultDirectoryForExport", ParentFolder,  GroovePropertyFlag_Private | GroovePropertyFlag_IdentitySerialize);
							PropertyList.SetPropertyAsString("DefaultFileNameForExport", FileName,  GroovePropertyFlag_Private | GroovePropertyFlag_IdentitySerialize);
						}


						break;
					}

					return FilePath;
				}

				function ChooseMSProjectFileToImportFrom()
				{
					var FilePath = "";

					var DefaultDirectory;
					if(true == PropertyList.PropertyExists("DefaultDirectoryForImport"))
						DefaultDirectory = PropertyList.OpenPropertyAsString("DefaultDirectoryForImport");
                    else
						DefaultDirectory = "";

					var DefaultFileName;
					if(true == PropertyList.PropertyExists("DefaultFileNameForImport"))
						DefaultFileName = PropertyList.OpenPropertyAsString("DefaultFileNameForImport");
                    else
						DefaultFileName = "";

					var FileResult = App.OpenFileDialog(g_cSTR_IMPORT_FROM_CAPTION, g_cSTR_FILTER_STRING, DefaultDirectory, DefaultFileName, g_pAccount);

					if (GrooveDialogBoxResultCode_OK == FileResult.Result)
						FilePath = FileResult.Data;

                    if("" != FilePath)
					{
						var fso = GrooveScriptFunctions.CreateNewObject("Groove.Win32FileSystemServices");
						var ParentFolder = fso.OpenFileFolderPath(FilePath);
						var FileName = fso.OpenFileFullName(FilePath);

						PropertyList.SetPropertyAsString("DefaultDirectoryForImport", ParentFolder,  GroovePropertyFlag_Private | GroovePropertyFlag_IdentitySerialize);
						PropertyList.SetPropertyAsString("DefaultFileNameForImport", FileName,  GroovePropertyFlag_Private | GroovePropertyFlag_IdentitySerialize);
					}

					return FilePath; 
				}

