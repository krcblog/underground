						// Remember original item info
						g_CurrentProject = i_Project;

						// Update UI with current members
						RefreshContactList();
						
						var ScheduledStart = new Date(g_CurrentProject.ScheduledStart);
						var ScheduledFinish = new Date(g_CurrentProject.ScheduledFinish);

						// Item name
						ProjectNameLabel.SetText(GPTPROJECT_S_PROJECTNAME);
						ProjectName.Text = g_CurrentProject.Name;
						
						// Team leader
						TeamLeaderLabel.SetText(GPTPROJECT_S_LEADER);

						if("" != g_CurrentProject.ProjectLeaderContactURL)
						{
							var LeaderIndex = GrooveIndex_NotFound;
							
							if(g_ContactNameURLMap.URLExists(g_CurrentProject.ProjectLeaderContactURL))
							{
								var Name = g_ContactNameURLMap.FindByURL(g_CurrentProject.ProjectLeaderContactURL);
								LeaderIndex = TeamLeaderValue.FindStringExact(0, Name);
							}

							if(GrooveIndex_NotFound != LeaderIndex)
							{
								TeamLeaderValue.SetSelectionByIndex(LeaderIndex);
							}
						}

						// Scheduled start
						ScheduledStartLabel.SetText(GPTPROJECT_S_SCHEDULED_START);
						ScheduledStartDateValue.Text = FormatDayAndDate(g_CurrentProject.ScheduledStart,
																		GrooveIntlDateFormatStyle_Short);

						// Scheduled finish
						ScheduledFinishLabel.SetText(GPTPROJECT_S_SCHEDULED_FINISH);
						ScheduledFinishDateValue.SetText(FormatDayAndDate(g_CurrentProject.DisplayFinish,
																		  GrooveIntlDateFormatStyle_Short));

						// Status
						StatusLabel.SetText(GPTPROJECT_S_STATUS);
						StatusValue.SetText(MHF_GetStatusStringFromEnum(g_CurrentProject.Status));

						// Show actual start/finish?
						LayoutActualDatesForStatus(g_CurrentProject.Status);

						// Actual start
						ActualStartLabel.SetText(GPTPROJECT_S_ACTUALSTART);
						ActualStartDateValue.SetText(FormatDayAndDate(g_CurrentProject.ActualStart,
																	  GrooveIntlDateFormatStyle_Short));

						// Actual finish
						ActualFinishLabel.SetText(GPTPROJECT_S_ACTUALFINISH);
						ActualFinishDateValue.SetText(FormatDayAndDate(g_CurrentProject.ActualFinish,
																	   GrooveIntlDateFormatStyle_Short));
						
						// Set labels
						ObjectiveLabel.SetText(GPTPROJECT_S_OBJECTIVE);

						//
						if(0 != i_Project.Objective.GetContentCount())
						{
							ObjectiveView.ComplexCellContent.ReadContentFromElement(i_Project.Objective);
						}
						else
						{
							ObjectiveView.RTFContent = "";
						}

						// Attachments
						g_AttachmentCount = g_ParentSubForm.RefreshAttachmentsMenu
															(g_CurrentProject.ID,
														     AttachmentsMenu.UICommandContainer,
														     AttachmentButton,
														     true); // Edit mode

						// Set input focus on project name
						ProjectDetailsHeaderViewContainer.SetFocus("ProjectName");
					}

					function HasChanged()
					{
						// this only compares the fields that we are actually using
						return g_CurrentProject.Name != GetName() ||
							   g_CurrentProject.ProjectLeaderContactURL != GetAssignedToContactURL() ||
							   g_CurrentProject.Status != GetStatus() ||
							   g_CurrentProject.ScheduledStart != GetScheduledStart() ||
							   g_CurrentProject.ScheduledDuration != GetScheduledDuration() ||
							   false == g_CurrentProject.Objective.Equals(GetDescription()) ||
							   true == g_ParentSubForm.Attachments.HasChanged();
					}

					function ValidateInput()
					{
						// Validate start date
						if (false == ValidateStartDate(true))
						{
							// Reset start date field to default if there is no data.
							if ("" == ScheduledStartDateValue.Text)
								ScheduledStartDateValue.Text =
												 FormatDayAndDate(g_CurrentProject.ScheduledStart,
																  GrooveIntlDateFormatStyle_Short);

							// Set focus and selection to the invalid item.
							ProjectDetailsHeaderViewContainer.SetFocus("ScheduledStartDateValue");
							ScheduledStartDateValue.SetSelection(0, ScheduledStartDateValue.Text.length);

							return false;
						}

						// Validate name
						if ("" == TrimHead(ProjectName.Text))
						{
							PHF_AlertUser(GPTPROJECT_MB_PROJECT_NAME_REQUIRED);
							ProjectName.Text = g_CurrentProject.Name;

							// Set focus and selection to the invalid item.
							ProjectDetailsHeaderViewContainer.SetFocus("ProjectName");
							ProjectName.SetSelection(0, ProjectName.Text.length);
							return false;
						}

						return true;
					}

					function ValidateStartDate(i_PopupMsg)
					{
						var StartDate = ParseDayAndDate(ScheduledStartDateValue.Text,
														GrooveIntlDateFormatStyle_Short);
						if(StartDate < 0 || isNaN(StartDate))
						{
							if(i_PopupMsg)
								PHF_AlertUser(GPTPROJECT_MB_ENTER_VALID_START_DATE);

							return false;
						}

						// Move start constraint to a work day if necessary.
						var WorkWeekUserStart = g_ProjectDMD.GetStartDateInWorkWeek(StartDate);
						if(WorkWeekUserStart != StartDate)
						{
							var UpdatedUserStart = FormatDayAndDate(WorkWeekUserStart,
																	GrooveIntlDateFormatStyle_Short);
							ScheduledStartDateValue.Text = UpdatedUserStart;
							
							if(i_PopupMsg)
							{
								PHF_AlertUser(GPTPROJECT_MB_START_MOVED_TO_WORKDAY + UpdatedUserStart + GPTPROJECT_MB_DOT);
							}
						}

						return true;
					}

