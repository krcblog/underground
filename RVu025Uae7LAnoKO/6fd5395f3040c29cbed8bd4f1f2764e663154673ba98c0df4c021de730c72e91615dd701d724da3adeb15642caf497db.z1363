	function PrintCtrl_DocumentComplete(i_Disp, i_URL)
	{
		DeleteXMLFile();
		if(g_NavigationStarted)
		{
			g_NavigationStarted = false;
			if(i_URL != "about:blank")
			{
				g_bRefreshNeeded = false;
				SaveButton.Enabled = true;
				PrintButton.Enabled = true;
			}

			g_WindowsManager.ShowWaitCursor(false);
		}
	}

//************************************************************
// Command Handlers
//************************************************************

	function NavigateToNewReport()
	{
		// Return if already started
		if(g_NavigationStarted)
			return;

		g_NavigationStarted = true;
		g_WindowsManager.ShowWaitCursor(true);

		// Delete current report file
		DeleteXMLFile();

		// Get current report selection.
		var Folder = ReportTypeComboBox.GetSelectionText();

		if(g_ConfigurationSubFormCode)
			g_ConfigurationSubFormCode.IssueFilteringRequest(g_DefaultReportFolder + "\\" + Folder, g_bRefreshNeeded);
		else
			FireEvent("", "OnFilter", g_DefaultReportFolder + "\\" + Folder, g_bRefreshNeeded);
	}

