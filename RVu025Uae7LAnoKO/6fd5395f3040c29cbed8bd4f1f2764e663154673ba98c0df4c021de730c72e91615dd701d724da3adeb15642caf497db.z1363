							var pIDeleteCmdRecord = PDHF_BuildDeleteTasksCmdRecord(i_pItemIDEnum);
							RecordSetEngine.ReplaceRecord(pIDeleteCmdRecord.RecordID, pIDeleteCmdRecord);
							pIDelta.Commit();
						}
						catch(e)
						{
							pIDelta.Abort();

							PDHF_SetError(PROJECTTRACKING_E_OPERATION_FAILED, "Error deleting tasks");
						}

						// Remove listener
						PDHF_RemoveListener(i_pIProgress);
					}
					//
					// caller is responsible for locking the database for writing
					//
					function ReplaceProject(i_GrooveProjectDetails, i_pIProgress)
					{
						// Add this listener
						PDHF_AddListener(i_pIProgress);

						var DynamicsMgr = PropertyList.OpenProperty(PROPERTY_DYNAMICS_MANAGER);
						var pIDelta = DynamicsMgr.CreateDelta();
						try
						{
							var OriginalStart;
							var Record = RecordSetEngine.OpenRecord(GrooveRunTimeData.GetItem("PROJECT_ID"));

							var FirstConfigure = PDHF_UpdateFieldIfChanged(Record, "IsConfigured", true);
							PDHF_UpdateFieldIfChanged(Record, "Name", i_GrooveProjectDetails.Name);
							PDHF_UpdateElementFieldIfChanged(Record, "Objective", i_GrooveProjectDetails.Objective);

							// Update unread mark?
							if(Record.OpenField("_IsRead") != i_GrooveProjectDetails.IsRead)
							{
								RecordSetEngine.MarkRecordRead(Record.RecordID, i_GrooveProjectDetails.IsRead);
							}

							// Generate reschedule command when the start date changes -or- the first
							// time that the user sets the start date, since we may have imported from a 
							// pre-seeded injectable
							if(Record.OpenField("ScheduledStart") != i_GrooveProjectDetails.ScheduledStart ||
							   true == FirstConfigure)
							{
								var pIRescheduleProjectCmdRecord = PDHF_BuildRescheduleProjectCmdRecord(i_GrooveProjectDetails.ScheduledStart);
								RecordSetEngine.ReplaceRecord(pIRescheduleProjectCmdRecord.RecordID, pIRescheduleProjectCmdRecord);
							}

							// Update the team leader last since some permission checks are based on this field
							PDHF_UpdateFieldIfChanged(Record, "TeamLeaderContactURL", i_GrooveProjectDetails.ProjectLeaderContactURL);

							pIDelta.Commit();
						}
						catch(e)
						{
							pIDelta.Abort();
							PDHF_SetError(PROJECTTRACKING_E_OPERATION_FAILED, "Error updating project");
						}

						// remove this listener
						PDHF_RemoveListener(i_pIProgress);
					}


					//
					// caller is responsible for locking the database for writing
					//
					function ReplaceTask(i_GrooveTaskDetails, i_pIProgress)
					{
						// add this listener
						PDHF_AddListener(i_pIProgress);

						var DynamicsMgr = PropertyList.OpenProperty(PROPERTY_DYNAMICS_MANAGER);
						var pIDelta = DynamicsMgr.CreateDelta();
						try
						{
							if(RecordSetEngine.HasRecord(i_GrooveTaskDetails.ID))
							{
								var DurationChanged = false;
								var StartConstraintChanged = false;
								var StatusChanged = false;
								var PriorityChanged = false;
								var ActualStartChanged = false;
								var ActualFinishChanged = false;

								var Record = RecordSetEngine.OpenRecord(i_GrooveTaskDetails.ID);

								// Check permissions
								var bCanReplaceScheduleInfo = CanReplaceItemScheduleInfo(Record.RecordID);

								// Change fields that are not calculated
								if (bCanReplaceScheduleInfo)
								{
									PDHF_UpdateFieldIfChanged(Record, "Name", i_GrooveTaskDetails.Name);
									PDHF_UpdateDoubleFieldIfChanged(Record, "_ParentID", i_GrooveTaskDetails.ParentID);
									PDHF_UpdateFieldIfChanged(Record, "IsPrivate", i_GrooveTaskDetails.IsPrivate);
								}

								PDHF_UpdateElementFieldIfChanged(Record, "Description", i_GrooveTaskDetails.Description);

								// If it's not a summary task, allow dates + status to be set
								if (false == PDHF_HasChildren(Record.RecordID))
								{
									if (bCanReplaceScheduleInfo)
									{
										StartConstraintChanged =  (Record.OpenField("UserStartConstraint") != i_GrooveTaskDetails.UserStartConstraint);
										StartConstraintChanged |= (Record.OpenField("ConstrainStartDate") != i_GrooveTaskDetails.ConstrainStartDate);
										DurationChanged = (Record.OpenField("ScheduledDuration") != i_GrooveTaskDetails.ScheduledDuration);
										PriorityChanged = (Record.OpenField("Priority") != i_GrooveTaskDetails.Priority);
									}

									StatusChanged = (Record.OpenField("Status") != i_GrooveTaskDetails.Status);
									ActualStartChanged = (Record.OpenField("ActualStart") !=  i_GrooveTaskDetails.ActualStart);
									ActualFinishChanged = (Record.OpenField("ActualFinish") != i_GrooveTaskDetails.ActualFinish);
								}

								// Update unread mark?
								if (Record.OpenField("_IsRead") != i_GrooveTaskDetails.IsRead)
								{
									RecordSetEngine.MarkRecordRead(Record.RecordID, i_GrooveTaskDetails.IsRead);
								}

								// If the calculated fields changed, trigger a recalc operation
								if (StartConstraintChanged || DurationChanged || StatusChanged || 
								    PriorityChanged || ActualStartChanged || ActualFinishChanged)
								{
									// Only changing the status fields?
									var StatusOnly = (false == StartConstraintChanged) && 
													 (false == DurationChanged) &&
													 (false == PriorityChanged);

									var pIRecalculateCmdRecord = PDHF_BuildChangeTaskScheduleCmdRecord(i_GrooveTaskDetails, StatusOnly);
									RecordSetEngine.ReplaceRecord(pIRecalculateCmdRecord.RecordID, pIRecalculateCmdRecord);
								}

								if (bCanReplaceScheduleInfo)
								{
									// Replace the assignee last since some permission checks are based on this field
									PDHF_UpdateFieldIfChanged(Record, "AssignedToContactURL", i_GrooveTaskDetails.AssignedToContactURL);
								}
							}

							pIDelta.Commit();
						}
						catch(e)
						{
							pIDelta.Abort();

							// Handle access denied case
							if (-2147024891 == e.number)
							{
								//xxx Don't put up error.  Need to display a message to user instead.
								// PDHF_SetError(e.number, e.message);
							}
							else
								PDHF_SetError(PROJECTTRACKING_E_OPERATION_FAILED, "Updating task");
						}

						// Remove this listener
						PDHF_RemoveListener(i_pIProgress);
					}


