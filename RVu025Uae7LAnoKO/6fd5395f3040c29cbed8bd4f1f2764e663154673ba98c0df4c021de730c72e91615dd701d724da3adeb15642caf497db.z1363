					function Initialize()
					{
						SameLevelRadioButton.CheckState = GrooveCheckState_Checked;
						PlaceAsSubRadioButton.CheckState = GrooveCheckState_Unchecked;
						NoEarlierThanRadioButton.Label = GPTPROJECT_S_CONSTRAINSTARTDATE;
						NoEarlierThanRadioButton.CheckState = GrooveCheckState_Checked;
						ASAPRadioButton.Label = GPTPROJECT_S_STARTASAP;
						ASAPRadioButton.CheckState = GrooveCheckState_Unchecked;
						PredecessorsButton.Label = PHF_GetPredecessorLabel(0);
					}

					function Terminate()
					{
						g_PredecessorCache = null
					}

					//---------------------------------------------------------
					// Command handlers
					//---------------------------------------------------------

					function OnTranslateAccelerator(i_Msg, i_KeyCode, i_KeyState, i_KeyModifiers)
					{
						if(i_Msg == 0x100 && i_KeyCode == 0x54 && i_KeyModifiers == GROOVE_KEYMOD_CONTROL)//Ctrl-T
						{
							RTFInsertTime();
							return true;
						}
						else if(i_Msg == 0x100 && i_KeyCode == 0x53 && i_KeyModifiers == GROOVE_KEYMOD_CONTROL)//Ctrl-S
						{
							SaveButton_OnCommand("", null)
							return true;
						}
						else if(i_Msg == 0x100 && i_KeyCode == 0x1b)//Esc
						{
							CancelButton_OnCommand("", null);
							return true;
						}

						return false;
					}

					function NoEarlierThanRadioButton_OnCommand(URL, Data)
					{
						NoEarlierThanRadioButton.CheckState = GrooveCheckState_Checked;
						ASAPRadioButton.CheckState = GrooveCheckState_Unchecked;
						StartDateEdit.Enabled = true;
						StartDateButton.Enabled = true;
					}

					function ASAPRadioButton_OnCommand(URL, Data)
					{
						var StartDate = ParseDayAndDate(StartDateEdit.Text,
														GrooveIntlDateFormatStyle_Short);

						// Validate the start date.
						if(StartDate < 0 || isNaN(StartDate) || StartDate < g_ProjectStartTime)
						{
							StartDate = g_ProjectStartTime;
						}

						// Make sure the start date is the beginning of the week.
						var WorkWeekUserStart = g_ProjectDMD.GetStartDateInWorkWeek(StartDate);
						if(WorkWeekUserStart != StartDate)
						{
							StartDate = WorkWeekUserStart;
						}
						StartDateEdit.Text = FormatDayAndDate(StartDate,
															  GrooveIntlDateFormatStyle_Short);

						ASAPRadioButton.CheckState = GrooveCheckState_Checked;
						NoEarlierThanRadioButton.CheckState = GrooveCheckState_Unchecked;
						StartDateEdit.Enabled = false;
						StartDateButton.Enabled = false;
					}

					function SameLevelRadioButton_OnCommand(URL, Data)
					{
						SameLevelRadioButton.CheckState = GrooveCheckState_Checked;
						PlaceAsSubRadioButton.CheckState = GrooveCheckState_Unchecked;
					}

					function PlaceAsSubRadioButton_OnCommand(URL, Data)
					{
						// Need to check predecessor only if Save and Create has been called
						// with predecessors assigned.
						if (g_PredecessorCache &&
							g_NewPredecessorsArray &&
							g_NewPredecessorsArray.length > 0)
						{
							var Telespace = PropertyList.OpenProperty(PROPERTY_TELESPACE);
							var Transaction = Telespace.OpenTransaction(true);
							try
							{
								if (g_ProjectDMD.DoesItemExist(g_SelectionID))
								{
									var CycleCreated = false;
									var pIPredecessorIDEnum = CreateDoubleEnum();
									for(var i = 0; i < g_NewPredecessorsArray.length; ++i)
									{
										// Handle edge condition. The parent task may not be a predecessor.
										if (g_SelectionID == g_NewPredecessorsArray[i])
										{
											CycleCreated = true;
											break;
										}

										// If the item has been deleted, ignore it.
										if (g_ProjectDMD.DoesItemExist(g_NewPredecessorsArray[i]))
											pIPredecessorIDEnum.Add(g_NewPredecessorsArray[i]);
									}

									// Check if we are creating circular dependency here
									pIPredecessorIDEnum = pIPredecessorIDEnum.DoubleEnum;
									if (CycleCreated ||
										g_ProjectDMD.DoesAddPredecessorsCreateCycle(g_SelectionID, pIPredecessorIDEnum))
									{
										// Alert the user
										Transaction.Abort();
										PlaceAsSubRadioButton.CheckState = GrooveCheckState_Unchecked;
										PHF_AlertUser(GPTPROJECT_MB_PARENT_CAUSES_CYCLE);
										return;
									}
								}

								Transaction.Commit();
							}
							catch (e)
							{
								Transaction.Abort();
								PlaceAsSubRadioButton.CheckState = GrooveCheckState_Unchecked;
								GrooveDebugFunctions.DisplayAssert(0, "Error checking for cycle");
								return;
							}
						}

						PlaceAsSubRadioButton.CheckState = GrooveCheckState_Checked;
						SameLevelRadioButton.CheckState = GrooveCheckState_Unchecked;
					}

					function StartDateButton_OnCommand(URL, Data)
					{
						GetDateFromPopup();
					}

					function OKButton_OnCommand(URL, Data)
					{
						if(ValidateInput())
						{
							g_SaveOnce = true;
							var Window = UIPropertyList.OpenProperty(UIPROPERTY_TOP_LEVEL_WINDOW);
							Window.Close(GrooveDialogBoxResultCode_OK);
						}
					}

					function SaveButton_OnCommand(URL, Data)
					{
						if(ValidateInput())
						{
							g_PredecessorCache = null;
							PredecessorsButton.Label = PHF_GetPredecessorLabel(0);

							g_SaveOnce = false;
							var Window = UIPropertyList.OpenProperty(UIPROPERTY_TOP_LEVEL_WINDOW);
							Window.Close(GrooveDialogBoxResultCode_OK);
						}
					}

					function ValidateInput()
					{
						// Check duration
						if (!ValidateDuration())
						{
							PHF_AlertUser(GPTPROJECT_MB_ENTER_VALID_DURATION_DATE);

							// Set focus and selection to the invalid item.
							MiddleView.SetFocus("DurationEdit");
							DurationEdit.SetSelection(0, DurationEdit.Text.length);

							return false;
						}

						// Check start date
						if(!ValidateStartDate(true))
						{
							// Set focus and selection to the invalid item.
							MiddleView.SetFocus("StartDateEdit");
							StartDateEdit.SetSelection(0, StartDateEdit.Text.length);

							return false;
						}

						// Must provide task name
						if("" == TrimHead(TaskNameEdit.Text))
						{
							PHF_AlertUser(GPTPROJECT_MB_NAME_REQUIRED);

							// Set focus and selection to the invalid item.
							MiddleView.SetFocus("TaskNameEdit");
							TaskNameEdit.SetSelection(0, TaskNameEdit.Text.length);

							return false;
						}

						return true;
					}

					function CancelButton_OnCommand(URL, Data)
					{
						var Window = UIPropertyList.OpenProperty(UIPROPERTY_TOP_LEVEL_WINDOW);
						Window.Close(GrooveDialogBoxResultCode_Cancel);
					}

