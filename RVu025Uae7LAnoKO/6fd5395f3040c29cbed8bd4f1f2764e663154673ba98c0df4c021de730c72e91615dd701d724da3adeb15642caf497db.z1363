				//HideMenuCommands
				function HideMenuCommands()
				{
					// Note that all commands the tool supports appear on one of the menus above
					if (typeof g_FileDocumentCC != "undefined")
					{
						// File Document Menu

						//  Export
						if (g_FileDocumentCC.UICommandExists("GUIC_RichText_Export"))
							g_FileDocumentCC.RemoveUICommand("GUIC_RichText_Export");
					}

					//Below is the Format Menu
					if (g_FormatFontCC)
					{
						g_FormatFontCC.UICommand.Label = "";
						g_FormatFontCC.UICommand.Mnemonic = "";
						// We own this menu; therefore we can call RemoveAll.
						g_FormatFontCC.UICommandContainer.RemoveAllUICommands();
					}	
			
				}

				//ExecuteMenuCommands ..hook up the appropriate functions here
				function ExecuteMenuCommand(i_UICommand)
				{
					var CommandURL= i_UICommand.CommandURL;


					if (CommandURL == "GUIC_RichText_Export")
					{	
						ExportToRTF();
					}
					//undo	
					else if (CommandURL == "GUIC_Undo")
					{
                        SubmitControlChar ("Z");
					}
					//Redo
					else if (CommandURL == "GUIC_Redo")
					{	
						SubmitControlChar ("Y");
					}
					//Cut
                    else if (CommandURL == "GUIC_Cut")
                    {
                        SubmitControlChar ("X");
                    }
					//Copy
                    else if (CommandURL == "GUIC_Copy")
                    {
                        SubmitControlChar ("C");
                    }
					//Paste
                    else if (CommandURL == "GUIC_Paste")
                    {
                        SubmitControlChar ("V");
                    }

					//Select all
                    else if (CommandURL == "GUIC_SelectAll")
                    {
                        SubmitControlChar ("A");
                    }
					//Delete
                    else if (CommandURL == "GUIC_Delete")
                    {
						TextView.SubmitKeyPress(GrooveTextKey_Delete ,false, false);
                    }
					//Mark Tool Read
                    else if (CommandURL == "GUIC_MarkAllRead")
                    {
						MarkRead ();
                    }
					//Mark Tool UnRead
                    else if (CommandURL == "GUIC_MarkAllUnread")
                    {
						MarkUnread ();
                    }
					//Print
