                      var g_NumberOfRSERecordsExported  = 0;
                      var g_NumberOfDSEFilesExported    = 0;
                        
                      function ExportButton_OnCommand(i_pIUICommand)
                      {                  
                        // If this call fails, either with an exception or because the user cancelled
                        // out of the Save File dialog, we do not want to make the next call.
                        var bResult = ExportRecordSetEngineData();
                        
                        if (!bResult)
                          return;                          
                          
                        ExportDocumentShareEngineData();
                        
                        // Show the user the results of the export.
		                    var strLastDirectory = "";
                        var strToolName = PropertyList.OpenProperty(PROPERTY_TOOL_NAME);
		                    if (PropertyList.PropertyExists(strToolName + "LastDirectory"))
			                    strLastDirectory = PropertyList.OpenProperty(strToolName + "LastDirectory");
                          
                        GrooveScriptFunctions.DoMessageBox("Team Project data successfully exported to\n\t" + strLastDirectory +
                                                           "\n\nExported:\n\t" + g_NumberOfRSERecordsExported + " records\n\t" + g_NumberOfDSEFilesExported + " files",
                                                           "Team Project 2007 Data Export");
                                                           
                        // Zero out the counters in case the user hits the Export button again.
                        g_NumberOfRSERecordsExported  = 0;
                        g_NumberOfDSEFilesExported    = 0;
                      }
                      
                      function ExportRecordSetEngineData()
                      {          
                        var bResult = false;
                      
						            // Do not open a transaction on the telespace. We are going to call
                        // ExportRecordsToFile which will cause the start of a transaction.
                        
                        //  Perform all operations inside of try/catch block.
                        try
                        {                  
                          if(RecordSetEngine.NumRecords > 0)
                          {
                            var pIRecordIDEnu␀ð ␀␀␀␀␀␀␀␀␀m = RecordSetEngine.OpenRecordIDEnum();
                            bResult = MyExportRecordsToFile(pIRecordIDEnum, RecordSetEngine, PropertyList, PropertyList.OpenProperty(PROPERTY_TOOL_NAME));
                          }
                          else
                          {                    
                            GrooveScriptFunctions.DoMessageBox("There is no data to export.", "Team Project 2007 Data Export");
                            bResult = false;
                          }
                          return bResult;
                        }
                        catch(e)
                        {                   
                          return false;
                        }                  
                      }                
                      
                      function ExportDocumentShareEngineData()
                      {                                      
                        //  Perform all operations inside of try/catch block.
                        try
                        {                       
			                    var strRootFolderPath = "";
                          var strToolName = PropertyList.OpenProperty(PROPERTY_TOOL_NAME);
                          
			                    if (PropertyList.PropertyExists(strToolName + "LastDirectory"))
				                    strRootFolderPath = PropertyList.OpenProperty(strToolName + "LastDirectory");
			                    else
			                    {
				                    var pWin32FileSystemServices = GrooveScriptFunctions.CreateNewObject("Groove.Win32FileSystemServices");
				                    strRootFolderPath = pWin32FileSystemServices.OpenPersonalFilesFolderPath();
			                    }
                          
                          var strRootShareFolderID = DocumentShareEngine.Data.OpenRootShareFolderID(); 
                          
                          // It may take a while to process all the files in the DocumentShareEngine,
                          // so see if there is work to do and display the 'Busy Window' while processing.
                          if (DocumentShareEngine.Data.OpenShareObjectEnum (strRootShareFolderID) != null)
                          {
                            var strBusyWindowID = StartBusyWindow(UIPropertyList.OpenProperty(UIPROPERTY_TOP_LEVEL_WINDOW),
                                                                  "Exporting Data",
                                                                  "Exporting Data from Team Project tool. \nThis may take a while...",
                                                                  0);
                          
                            MyExportDocumentShareFiles(strRootShareFolderID, strRootFolderPath, null);
                            
                            // Close the 'Busy Window'.
                            StopBusyWindow(strBusyWindowID)
                          }
                          return true;
                        }
                        catch(error)
                        {                  
                          GrooveScriptFunctions.DoMessageBox(error.Description, "Team Project 2007 Data Export");
                          return false;
                        }                  
                      }
                      
                      function MyExportRecordsToFile(i_RecordIDEnum, i_RecordSetEngine, i_PropertyList, i_ToolName)
		                  {
			                  try
			                  {
				                  var ExportElement = i_RecordSetEngine.ExportToElement(i_RecordIDEnum);
			                  }
			                  catch (error)
			                  {               
                          GrooveScriptFunctions.DoMessageBox(error.Description, "Team Project 2007 Data Export");
				                  return false;
			                  }

			                  // Before we serialize the element, we need to make any linked files dbrelative
			        ␀ð ␀␀␀␀␀␀␀␀␀          // this is done do to make sure that when the file is imported all linked binary
			                  // documents are db relative and not absolute
			                  try
			                  {
				                  // 1. get the document which contains the element
				                  var pXMLDocument = ExportElement.OpenXMLDocument();
				                  // 2. get a locator for this document
				                  var pLocator = pXMLDocument.CreateLocator();
				                  // 3. create a query string
				                  var strLocatorString = ".//urn:groove.net:File"
				                  // 4. get an element enum matching our query
				                  var pFileElementEnum = pLocator.OpenElementEnum(strLocatorString, ExportElement);
				                  // 5. loop through the enum and check to see if the returned element contains links
				                  //    if it does, make the links db relative
				                  while (pFileElementEnum.HasMore())
				                  {
					                  var pFileElement = pFileElementEnum.OpenNext();
					                  if (pFileElement.IsLinkElement())
						                  pFileElement.SetAttribute("MakeDBRelative", "true");
				                  }
			                  }
			                  catch (error)
			                  {               
                          GrooveScriptFunctions.DoMessageBox(error.Description, "Team Project 2007 Data Export");
				                  return false;
			                  }
                        
                        try
                        {
			                    var strLastDirectory = "";
			                    var Account = OpenAccountFromPropertyList(i_PropertyList);
			                    var App = GrooveScriptFunctions.CreateNewObject("Groove.Application");
			                    if (i_PropertyList.PropertyExists(i_ToolName + "LastDirectory"))
				                    strLastDirectory = i_PropertyList.OpenProperty(i_ToolName + "LastDirectory");
			                    else
			                    {
				                    var pWin32FileSystemServices = GrooveScriptFunctions.CreateNewObject("Groove.Win32FileSystemServices");
				                    strLastDirectory = pWin32FileSystemServices.OpenPersonalFilesFolderPath();
			                    }
				                    
                          var FileResult = App.SaveFileDialog("Export Record(s) to File..." , "XML File (*.xml)|*.xml||", strLastDirectory, i_ToolName+"."+ GLOBAL_XML_EXT , Account, 1, false);

			                    if (GrooveDialogBoxResultCode_OK == FileResult.Result)
			                    {
				                    if (FileResult.Data.lastIndexOf(".xml") == -1)
					                    FileResult.Data = FileResult.Data + ".xml";

				                    var strPath = LeftBack(FileResult.Data, "\\");
				                    i_PropertyList.SetProperty(i_ToolName +"LastDirectory", strPath, 0);
				                    var BOS = CreateByteOutputStreamFromFile(FileResult.Data);
				                    ExportElement.SerializeToStream(BOS, GrooveSerializeType_XML, GrooveCharEncodingType_UTF8, GrooveSerializeOption_Default);
                            BOS.Close();                      
                                
                            g_NumberOfRSERecordsExported = i_RecordSetEngine.NumRecords;                      
                            return true;
			                    }  
                          else
                          {
                            GrooveScriptFunctions.DoMessageBox("Action cancelled by user - No data exported.", "Team Project 2007 Data Export");
                            return false;
                          }
			                  }
			                  catch (error)
			                  {               
                          GrooveScriptFunctions.DoMessageBox(error.Description, "Team Project 2007 Data Export");
				                  return false;
			                  }
		              ␀ð ␀␀␀␀␀x␀␀    }              
                      
                      function MyExportDocumentShareFiles(i_ParentFolderID, i_RootFolderPath, i_FolderRelPath)
                      {                  
                        //  Perform all operations inside of try/catch block.                  
                        try
                        {
                          var ShrObjEnum = DocumentShareEngine.Data.OpenShareObjectEnum (i_ParentFolderID);
                          if (ShrObjEnum == null)
                              return;
                          
                          while (ShrObjEnum.HasMore())
                          {
                              var ShrObj = ShrObjEnum.OpenNext().ShareObject;
                              if (ShrObj.Type == "File")
                              {
                                  var FolderPath = i_RootFolderPath;
                                  if (i_FolderRelPath != null)
                                      FolderPath += "\\" + i_FolderRelPath;

                                  var Contents = DocumentShareEngine.Data.OpenFileContents (ShrObj.ID);
                                  
                                  var pWin32FileSystemServices = GrooveScriptFunctions.CreateNewObject("Groove.Win32FileSystemServices");
                                  var ExportPath = pWin32FileSystemServices.ExportFileToFolder (Contents, FolderPath, ShrObj.FullName, false, true);
                                  g_NumberOfDSEFilesExported += 1;
                              }
                              else if (ShrObj.Type == "Folder")
                              {
