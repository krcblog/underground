					function UpdatePredecessorCache()
					{
							MHF_ShowWaitCursor(true);

							// Get the list of all tasks (with predecessor information)
							var Telespace = PropertyList.OpenProperty(PROPERTY_TELESPACE);
							var Transaction = Telespace.OpenTransaction(true);
							try
							{
								g_PredecessorCache = PHF_GetTaskArray(g_ProjectDMD);

								Transaction.Commit();
							}
							catch(e)
							{
								MHF_ShowWaitCursor(false);
								Transaction.Abort();
								GrooveDebugFunctions.DisplayAssert(0, "Error refreshing predecessor list");
								return;
							}

							MHF_ShowWaitCursor(false);

							// Reset selection state
							g_PredecessorSelectionState = null;
					}					

					function PredecessorsButton_OnCommand()
					{
						// Create the predecessor dialog box
						var pIPredecessorWindow = PHF_CreateEditPredecessorDialog(EditPredecessorsSubForm);

						// Grab edit predecessor script dispatch
						var PredecessorDelegate = pIPredecessorWindow.OpenDelegateComponent();
						g_EditPredecessorsScriptDispatch = PredecessorDelegate.GetScriptDispatch();

						// Retrieving the predecessor array may take a long time
						// so we'll do it only once for this dialog box.
						// Only refresh the cache the first time the button is pressed.
						if(null == g_PredecessorCache)
						{
							UpdatePredecessorCache();
						}

						// Set the project data model delegate
						g_EditPredecessorsScriptDispatch.SetProjectDataModelDelegate(g_ProjectDMD);

						// Initialize the dialog data
						g_EditPredecessorsScriptDispatch.InitializeDialogData
													( -1,
													  GetName(),
													  null,
													  g_PredecessorCache );



						// Pass the Selection and Parent ID's and indicate if we are creating a sub-task.
						g_EditPredecessorsScriptDispatch.SetSelectionID(GetSelectionID());
						g_EditPredecessorsScriptDispatch.SetParentID(GetParentID());
						g_EditPredecessorsScriptDispatch.SetSubtaskFlag(IsSubLevelChecked());

						// Set selection state
						if(null != g_PredecessorSelectionState)
						{
							g_EditPredecessorsScriptDispatch.SetSelectionState(g_PredecessorSelectionState);
						}

						// Show modal dialog
						if(GrooveDialogBoxResultCode_OK == pIPredecessorWindow.DoModal())
						{
							// Remember user selection
							g_PredecessorSelectionState = g_EditPredecessorsScriptDispatch.GetSelectionState();

							g_NewPredecessorsArray = g_EditPredecessorsScriptDispatch.GetNewPredecessorsArray();

							// Update UI
							PredecessorsButton.Label = PHF_GetPredecessorLabel(g_EditPredecessorsScriptDispatch.GetSelectionCount());
						}

						// Destroy the window
						pIPredecessorWindow.Destroy();
					}

					function ValidateStartDate(i_PopupMsg)
					{
						var StartDate = ParseDayAndDate(StartDateEdit.Text,
														GrooveIntlDateFormatStyle_Short);
						if(StartDate < 0 || isNaN(StartDate))
						{
							if(i_PopupMsg)
								PHF_AlertUser(GPTPROJECT_MB_ENTER_VALID_START_DATE);

							return false;
						}

						// Verify start is after project start.
						if(StartDate < g_ProjectStartTime && GetConstrainStartFlag())
						{
							if(i_PopupMsg)
							{
								PHF_AlertUser(GPTPROJECT_MB_START_DATE_AFTER_PROJECT_DATE);
							}
							return false;
						}

						// Move start constraint to a work day if necessary.
						var WorkWeekUserStart = g_ProjectDMD.GetStartDateInWorkWeek(StartDate);
						if(WorkWeekUserStart != StartDate)
						{
							var UpdatedUserStart = FormatDayAndDate(WorkWeekUserStart,
																	GrooveIntlDateFormatStyle_Short)
							StartDateEdit.Text = UpdatedUserStart;
							
							if(i_PopupMsg)
							{
								PHF_AlertUser(GPTPROJECT_MB_START_MOVED_TO_WORKDAY + UpdatedUserStart + GPTPROJECT_MB_DOT);
								return false;
							}
						}

						return true;
					}

					function ValidateDuration()
					{
						if(isNaN(DurationEdit.Text) || DurationEdit.Text == "" ||
							(DurationEdit.Text % 1) > 0 || (DurationEdit.Text >= 10000) || 
							DurationEdit.Text < 0)
						{
							return false;
						}

						return true;
					}

