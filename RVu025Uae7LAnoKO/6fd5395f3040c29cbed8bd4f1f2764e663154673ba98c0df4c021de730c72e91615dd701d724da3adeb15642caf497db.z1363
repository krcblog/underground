					//
					// Constants
					//
					var MSPERDAY = 86400000; // Milliseconds per day = 24 * 60 * 60 * 1000;

					//
					// Globals
					// 
					var g_ProjectDMD = null;
					var g_CurrentItem = null;
					var g_PrevItem = null;
					var g_ContactNameURLMap = null;
					var g_CanEditScheduleInfo = false;
					var g_pIPredecessorWindow = null;
					var g_EditPredecessorsScriptDispatch = null;
					var g_PredecessorSelectionState = null;
					var g_PredecessorOriginalSelectionState = null;
					var g_ParentSubForm = null;
					var g_ProjectStartTime = -1;
					var g_AttachmentCount = 0;
					var	g_NewPredecessorsArray = null;
					var	g_RemovePredecessorsArray = null;

					function Initialize()
					{
						var StatusArray = MHF_GetStatusArray();
						for (var i = 0; i < StatusArray.length; ++i)
						{
							StatusValueEdit.AddItem(StatusArray[i]);
						}

						var PriorityArray = MHF_GetPriorityArray();
						for (var i = 0; i < PriorityArray.length; ++i)
						{
							PriorityValueEdit.AddItem(PriorityArray[i]);
						}
					}

					function Terminate()
					{
					}

					function OnRevokeUIProperties()
					{
						// Destroy the predecessor window
						DestroyPredecessorWindow();
					}

					function SetContactList(i_ContactNameURLMap)
					{
						g_ContactNameURLMap = i_ContactNameURLMap;
					}

					function SetProjectDataModelDelegate(i_ProjectDMD)
					{
						g_ProjectDMD = i_ProjectDMD;
					}

					function RefreshContactList()
					{
						// Reset UI
						AssignedToCB.ResetContent();

						if (g_ContactNameURLMap)
						{
							// Populate cache and combo box
							for(var i = 0; i < g_ContactNameURLMap.GetCount(); ++i)
							{
								var CurrentContactNameURLTuple = g_ContactNameURLMap.GetAt(i);
								var FullName = CurrentContactNameURLTuple.Name;

								AssignedToCB.AddItem(FullName);
							}

							// Add in "unassigned"
							AssignedToCB.InsertItem(0, GPT_S_UNASSIGNED);
							g_ContactNameURLMap.Add(GPT_S_UNASSIGNED, "");
						}
					}

					function SetParentSubForm(i_ParentSubForm)
					{
						g_ParentSubForm = i_ParentSubForm;
					}

					function GetDefaultContactURLForItem()
					{
						if(g_ContactNameURLMap.URLExists(CHF_GetUserURL()))
						{
							return CHF_GetUserURL();
						}
						else
						{
							return "";
						}
					}

					function GetCurrentItem()
					{
						return g_CurrentItem;
					}

					function DestroyPredecessorWindow()
					{
						if (null != g_pIPredecessorWindow)
						{
							g_pIPredecessorWindow.Destroy();
							g_pIPredecessorWindow = null;
							g_EditPredecessorsScriptDispatch = null;
						}
					}

					function EditTask(i_Task, i_CanEditScheduleInfo, i_ProjectStartTime)
					{
						// Reset predecessor arrays for different task.
						if (g_CurrentItem != i_Task)
						{
							g_NewPredecessorsArray = null;
							g_RemovePredecessorsArray = null;
						}

						// Remember original item info
						g_CurrentItem = i_Task;
						g_CanEditScheduleInfo = i_CanEditScheduleInfo;
						g_ProjectStartTime = i_ProjectStartTime;

						// This is only used when creating a new task
						g_PrevItem = null;	

						// Reset the predecessor window so that it gets repopulated for the current item
						DestroyPredecessorWindow();

						// Name
						ShowName();

						// Owner
						ShowOwner();

						// Only allow scheduled info to be modified if the user has
						// sufficient privs AND the item is not a summary task
						if(i_CanEditScheduleInfo && false == i_Task.IsSummary)
						{
							ShowScheduleInfoEditControls();
						}
						else
						{
							ShowScheduleInfoStaticControls();
						}

						// Site the appropriate controls based on the state of this item
						if(i_Task.IsSummary)
						{
							ShowSummaryTaskControls();
						}
						else
						{
							ShowTaskControls();
						}


						// Item label
						ItemNameLabel.SetText(GPTPROJECT_S_TASKNAME);
						
						// Owner label
						var OwnerIndex;
						OwnerLabel.SetText(GPTPROJECT_S_OWNER);
						
						// Days label
						DaysLabel.SetText(GPT_PROJECT_S_DAY_S_IN_PAREN);

						// Constrain start
						ASAPRadioButton.Label = GPTPROJECT_S_STARTASAP;
						ASAPRadioButton.CheckState = g_CurrentItem.ConstrainStartDate ? 
																GrooveCheckState_Unchecked :
																GrooveCheckState_Checked;
						NoEarlierThanRadioButton.Label = GPTPROJECT_S_CONSTRAINSTARTDATE;
						NoEarlierThanRadioButton.CheckState = g_CurrentItem.ConstrainStartDate ? 
																GrooveCheckState_Checked :
																GrooveCheckState_Unchecked;
						if(NoEarlierThanRadioButton.CheckState == GrooveCheckState_Checked)
						{
							UserStartDateValueEdit.Enabled = true;
							UserStartPopupButton.Enabled = true;
						}
						else
						{
							UserStartDateValueEdit.Enabled = false;
							UserStartPopupButton.Enabled = false;
						}

						// Scheduled finish
						RefreshFinishDate();

						// Set labels
						DescriptionLabel.SetText(GPTPROJECT_S_DESCRIPTION);

						// Attachments
						g_AttachmentCount = g_ParentSubForm.RefreshAttachmentsMenu
															(g_CurrentItem.ID,
														     AttachmentsMenu.UICommandContainer,
														     AttachmentButton,
														     true); // Edit mode

						// Determine how many predecessors the item has
						var pIPredecessors = g_CurrentItem.OpenPredecessorEnum();
						var i = 0;
						while(pIPredecessors.HasMore())
						{
							pIPredecessors.OpenNextPair();
							++i;
						}
						PredecessorsButton.Label = PHF_GetPredecessorLabel(i)


						if(0 != g_CurrentItem.Description.GetContentCount())
						{
							DescriptionView.ComplexCellContent.ReadContentFromElement(g_CurrentItem.Description);
						}
						else
						{
							DescriptionView.RTFContent = "";
						}

						// Set focus to item name
						ProjectDetailsHeaderViewContainer.SetFocus("ItemNameEdit");
					}

					function ShowSummaryTaskControls()
					{
