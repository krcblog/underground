							Flags |= GrooveTextParaFlag_NumberBullet;
						else if ((ListType & 0xFFFF) != tomListBullet)
							Flags |= GrooveTextParaFlag_NumberBullet;
						else
							Flags |= GrooveTextParaFlag_NumberNone;

						SubmitParaChange (Flags, tomUndefined, TextView);
                    }
