				var m_ItemCount, m_Initialized = false;

				// time UI constants
				var AM = " AM";
				var PM = " PM";
				var COLON = ":";

				function OnSetUIProperties()
				{
					// clear our the list
					CalendarList.DeleteAllItems();
					m_ItemCount = 0;

					// enable event capturing
					CalendarList.Enabled = true;

					// only insert column headers once per "session"
					if (!m_Initialized)
					{
						CalendarList.InsertColumn(0,"Start", 0);
						CalendarList.InsertColumn(1,"End", 0);
						CalendarList.InsertColumn(2,"Event", 0);
						CalendarList.SetColumnWidth(0,150);
						CalendarList.SetColumnWidth(1,150);
						CalendarList.SetColumnWidth(2,300);
						m_Initialized = true;
					}
				}

				// called by "includer" to fill the list
				function FillList(i_RecordEnum)
				{
					// clear our the list
					CalendarList.DeleteAllItems();
					m_ItemCount = 0;

					var Record, EventTitle, StartDate, StartText, EndDate, EndText;
					while (i_RecordEnum.HasMore())
					{
						Record = i_RecordEnum.OpenNext();

						// Start Date
						StartDate = new Date(Record.OpenField("Start"));
						StartText = FormatDate(StartDate, GROOVE_DATE_FORMAT_SHORT)
						StartText += " " + TimeStrFromDateObj(StartDate);
						CalendarList.InsertItem(m_ItemCount, StartText, 0);

						// End Date
						EndDate = new Date(Record.OpenField("End"));
						EndText = FormatDate(EndDate, GROOVE_DATE_FORMAT_SHORT)
						EndText += " " + TimeStrFromDateObj(EndDate);
						CalendarList.SetItemText(m_ItemCount, 1, EndText);

						// Subject
						EventTitle = Record.OpenField("Subject");
						CalendarList.SetItemText(m_ItemCount, 2, EventTitle);
						m_ItemCount++;
					}
				}

				// Also used by CalendarAddEntryDialog -- should be moved to GrooveCalendarHelpersGlue.gsl
				function TimeStrFromDateObj(i_DateObj)
				{
					if (i_DateObj.getHours() < 12)
						var AMPMStr = AM;
