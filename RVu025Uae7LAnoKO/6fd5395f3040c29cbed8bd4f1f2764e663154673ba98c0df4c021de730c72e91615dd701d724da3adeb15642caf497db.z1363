					function RefreshPredecessorList()
					{
						var NumPredecessors = 0;
						var pIPredecessorEnum = g_Item.OpenPredecessorEnum();

						// Empty menu
						PredecessorsMenu.RemoveAllUICommands();
						while(pIPredecessorEnum.HasMore())
						{
							var pIPredecessorPair = pIPredecessorEnum.OpenNextPair();
							var pIUICommand = PredecessorsMenu.InsertUICommand("PROJECT_VIEWPREDECESSORURL", PredecessorsMenu.GetNumberOfUICommands());

							// Set name; handle ampersands.
							var Name = pIPredecessorPair.First.replace(/\&/g, "\&\&");
							pIUICommand.Label = Name;

							// Set ID
							pIUICommand.UserData = pIPredecessorPair.Second;	// ID
							++NumPredecessors;
						}

						PredecessorsButton.Label = PHF_GetPredecessorLabel(NumPredecessors, false);
						return NumPredecessors;
					}

					function PredecessorsMenu_OnCommand(i_UICommand)
					{
						if("PROJECT_VIEWPREDECESSORURL" == i_UICommand.CommandURL)
						{
