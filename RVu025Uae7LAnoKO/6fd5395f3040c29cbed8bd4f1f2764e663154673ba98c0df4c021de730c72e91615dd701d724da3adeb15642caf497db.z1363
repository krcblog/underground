					//
					// Constants
					//
					var g_MSPERDAY = 86400000;			// 24 * 60 * 60 * 1000;

					//
					// Globals
					//
					var g_PeopleProvider = null;
					var g_ProjectDMD = null;
					var g_ProjectID = -1;
					var g_ParentSubForm = null;
					var g_AttachmentCount = 0;

					function Initialize()
					{
					}

					function SetPeopleProvider(i_PeopleProvider)
					{
						g_PeopleProvider = i_PeopleProvider;
					}

					function SetProjectDataModelDelegate(i_ProjectDMD)
					{
						g_ProjectDMD = i_ProjectDMD;
					}

					function SetParentSubForm(i_ParentSubForm)
					{
						g_ParentSubForm = i_ParentSubForm;
					}

					function UpdateButtonsState(i_CanDelete, i_CanEdit)
					{
						if(g_ParentSubForm)
						{
							EditButton.Enabled = i_CanEdit;
						}
					}

					function ShowProject(i_Project)
					{
						// Remember project
						g_ProjectID = i_Project.ID;

						var ScheduledStart = new Date(i_Project.ScheduledStart);

						// Item name
						var Description = i_Project.Name + " " + GPT_S_PROJECTNAME_TAG;
						ProjectName.SetText(Description);

						// Status
						StatusLabel.SetText(GPTPROJECT_S_STATUS);
						StatusValue.SetText(MHF_GetStatusStringFromEnum(i_Project.Status));
						
						// Team leader (default to missing contact)
						TeamLeaderLabel.SetText(GPTPROJECT_S_LEADER);
						TeamLeaderValue.SetText(PHF_GetPersonNameForDisplay(g_PeopleProvider, i_Project.ProjectLeaderContactURL));
						
						// Scheduled start
						ScheduledStartLabel.SetText(GPTPROJECT_S_SCHEDULED_START);
						ScheduledStartDateValue.SetText(FormatDayAndDate(i_Project.ScheduledStart,
																		 GrooveIntlDateFormatStyle_Short));

						// Scheduled duration
						ScheduledDurationLabel.SetText(GPTPROJECT_S_SCHEDULED␀ð ␀␀␀␀␀°␀␀␀_DURATION);
						var nDays = i_Project.ScheduledDuration / g_MSPERDAY;
						var DurationText = nDays + " " + ((nDays == 1) ? GPT_PROJECT_S_DAY : GPT_PROJECT_S_DAYS);
						ScheduledDurationValue.SetText(DurationText);

						// Display finished date
						ScheduledFinishLabel.SetText(GPTPROJECT_S_SCHEDULED_FINISH);
						ScheduledFinishDateValue.SetText(FormatDayAndDate(i_Project.DisplayFinish,
																		  GrooveIntlDateFormatStyle_Short));
						
						// Actual start
						if(i_Project.Status == GrooveProjectToolsetStatusType_InProgress ||
						   i_Project.Status == GrooveProjectToolsetStatusType_Complete)
						{
							ActualStartLabel.Visible = true;
							ActualStartLabel.SetText(GPTPROJECT_S_ACTUALSTART);
							ActualStartDateValue.Visible = true;
							ActualStartDateValue.SetText(FormatDayAndDate(i_Project.ActualStart,
																		  GrooveIntlDateFormatStyle_Short));
						}
						else
						{
							ActualStartLabel.Visible = false;
							ActualStartDateValue.Visible = false;
						}

						// Actual finish
						if(i_Project.Status == GrooveProjectToolsetStatusType_Complete)
						{
							ActualFinishLabel.Visible = true;
							ActualFinishLabel.SetText(GPTPROJECT_S_ACTUALFINISH);
							ActualFinishDateValue.Visible = true;
							ActualFinishDateValue.SetText(FormatDayAndDate(i_Project.ActualFinish,
																		   GrooveIntlDateFormatStyle_Short));
						}
						else
						{
							ActualFinishLabel.Visible = false;
							ActualFinishDateValue.Visible = false;
						}
						
						// Attachments
						g_AttachmentCount = g_ParentSubForm.RefreshAttachmentsMenu
													(g_ProjectID,
													 AttachmentsMenu.UICommandContainer,
													 AttachmentButton,
													 false); // View mode

						// Description
						ObjectiveLabel.SetText(GPTPROJECT_S_OBJECTIVE);

						if(0 != i_Project.Objective.GetContentCount())
						{
							ObjectiveView.ComplexCellContent.ReadContentFromElement(i_Project.Objective);
						}
						else
						{
							ObjectiveView.RTFContent="";
						}
					}

					function CopyToClipboard()
					{
						SendClipboardCommand(GROOVE_VK_C);
					}

					function CanCopyToClipboard()
					{
						return ObjectiveView.CanCopy;
					}

					function DoesDescriptionHaveFocus()
					{
						if (UIPropertyList.PropertyExists(UI_PROPERTY_UIACTIVE_COMPONENT))
						{
							var pIFocusComponent = UIPropertyList.OpenProperty(UI_PROPERTY_UIACTIVE_COMPONENT);
							if("ObjectiveView" == pIFocusComponent.Component.OpenName())
								return true;
						}

						return false;
					}

					function SendClipboardCommand(i_CommandChar)
					{
						if(DoesDescriptionHaveFocus())
						{
							var ControlCmd = String.fromCharCode (i_CommandChar & 0x1F)
							ObjectiveView.SubmitText(ControlCmd);
						}
					}

					// EditButton's CommandURL is set in ProjectView.gsl
					function EditButton_OnCommand(i_pICommand)
					{
						g_ParentSubForm.OnCommand(i_pICommand.UICommand)
					}

					function MarkCompleteButton_OnCommand(i_pICommand)
					{
						g_ParentSubForm.MarkCurrentComplete();
					}

					function AttachmentsMenu_OnCommand(i_pICommand)
					{
						var bAttachmentsChanged = g_ParentSubForm.ProcessAttachmentsMenuCommand(i_pICommand, g_ProjectID);
						if (bAttachmentsChanged)
						{
							g_AttachmentCount = g_ParentSubForm.RefreshAttachmentsMenu
														(g_ProjectID,
														 AttachmentsMenu.UICommandContainer,
														 AttachmentButton,
														 false); // View mode
						}
					}


					function AttachmentButton_OnUpdateCommandState(i_pICommand)
					{
						i_pICommand.Enabled = g_AttachmentCount > 0;
