	function PopulateReportTypeCombobox(i_FilePath)
	{
		try
		{
			var FSO = g_FSS.OpenFileSystemObject(i_FilePath);
			var FolderFSOEnum = FSO.Win32Folder.OpenContentFileSystemObjectsEnum();

			while(FolderFSOEnum.HasMore())
			{
				var PerentFSO = FolderFSOEnum.OpenNext();
				var ChildFolderName = PerentFSO.FullName;
				if(IsFile(ChildFolderName))
					continue;

				// Add to combobox
				ReportTypeComboBox.AddItem(ChildFolderName);

				// Process subfolder
				PopulateReportTypeCombobox(i_FilePath + "\\" + ChildFolderName);
			}
		}
		catch(e)
		{
			var Window = UIPropertyList.OpenProperty(UIPROPERTY_TOP_LEVEL_WINDOW);
			var Account = OpenAccountFromPropertyList(PropertyList);
			g_App.GrooveMessageBox(Window, Account, e.message);
		}
	}

	function DeleteXMLFile()
	{
		try
		{
			if(g_bCleanUp && g_NavigateToURL != "" && g_FileServices.DoesFileExist(g_NavigateToURL))
				g_FileServices.DeleteFile(g_NavigateToURL);
		}
		catch(error)
		{
		}
	}

	function GetSaveToXMLFilePathFromUser()
	{
		var FilePath = "";
		var FileFilter = "*.xml|*.xml||";
		var strLastDirectory = g_FileServices.OpenPersonalFilesFolderPath();

		if(PropertyList.PropertyExists("LastDirectory"))
			strLastDirectory = PropertyList.OpenPropertyAsString("LastDirectory");

		var pAccount = OpenAccountFromPropertyList(PropertyList);
		var pFileResult = g_App.SaveFileDialog
									(
										"Save As", 
										FileFilter, 
										strLastDirectory, 
										"", 
										pAccount, 
										1, 
										true
									);

		if(GrooveDialogBoxResultCode_OK != pFileResult.Result)
			return FilePath;

		var FilePath = pFileResult.Data;
		if(FilePath.search(".xml") < 0)
			FilePath += ".xml";

		PropertyList.SetPropertyAsString
					(
						"LastDirectory",
						LeftBack(FilePath, "\\"),
						GroovePropertyFlag_Private
					);

		return FilePath;
	}

	function SaveElement2File(i_pRootElem, i_FilePath)
	{
		var pByteOutputStream = GROOVE_GLOBAL_MISC_POINTER.StreamFactory.CreateByteOutputStreamFromFile
												(
													i_FilePath,
													false,
													false
												);
		var pCharOutputStream = pByteOutputStream.OpenCharOutputStream(GrooveCharEncodingType_UTF8);
		pCharOutputStream.WriteString("<?xml version='1.0'?><?groove.net version='1.0'?>"  + "\r\n");
		pCharOutputStream.WriteString("<?xml-stylesheet type=\"text/xsl\" href=\"" + g_XSLFileName + "\"?>"  + "\r\n");
		pCharOutputStream.Flush();
		pCharOutputStream.Close();

		pByteOutputStream = GROOVE_GLOBAL_MISC_POINTER.StreamFactory.CreateByteOutputStreamFromFile
												(
													i_FilePath,
													true,
													false
												);
		i_pRootElem.SerializeToStream
						(
							pByteOutputStream,
							GrooveSerializeType_XML,
							GrooveCharEncodingType_UTF8,
							GrooveSerializeOption_ConvertLFToCRLF |
							GrooveSerializeOption_WithFormatting |
							GrooveSerializeOption_SortedAttributes |
							GrooveSerializeOption_NoProlog
						);
		pByteOutputStream.Close();
	}

	function CopyFolderContent(i_FromFolder, i_ToFolder)
	{
		var FS = GrooveScriptFunctions.CreateNewObject("Groove.Win32FileSystemServices");
		FS.CopyFolder(i_FromFolder + "\\", i_ToFolder + "\\", false);
	}

	function IsFile(i_FSOName)
	{
		if(i_FSOName.indexOf(".") != -1)
			return true;
		return false;
	}


