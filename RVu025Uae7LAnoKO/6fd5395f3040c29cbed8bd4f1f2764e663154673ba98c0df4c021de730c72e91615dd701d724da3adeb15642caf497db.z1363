                    else if (CommandURL == "GUIC_RichText_IncreaseIndent")
                    {
						SubmitParaChange (GrooveTextParaFlag_IndentRelative, 18.0, TextView);
                    }
					//DecreaseIndent
                    else if (CommandURL == "GUIC_RichText_DecreaseIndent")
                    {
						SubmitParaChange (GrooveTextParaFlag_IndentRelative, -18.0, TextView);
                    }
				}
				
