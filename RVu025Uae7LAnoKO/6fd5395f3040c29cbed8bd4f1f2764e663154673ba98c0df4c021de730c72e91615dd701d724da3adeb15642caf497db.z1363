					function GetItemIDsForParticipantAndStatus(i_ParticipantURL, i_Status)
					{
						var pIIDEnum = CreateDoubleEnum();
						try
						{
							var pICollection = Collection.OpenCollection();
							var Query;
							var pIResultCollection;
							if(i_ParticipantURL == GPT_S_STATUS_ALL &&
								i_Status == GrooveProjectToolsetStatusFilterType_All)
							{
								pIResultCollection = pICollection;
							}
							else if(i_ParticipantURL != GPT_S_STATUS_ALL &&
								i_Status == GrooveProjectToolsetStatusFilterType_All)
							{
								Query = "/[@AssignedToContactURL='" + i_ParticipantURL + "'] or /[@TeamLeaderContactURL='" + i_ParticipantURL + "']";
								pIResultCollection = pICollection.Find(Query);
							}
							else if(i_ParticipantURL == GPT_S_STATUS_ALL &&
								i_Status != GrooveProjectToolsetStatusFilterType_All)
							{
								Query = "/[@Status='" + i_Status + "']";
								pIResultCollection = pICollection.Find(Query);
							}
							else if(i_ParticipantURL != GPT_S_STATUS_ALL &&
								i_Status != GrooveProjectToolsetStatusFilterType_All)
							{
								Query = "/[@Status='" + i_Status + "'] and (/[@AssignedToContactURL='" + i_ParticipantURL + "'] or /[@TeamLeaderContactURL='" + i_ParticipantURL + "'])";
								pIResultCollection = pICollection.Find(Query);
							}
							else
							{
								GrooveDebugFunctions.DisplayAssert(0, "Invalid Status or ParticipantURL in GetItemIDsForParticipantAndStatus");
								return;
							}

							if(0 != pIResultCollection.GetRecordCount())
							{
								var pICursor = pIResultCollection.GetCursor();

								for(var i = 0; i < pIResultCollection.GetRecordCount(); ++i)
								{
									var pICollectionRecord = pICursor.OpenRecord();
									if(pICollectionRecord != null)
										pIIDEnum.Add(pICollectionRecord.RecordID);

									// Increment cursor
									pICursor.MoveAdv(GrooveCollectionCursorPosition_Current, GrooveCollectionNavigationOp_NextAny, 1, GrooveCollectionNavigationOption_IncludeCollapsedRows);
								}
							}
						}
						catch(e)
						{
							PDHF_SetError(PROJECTTRACKING_E_OPERATION_FAILED, "Error retrieving task IDs");
						}

						return pIIDEnum.DoubleEnum;
					}

