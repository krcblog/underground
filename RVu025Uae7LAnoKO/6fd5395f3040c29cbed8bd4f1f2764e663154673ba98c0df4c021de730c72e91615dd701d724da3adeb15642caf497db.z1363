					//
					// caller is responsible for locking the database for reading
					//
					function GetPendingItemIDEnum()
					{
						try
						{
							var ItemEnum = CreateDoubleEnum();

							var RecordIDEnum = RecordSetEngine.OpenRecordIDEnum();
							while(RecordIDEnum.HasMore())
							{
								var RecordID = RecordIDEnum.OpenNext();
								var Record = RecordSetEngine.OpenRecord(RecordID);
								if("urn:groove.net:ProjectTaskRecord" == Record.Type &&
									false == PDHF_HasChildren(RecordID))
								{
									ItemEnum.Add(RecordID);
								}
							}

							return ItemEnum.DoubleEnum();
						}
						catch(e)
						{
							PDHF_SetError(PROJECTTRACKING_E_OPERATION_FAILED, "Error retrieving pending tasks");
						}
					}


					//
					// caller is responsible for locking the database for reading
					//
					function GetPendingItemIDEnumForContact(i_PersonContactURL)
					{
						try
						{
							var pIIDEnum = CreateDoubleEnum();
							var Query = PDHF_XPathConstructSimpleQuery("AssignedToContactURL", i_PersonContactURL);
							var pICollection = Collection.OpenCollection();

							var PendingItemRecordCollection = pICollection.Find(Query);
							PendingItemRecordCollection.CollectionConfigure.UseSort("ContactURLIndex", false);

							// don't try to move cursor on an empty collection (it crashes)
							if(0 != PendingItemRecordCollection.GetRecordCount())
							{
								// build record ID enum
								PendingItemRecordCollection.MoveCursor(GrooveCollectionCursorPosition_First, GrooveCollectionNavigationOp_NextAny, 0);
								var Cursor = PendingItemRecordCollection.GetCursor();
								var i;

								for(i = 0; i < PendingItemRecordCollection.GetRecordCount(); ++i)
								{
									var pICollectionRecord = Cursor.OpenRecord();

									if(null == pICollectionRecord)
									{
										GrooveDebugFunctions(0, "Missing pending items");
										break;
									}
									
									var pIRecord = RecordSetEngine.OpenRecord(pICollectionRecord.RecordID);

									if("urn:groove.net:ProjectTaskRecord" == pIRecord.Type &&
									   false == PDHF_HasChildren(pIRecord.RecordID))
									{
										pIIDEnum.Add(pIRecord.RecordID);
									}

									// increment cursor
									Cursor.Move(GrooveCollectionCursorPosition_Current, GrooveCollectionNavigationOp_NextAny, 1);
								}
							}
							return pIIDEnum.DoubleEnum;
						}
						catch(e)
						{
							PDHF_SetError(PROJECTTRACKING_E_OPERATION_FAILED, "Error finding pending tasks");
						}
					}



