		if (dir)
		{
			if (_nextObj != undefined)
				x = _nextObj;
			else
				x = _firstObj;
		}
		else
		{
			if (_prevObj != undefined)
				x = _prevObj;
			else
				x = _lastObj;
		}
		if (x.tabIndex != o.tabIndex)
		{
			_foundList = new Object();
			_foundList.tabIndex = x.tabIndex;
			_foundList[x._name] = x;
		}
		else
		{
			if (_foundList == undefined)
			{
				_foundList = new Object();
				_foundList.tabIndex = x.tabIndex;
			}
			_foundList[x._name] = x;
		}

