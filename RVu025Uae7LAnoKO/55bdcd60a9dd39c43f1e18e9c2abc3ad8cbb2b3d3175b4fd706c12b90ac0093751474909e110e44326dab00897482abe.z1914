			var header_mc:SimpleButton = this[kHeaderNameBase + i];
			var content_mc:MovieClip = getChildAt(i);
			var mask_mc:MovieClip = this[kMaskNameBase + i];

			header_mc._y = y;
			y += header_mc.height;

			if (i == oldSelectedIndex)
			{
