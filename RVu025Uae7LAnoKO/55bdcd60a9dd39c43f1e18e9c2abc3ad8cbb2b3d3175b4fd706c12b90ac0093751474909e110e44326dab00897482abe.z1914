*/
	function init(Void):Void
	{
		super.init();
	}

/**
* @private
* map keys to scroll events
*/
	function isScrollBarKey(k:Number):Boolean
	{
		if (k == Key.UP)
		{
			scrollIt("Line",-1);
			return true;
		}
		else if (k == Key.DOWN)
		{
			scrollIt("Line",1);
			return true;
		}
		else if (k == Key.PGUP)
		{
			scrollIt("Page",-1);
			return true;
		}
		else if (k == Key.PGDN)
		{
			scrollIt("Page",1);
			return true;
		}

		return super.isScrollBarKey(k);

	}

	[Bindable]
	[ChangeEvent("scroll")]
	var	_inherited_scrollPosition:Number;
