	{
		var indices = getSelectedIndices();
		var tmpArray= new Array();
		for (var i=0; i<indices.length; i++) {
			tmpArray.push(getItemAt(indices[i]));
		}
		return (tmpArray.length>0) ? tmpArray : undefined;
	}

