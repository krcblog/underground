		// 7 8 9
		var sp:Object = this._stagePoints = {};
		sp[1] = {x:0, y:0};
		sp[2] = {x:0, y:0};
		sp[3] = {x:Stage.width, y:0};
		sp[4] = {x:0, y:0};
