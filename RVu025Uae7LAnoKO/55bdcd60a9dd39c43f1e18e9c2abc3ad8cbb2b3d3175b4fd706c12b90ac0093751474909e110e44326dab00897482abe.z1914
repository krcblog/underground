	{
		var p = propName.charAt(0).toUpperCase() + propName.substr(1);
		var s = null;
		var g = function(Void)
		{
			return this["get" + p]();
		}
		if (setter) {
			s = function(val)
			{
				this["set" + p](val);
			}
		}
		obj.addProperty(propName, g, s);

	}




