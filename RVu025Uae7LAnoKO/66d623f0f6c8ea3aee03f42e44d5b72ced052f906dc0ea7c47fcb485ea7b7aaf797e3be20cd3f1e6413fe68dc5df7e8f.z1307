			</xsl:for-each>
			
			<xsl:call-template name="templ_str_ConductorShortUnCap"/>
			
			<xsl:call-template name="templ_prop_Dot"/>
			<xsl:call-template name="templ_prop_Space"/>
			
		</xsl:if>
