        <xsl:if test ="(string-length($CollectionName)>0 or string-length($FirstElement)>0) and string-length($SecondElement)>0 ">
          
          
          
          <xsl:value-of select="$constGost_SecondaryResponsibilitiesSeparator"/>
        </xsl:if>
