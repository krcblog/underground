										<xsl:if test="string-length(b:Medium) > 0">
											<xsl:value-of select="$constGost_SourceTypeOpen"/>
											<xsl:value-of select="b:Medium"/>
											<xsl:value-of select="$constGost_SourceTypeClose"/>
										</xsl:if>
									</xsl:when>
									<xsl:otherwise>
