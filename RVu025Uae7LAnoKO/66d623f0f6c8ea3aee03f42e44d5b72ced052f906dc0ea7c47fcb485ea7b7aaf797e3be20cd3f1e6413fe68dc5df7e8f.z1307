	  		<xsl:if test="string-length($tempFirst)>0 and string-length($tempSecond)>0">
				<xsl:call-template name="templ_prop_EnumSeparator"/>
			</xsl:if>

			<xsl:if test="string-length($tempSecond)>0">
				<xsl:value-of select="$tempSecond"/>
