		  <xsl:if test ='string-length($SecondElement)>0'>
			  <xsl:value-of select = "$SecondElement"/>
		  </xsl:if>

		  
		  <xsl:if test ="($FirstElement ='' and $NonEmptyPrimSectionsCount >1) or ($NonEmptyPrimSectionsCount > 2)">
			  
			  
			  
			  
			  
