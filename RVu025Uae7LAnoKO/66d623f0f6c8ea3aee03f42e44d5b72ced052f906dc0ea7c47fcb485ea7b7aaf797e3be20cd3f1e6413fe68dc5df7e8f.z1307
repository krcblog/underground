	</xsl:template>


	<xsl:template name="templateA">
		<xsl:param name="first"/>
		<xsl:param name="second"/>
		<xsl:param name="third"/>
		
		<xsl:variable name="tempFirst">
			<xsl:call-template name="handleSpaces">
