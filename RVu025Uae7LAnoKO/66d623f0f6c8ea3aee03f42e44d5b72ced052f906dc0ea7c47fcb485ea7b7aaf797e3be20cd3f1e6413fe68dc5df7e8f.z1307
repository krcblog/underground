		  <xsl:if test ='string-length($FirstElement)>0'>
			  <xsl:value-of select = "$FirstElement"/>
		  </xsl:if>

		  
		  <xsl:if test ="not ($FirstElement ='') and $NonEmptyPrimSectionsCount > 1">
			  
			  
			  
			  
			  
			  <xsl:value-of select="$constGost_PrimaryResponsibilitiesSeparator"/>
