'-mno-sse4.1'
'-msse4.2'
'-mno-sse4.2'
'-msse4'
'-mno-sse4'
'-mavx'
'-mno-avx'
'-mavx2'
'-mno-avx2'
'-mavx512f'
'-mno-avx512f'
'-mavx512pf'
'-mno-avx512pf'
'-mavx512er'
'-mno-avx512er'
'-mavx512cd'
'-mno-avx512cd'
