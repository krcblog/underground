	DWORD	dwBumpDvBitMask;
    } DUMMYUNIONNAME3;
    __GNU_EXTENSION union {
	DWORD   dwBBitMask;             /* 18: mask for blue bits*/
	DWORD   dwVBitMask;             /* 18: mask for V bits*/
	DWORD	dwStencilBitMask;
	DWORD	dwBumpLuminanceBitMask;
    } DUMMYUNIONNAME4;
    __GNU_EXTENSION union {
