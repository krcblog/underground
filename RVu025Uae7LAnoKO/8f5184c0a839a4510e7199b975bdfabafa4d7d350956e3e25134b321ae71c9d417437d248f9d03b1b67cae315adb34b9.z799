  /* max-width: Set in new_tab.js. See measureNavDots() */
  margin-inline-end: 10px;
  outline: none;
  padding-inline-start: 2px;
  text-align: left;
  transition: margin-inline-end 250ms, max-width 250ms, opacity 250ms;
