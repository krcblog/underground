<!-- Common styles for signin-related tab modal dialogs. -->
<dom-module id="signin-dialog-shared">
  <template>
    <style>
      a {
        color: var(--cr-link-color);
        text-decoration: none;
      }

      body {
        margin: 0;
        padding: 0;
      }

      .container {
        color: var(--cr-primary-text-color);
        width: 448px;
      }

      .top-title-bar {
        align-items: center;
        border-bottom: var(--cr-separator-line);
        display: flex;
        font-size: 16px;
        height: 52px;
        padding: 0 24px;
      }

