							function OKButton_OnCommand(i_UICommand)
							{
								if (g_IsSearch)
								{
									var Window = UIPropertyList.OpenProperty(UIPROPERTY_TOP_LEVEL_WINDOW);
									Window.Close(GrooveWindowResult_OK);
									Window.Destroy(GrooveWindowResult_OK);
								}
								else								
									g_CallbackCode.UpdateViewFilter(DisplayQuery.GetText());
							}
							
							function CancelButton_OnCommand(i_UICommand)
							{
								var Window = UIPropertyList.OpenProperty(UIPROPERTY_TOP_LEVEL_WINDOW);
								Window.Close(GrooveWindowResult_Cancel);
								Window.Destroy(GrooveWindowResult_Cancel);
							}

							function AdvancedButton_OnCommand(i_UICommand)
							{
								PostEvent(729, 0);
							}
							
							function AdvancedButtonCode()
							{
								if (AdvancedButton.Label == "More Options" && ViewFilterConditionsViewContainer.LayoutByName != "ViewFilterConditionsAdvancedLayout")
								{
									ViewFilterConditionsViewContainer.LayoutByName = "ViewFilterConditionsAdvancedLayout";
									AdvancedButton.Label = "Less Options";
								}
								else
								{
									ViewFilterConditionsViewContainer.LayoutByName = "ViewFilterConditionsBasicLayout";
									AdvancedButton.Label = "More Options";
								}
							}
							
							function VerifyParens()
							{
								var Query = DisplayQuery.GetText();
								var OpenParenArray = Query.match(/\(/g);
								var CloseParenArray = Query.match(/\)/g);
								var Message = "Syntax Error: There are more ";
								var OpenNum = 0;
								var CloseNum = 0;
								if (OpenParenArray != null)
									OpenNum = OpenParenArray.length;
								
								if (CloseParenArray != null)
									CloseNum = CloseParenArray.length;
								
								if(OpenNum > CloseNum)
									Message += "'(' than ')'. ";
								else if (OpenNum < CloseNum)
									Message += "')' than '('. ";
								else if (OpenNum == CloseNum)
									return;
										
								Message += " Please correct before saving.";
								
								g_UIDelegate.DisplayError(Message);																	
							}

