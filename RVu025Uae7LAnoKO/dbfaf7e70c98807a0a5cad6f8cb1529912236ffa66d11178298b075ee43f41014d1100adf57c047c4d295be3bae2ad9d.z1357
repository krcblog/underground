						function SetInitialValues(i_SessionFields, i_ViewNames)
						{
							// Reset both list views for new data.
							FieldsListView.ResetContent();
							ViewsListView.ResetContent();
							PreviewListView.ResetContent();

							// Reset global field result array.
							g_FieldResultArray = new Array();

							// Insert new field data into fields list view.
							var i = 0;
							var iCount = i_SessionFields.length;
							while (i < iCount)
							{
								var pIField = g_CallbackCode.g_pIFormsToolFields.OpenByName(i_SessionFields[i]);
								var FieldLabel = pIField.Label;

								var NewIndex = FieldsListView.AddItem(FieldLabel);
								FieldsListView.SetItemText(NewIndex, 1, GetStringForFieldTypeEnumValue(pIField.FieldType));
								FieldsListView.SetItemData(NewIndex, pIField);

								var NewResult = new FieldResult();
								NewResult.ViewNames = new Array();
								NewResult.pIField = pIField;
								g_FieldResultArray[i] = NewResult;
								i++;
							}


							// Insert view data into views list view.
							
							while (i_ViewNames.HasMore())
							{
								ViewsListView.AddItem(i_ViewNames.OpenNext());
								i++;
							}

							// Set initial selection of the fields list view.
