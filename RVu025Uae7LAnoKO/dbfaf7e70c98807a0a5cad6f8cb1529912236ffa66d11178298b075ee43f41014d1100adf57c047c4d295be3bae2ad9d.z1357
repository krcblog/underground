								{
									int_SetIsDirty(true);
									g_PreviewDocument.parentWindow.changeTabColumns(i_Item);
								}

								function StylesComboBox_OnSelectionChange(i_Name, i_Item)
								{
									if (g_pForm != null)
										g_pForm.StyleName = int_GetStyleName();

									OnFormSaved();
									InitFormPreview(g_pForm);
									int_SetIsDirty(true);
								}

								function PreviewSourceTabs_OnSelectionChanged(i_ID, i_KeyState)
								{
									if (i_ID == "PV")
										g_IsViewSource = false;
									else if (i_ID == "HT")
										g_IsViewSource = true;

									OnFormSaved();
									InitFormPreview(g_pForm);
								}

								function int_GetStyleName()
								{
									var index = StylesComboBox.GetSelectionIndex();
									var StyleName = "";
									if (index != -1)
										StyleName = StylesComboBox.GetItemData(index);
									return StyleName;
								}

