						function FieldsListView_OnListViewSelectionChanged(i_Name, i_Index, i_Selected)
						{
							if (i_Selected)
							{
								// Reset checks in the views list view.
								g_ClearingChecks = true;
								ViewsListView.ClearAllChecks();
								ViewsListView.EnableAllChecks();
								g_ClearingChecks = false;

								var CurrentFieldResult = g_FieldResultArray[i_Index];
								var FieldViewArray = CurrentFieldResult.ViewNames;

								// Disable views which already have an attachments field
								// if the selected field is an attachments field - can not have more then
								// one attachments field per view.
								if (CurrentFieldResult.pIField.FieldType == "Attachments")
								{
									var ViewsEnum = ViewsListView.OpenCheckEnabledEnum();
									while (ViewsEnum.HasMore())
									{
										var index = ViewsEnum.OpenNext();
										if (g_CallbackCode.DoesViewHaveAttachmentsFieldByViewName(ViewsListView.GetItemText(index)))
											ViewsListView.SetCheckEnabledByIndex(index, false);
									}
								}
								// Set the label text for the views list view.
								var FieldName = FieldsListView.GetItemText(i_Index);
								ViewsLabel.SetText("Add \"" + FieldName + "\" to view:");

								// Check each view in the views list view that the field has been added to.
								if (FieldViewArray.length >= 1)
								{
									var ViewsEnum = ViewsListView.OpenCheckEnabledEnum();
									var i = 0;
									var iCount = FieldViewArray.length;
									while (i < iCount)
									{
										while (ViewsEnum.HasMore())
										{
											var ViewIndex = ViewsEnum.OpenNext();
											var ViewText = ViewsListView.GetItemText(ViewIndex)
											if (FieldViewArray[i] == ViewText)
												ViewsListView.SetCheckByText(ViewText);
										}
										ViewsEnum.Reset();
										i++;
									}
								}
							}
						}

						function ViewsListView_OnListViewCheckChanged(i_Name, i_Index, i_Checked)
						{
							var SelectionIndex = FieldsListView.SelectionIndex;
							if (!g_ClearingChecks && SelectionIndex != -1)
							{
								var CurrentFieldResult = g_FieldResultArray[SelectionIndex];
								CurrentFieldResult.ViewNames = new Array();

								// Saved the checked views into the fields object array.
								var ViewsEnum = ViewsListView.OpenCheckEnabledEnum();
								var ViewCount = 0;
								while (ViewsEnum.HasMore())
								{
									var ViewIndex = ViewsEnum.OpenNext();
									if (ViewsListView.GetCheckState(ViewIndex))
									{
										CurrentFieldResult.ViewNames[ViewCount] = ViewsListView.GetItemText(ViewIndex);
										ViewCount++;
									}
								}

								var FieldName = CurrentFieldResult.pIField.Label;
								var FieldViews = CurrentFieldResult.ViewNames.join(", ");

								// Set the preview list view text for the selected field.
								var PreviewIndex = PreviewListView.FindStringExact(FieldName);
								if (FieldViews != "")
								{
									if (PreviewIndex == -1)
										PreviewIndex = PreviewListView.AddItem(FieldName);
									PreviewListView.SetItemText(PreviewIndex, 1, FieldViews);
								}
								// Remove the field from the preview if no views are selected.
								else if (PreviewIndex != -1)
									PreviewListView.RemoveItemByIndex(PreviewIndex);
							}
						}

