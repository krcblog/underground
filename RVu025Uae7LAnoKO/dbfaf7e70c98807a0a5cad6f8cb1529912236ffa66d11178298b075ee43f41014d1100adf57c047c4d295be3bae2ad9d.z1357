									delete RTFHelpProvider;
									CollectGarbage();
								}
							}

							function InitObjectManager(i_pIObjectEnum, i_Type)
							{
								ResetGlobals();

								g_ObjectType = i_Type;

								var blnHideGroupButtons = true;

								// Set the correct labels and tooltips for the object type.
								if (g_ObjectType == GrooveFormsToolDesignObjectType_Script)
								{
									AddObjectButton.Tooltip = "Add a new script from scratch.";
									BrowseObjectButton.Tooltip = "Browse to add a .js or .vbs script file.";
									AddedObjectsHelpStatic.SetText("Click 'New...' to add a new script from scratch, or click 'Browse...' to browse for a script file.");
								}
								else if (g_ObjectType == GrooveFormsToolDesignObjectType_Style)
								{
									AddObjectButton.Tooltip = "Add a new, blank style";
									BrowseObjectButton.Tooltip = "Browse to add a .css file.";
									AddedObjectsHelpStatic.SetText("Click 'New...' to add a new style from scratch, or click 'Browse...' to browse for a style file.");
								}
								else if (g_ObjectType == GrooveFormsToolDesignObjectType_FieldGroup)
								{
									AddObjectButton.Tooltip = "Add a new field group";
									ModifyObjectButton.Tooltip = "Edit the currently selected field group";
									DeleteObjectButton.Tooltip = "Delete the currently selected field group";
									AddedObjectsHelpStatic.SetText("Click 'New...' to add a new field group from scratch, click 'Modify...' to modify a selected field group, or click 'Delete' to delete a selected field group.");
									blnHideGroupButtons = false;
								}
								else if (g_ObjectType == GrooveFormsToolDesignObjectType_TabGroup)
								{
									AddObjectButton.Tooltip = "Add a new tab group";
									ModifyObjectButton.Tooltip = "Edit the currently selected tab group";
									DeleteObjectButton.Tooltip = "Delete the currently selected tab group";
									AddedObjectsHelpStatic.SetText("Click 'New...' to add a new tab group from scratch, click 'Modify...' to modify a selected tab group, or click 'Delete' to delete a selected tab group.");
									blnHideGroupButtons = false;
								}

								// Display the correct buttons depending on the type of object.
								AddRemoveCellContents("BrowseObjectButton", AddObjectCodeInnerLayout, !blnHideGroupButtons);
								AddRemoveCellContents("ModifyObjectButton", AddObjectCodeInnerLayout, blnHideGroupButtons);
								AddRemoveCellContents("DeleteObjectButton", AddObjectCodeInnerLayout, blnHideGroupButtons);
								AddRemoveMultiCellContents("OKButton", AddObjectCodeControlLayout, !blnHideGroupButtons);

								if (blnHideGroupButtons)
									CancelButton.Label = "Cancel";
								else
									CancelButton.Label = "Done";

								// Add all existing objects to the list view.
								while (i_pIObjectEnum != null && i_pIObjectEnum.HasMore())
								{
									var pIFormsToolObject = i_pIObjectEnum.OpenNext();
									var ObjectName = pIFormsToolObject.IGrooveFormsToolDesignObject.Name;
									if (g_ObjectType == GrooveFormsToolDesignObjectType_Script)
									{
										if (!IsSystemScript(ObjectName))
										{
											var ScriptLanguage = pIFormsToolObject.Language;
											var ScriptCode = pIFormsToolObject.ScriptCode;

											AddedObjectsListView.AddItem2(ObjectName, pIFormsToolObject, GROOVE_NULL, -1);
										}
									}
									else
									{
										AddedObjectsListView.AddItem2(ObjectName, pIFormsToolObject, GROOVE_NULL, -1);
									}
								}
							}

