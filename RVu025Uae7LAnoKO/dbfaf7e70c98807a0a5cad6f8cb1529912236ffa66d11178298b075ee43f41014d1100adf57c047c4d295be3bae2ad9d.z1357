									if (typeof Property != "undefined" && Property != null && Property.Name == i_Name)
									{
										var PropertyInitialValueType = i_PropertyArray[i].PropertyInitialValueType;
										if (typeof PropertyInitialValueType == "undefined" || PropertyInitialValueType == null || (typeof PropertyInitialValueType.toString != "undefined" && PropertyInitialValueType.toString() == ""))
