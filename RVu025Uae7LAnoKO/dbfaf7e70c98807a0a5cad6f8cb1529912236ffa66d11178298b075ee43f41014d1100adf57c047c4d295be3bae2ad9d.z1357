								function SetCurrentField(i_Field, i_bCallback)
								{
									if (typeof i_Field != "undefined" && i_Field != null)
									{
										if (i_Field.Type == 2) // FormObjectType_Tab defined in form.js.
										{
											FieldNameLabel.SetText("No Field Selected");
											FieldColumnSpanComboBox.SetSelectionByIndex(-1);
											FieldColumnSpanComboBox.Enabled = false;
											FieldRowSpanComboBox.SetSelectionByIndex(-1);
											FieldRowSpanComboBox.Enabled = false;

											TabTextLabel.SetText(i_Field.Name);
											TabColumnsComboBox.Enabled = true;
											TabColumnsComboBox.SetSelectionByText(i_Field.NumberOfColumns);
										}
										else
										{
											TabTextLabel.SetText("No Tab Selected");
											TabColumnsComboBox.SetSelectionByIndex(-1);
											TabColumnsComboBox.Enabled = false;

											g_CurrentField = i_Field;
											FieldNameLabel.SetText(i_Field.Name);

											var Columns = 1;
											if (i_Field.Tab != null)
												Columns = parseInt(i_Field.Tab.NumberOfColumns);
											else
												Columns = parseInt(FormColumnsComboBox.GetSelectionText());

											if (!isNaN(Columns))
											{
												if (Columns == 1)
												{
													FieldColumnSpanComboBox.ResetContent();
													var ColumnSpanIndex = FieldColumnSpanComboBox.AddItem("1");
													FieldColumnSpanComboBox.SetSelectionByIndex(ColumnSpanIndex);
													FieldColumnSpanComboBox.Enabled = false;

													FieldRowSpanComboBox.SetSelectionByIndex(0);
													FieldRowSpanComboBox.Enabled = false;
												}
												else if (Columns > 1)
												{
													FieldColumnSpanComboBox.ResetContent();
													for (var i = 1; i <= Columns; i++)
													{
														var ColumnSpanIndex = FieldColumnSpanComboBox.AddItem(i);
													}
													FieldColumnSpanComboBox.SetSelectionByText(i_Field.ColumnSpan);
													FieldColumnSpanComboBox.Enabled = true;

													FieldRowSpanComboBox.SetSelectionByText(i_Field.RowSpan);
													FieldRowSpanComboBox.Enabled = true;
												}
											}
											else
											{
												FieldColumnSpanComboBox.Enabled = true;
												FieldColumnSpanComboBox.SetSelectionByText(i_Field.ColumnSpan);
												FieldRowSpanComboBox.Enabled = true;
												FieldRowSpanComboBox.SetSelectionByText(i_Field.RowSpan);
											}
											// Disable the button for field and tab groups.
											// FormObjectType_FieldGroup and FormObjectType_TabGroup defined in form.js.
											if (i_Field.Type == 0 || i_Field.Type == 1)
												MorePropertiesButton.Enabled = false;
											else
												MorePropertiesButton.Enabled = true;
										}

										if (i_bCallback)
											g_CallbackCode.SetCurrentField(i_Field.Name);
									}
									else
									{
										DisableComboBoxes();
										g_CurrentField = null;
									}
								}

								function ClearCurrentField()
								{
									FieldNameLabel.SetText("No Field Selected");
									TabTextLabel.SetText("No Tab Selected");
									DisableComboBoxes();

									g_CurrentField = null;
								}

