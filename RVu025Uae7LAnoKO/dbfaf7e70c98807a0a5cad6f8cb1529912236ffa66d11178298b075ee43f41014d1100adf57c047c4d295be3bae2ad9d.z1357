										if (ScriptName.lastIndexOf(".") == -1)
										{
											if (ScriptLanguage == "JavaScript")
												ScriptName += ".js";
											else if (ScriptLanguage == "VBScript")
												ScriptName += ".vbs";
										}
										*/
										
										// Get the script code depending on the script type.
