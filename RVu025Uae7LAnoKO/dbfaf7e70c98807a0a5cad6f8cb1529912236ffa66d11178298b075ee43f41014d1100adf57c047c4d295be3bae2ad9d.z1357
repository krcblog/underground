							function UpdateDisplayString(i_Index)
							{
								var ItemData = ConditionsListView.GetItemData(i_Index);
								var DisplayString = "";
								var PreviousItemData = null;
								var NumPrefixParens = ItemData.StartLevel;
								if (i_Index != 0)
								{
									PreviousItemData = ConditionsListView.GetItemData(i_Index - 1);
									NumPrefixParens = (ItemData.StartLevel - PreviousItemData.EndLevel);
								}
								
								
								var NumSuffixParens = ItemData.StartLevel - ItemData.EndLevel ;
								
																
								// Add the Parens								
								for (var i = 0; i < NumPrefixParens; i++)
								{
									DisplayString = "( " + DisplayString;
								}
								
								DisplayString += ItemData.Statement;
								
								for (var i = 0; i < NumSuffixParens; i++)
								{
									DisplayString = DisplayString + " )";
								}
								
								
								if (i_Index != (ConditionsListView.Count - 1))
								{
									var Op;
									if (ItemData.Operator != "")
										Op = ItemData.Operator.toUpperCase() + " ";
									else
										Op = GetRadioValue().toUpperCase();
									
									DisplayString += " " + Op;
								}							
									
								ConditionsListView.SetItemText(i_Index, 0, DisplayString);
							}
							
