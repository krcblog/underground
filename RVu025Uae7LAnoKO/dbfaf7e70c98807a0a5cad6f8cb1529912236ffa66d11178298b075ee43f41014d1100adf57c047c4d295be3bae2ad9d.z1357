							function SetupComboboxValues(i_Combobox, i_Value, i_DefaultArray, i_OptionalValue)
							{
								i_Combobox.ResetContent();
								var i = 0;
								var iCount = i_DefaultArray.length;
								while (i < iCount)
								{
									var NewValue = i_DefaultArray[i].replace(/,/g, i_Value);
									if (i_OptionalValue)
										NewValue = NewValue.replace(/\$/g, i_OptionalValue);
									i_Combobox.AddItem(NewValue);
									i++;
								}
								i_Combobox.Redraw();
							}

