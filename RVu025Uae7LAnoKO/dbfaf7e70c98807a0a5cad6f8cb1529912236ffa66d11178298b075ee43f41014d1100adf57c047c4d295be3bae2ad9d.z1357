							function VerifyParens()
							{
								var Query = DisplayQuery.GetText();
								var OpenParenArray = Query.match(/\(/g);
								var CloseParenArray = Query.match(/\)/g);
								var Message = "Syntax Error: There are more ";
								var OpenNum = 0;
								var CloseNum = 0;
								if (OpenParenArray != null)
									OpenNum = OpenParenArray.length;
								
								if (CloseParenArray != null)
									CloseNum = CloseParenArray.length;
								
								if(OpenNum > CloseNum)
									Message += "'(' than ')'. ";
								else if (OpenNum < CloseNum)
									Message += "')' than '('. ";
								else if (OpenNum == CloseNum)
									return;
										
								Message += " Please correct before saving.";
								
								g_UIDelegate.DisplayError(Message);																	
							}

