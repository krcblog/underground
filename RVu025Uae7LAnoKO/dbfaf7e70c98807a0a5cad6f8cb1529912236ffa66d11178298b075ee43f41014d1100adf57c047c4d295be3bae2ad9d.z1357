							function AcceptChangesApply()
							{
								try
								{
									ValidateField();

									g_ApplyButtonUsed = true;
									g_PreviousFieldType = "";
									g_WizardPage = 1;
									g_PropertyInfoInitialized = false;
									UpdateFieldInfo();
									// Reset all saved data for adding a new field.
									SetFieldType(null, true);
									SetEditableState(true);
									SetFieldTypeListBoxSelection(-1);
									SetFieldName("");
									SetFieldLabel(null);
									ResetPropertyVariables();
									ClearFieldPropertyInstructions();
									// Reset the page title because you are creating a new field.
									SetPageTitle("Create New Field");
									// Set proper layouts for adding a new field.
									FieldTypeGeneralDescriptionViewContainer.LayoutByName = "FieldTypeGeneralDescriptionLayout";
									BackNextButtonViewContainer.LayoutbyName = "BackNextButtonLayout";
									InnerViewContainer.LayoutByName = "InnerLayout";
									// Disable the back and next buttons.
									BackButton.Enabled = false;
									NextButton.Enabled = false;
								}
								catch (error)
								{
									DisplayOKMessageBox(error.description, "Invalid Field Label");
									SetPropertiesListBoxSelection(0);
									PropertyValueViewContainer.SetFocus("FieldLabelValueTextView");
								}
							}

							function AcceptChangesOK()
							{
								try
								{
									if (!g_ApplyButtonUsed)
									{
										ValidateField();
									}
									else if (g_WizardPage == 1 || (g_WizardPage == 2 && GetFieldLabel() == "" && g_PropertyLabelFound))
									{
										CancelChanges();
										return;
									}

									g_PreviousFieldType = "";
									g_WizardPage = 1;
									g_PropertyInfoInitialized = false;
									UpdateFieldInfo();
									ResetPropertyVariables();
									// Close the field wizard window.
									var Window = UIPropertyList.OpenProperty(UIPROPERTY_TOP_LEVEL_WINDOW);
									Window.Close(GrooveWindowResult_OK);
									Window.Destroy(GrooveWindowResult_OK);
								}
								catch (error)
