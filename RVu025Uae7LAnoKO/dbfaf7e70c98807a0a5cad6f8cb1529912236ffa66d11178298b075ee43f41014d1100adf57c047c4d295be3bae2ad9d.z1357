								PropertyInstructionsLabel.SetText(g_UIDelegate.IGrooveFormsToolUIDelegatePrivate.GetResourcedString(IDS_FORMS_TOOL_SUBFORMS_GRP_FLD_WIZ_PROP_SETTING));
								PropertyInstructionsFieldTypeLabel.SetText(i_FieldTypeName);
							}

