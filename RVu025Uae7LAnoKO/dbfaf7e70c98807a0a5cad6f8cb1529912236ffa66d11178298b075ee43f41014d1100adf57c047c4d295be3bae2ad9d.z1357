								MacroNameEdit.Text = "";
								MacroLanguageComboBox.SetSelectionByIndex(0);
								MacroScopeComboBox.SetSelectionByIndex(2);
								MacroCodeTextView.TextContent.PlainText = "";	
								
								// now init from the instance
								MacroNameEdit.Text = i_Instance.Name;
								MacroLanguageComboBox.SetSelectionByText(i_Instance.Language);
								MacroScopeComboBox.SetSelectionByIndex(g_CurrentMacro.Scope);
								g_Instance = i_Instance;

								// Always display the global script block by default.
								g_SystemScript = i_Instance.ScriptCode;
