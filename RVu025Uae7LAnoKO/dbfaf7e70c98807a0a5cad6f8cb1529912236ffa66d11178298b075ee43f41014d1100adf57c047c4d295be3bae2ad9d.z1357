							function ConvertToEscapedLookupString(i_LookupString)
							{
								var QueryString = "";

								var NameValueArray = i_LookupString.split("&");
								for (var i = 0; i < NameValueArray.length; i++)
								{
									var NameValuePair = NameValueArray[i].split("=");
									if (NameValuePair != "")
									{
										var Name = ConvertToOriginalName(NameValuePair[0]);
										var Value = ConvertToOriginalName(NameValuePair[1]);

										if (QueryString != "")
											QueryString += "&";

										QueryString += escape(Name) + "=" + escape(Value);
									}
								}

								return QueryString;
							}
							
							function ConvertToXMLLookupString(i_LookupString)
							{
								var QueryString = "";

								var NameValueArray = i_LookupString.split("&");
								for (var i = 0; i < NameValueArray.length; i++)
								{
									var NameValuePair = NameValueArray[i].split("=");
									if (NameValuePair != "")
									{
										var Name = unescape(NameValuePair[0]);
										var Value = unescape(NameValuePair[1]);

										if (QueryString != "")
											QueryString += "&";

										QueryString += CreateValidXMLName(Name) + "=" + CreateValidXMLName(Value);
									}
								}

								return QueryString;
							}

