								{
									var GroupLabel = GroupLabelTextView.TextContent.PlainText;
									GroupLabel = GroupLabel.replace("\r", "");
									GroupLabel = GroupLabel.replace("\n", " ");
									return TrimEnds(GroupLabel);
								}

