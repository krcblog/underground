						function Initialize()
						{
							// setup the tabs
							Tabs.AppendTab(BASICS_TAB_NAME, BASIC_TAB_URL);
							Tabs.AppendTab(OPTIONS_TAB_NAME, OPTIONS_TAB_URL);
							Tabs.AppendTab(SORTS_TAB_NAME, SORTS_TAB_URL);
							var DisplayText = DisplayValueLabel.GetText();
							var StoredText = StoredValueLabel.GetText();
							DisplayValueLabel.SetText(StoredText);
							StoredValueLabel.SetText(DisplayText);
							// Add the dynamic locale centric items
							DateFormatCombobox.AddItem(DATE_AND_TIME_AS_SHORT);
							DateFormatCombobox.AddItem(DATE_AND_TIME_AS_MEDIUM);
							DateFormatCombobox.AddItem(TIME_AS_SHORT);
							DateFormatCombobox.AddItem(TIME_AS_MEDIUM);
						}

