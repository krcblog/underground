							{
								UpdateTitle();
							}

							function CalendarView_OnCalendarCommand(i_Command)
							{
								if (i_Command == GrooveCalendarCommand_NewEntry)
									CloseWindow();
							}

							function TimeComboBox_OnDropDown(i_Name)
							{
								ValidateTime();
								FillTimeCombo(TimeComboBox);

								if (-1 == TimeComboBox.FindStringExact(0, TimeComboBox.Text))
								{
									// Insert the selected time at the appropriate point if it doesn't exist.
									var Start = CalendarView.SelectionStart;
									var DayStart = StartOfDay(Start);
									var NumMillis = EndOfDay(Start) - DayStart + 1;
									var MillOffset = Start - DayStart;
									var Index = Math.floor((MillOffset * TimeComboBox.Count) / NumMillis) + 1;
									var AddedIndex = TimeComboBox.InsertItem(Index, TimeComboBox.Text);
									TimeComboBox.SetSelectionByIndex(AddedIndex);
								}
							}

							function TimeComboBox_OnSelectionChange(i_Name, i_Item)
							{
								TimeComboBox.Text = i_Item;
								ValidateTime();
							}

							function TimeComboBox_OnComboBoxKillFocus(i_Name)
							{
								ValidateTime();
							}

							function OKButton_OnCommand(URL, Data)
							{
								CloseWindow();
