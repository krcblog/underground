											Columns = 1;

										g_CallbackCode.ApplyLayoutChanges(FieldReferenceEnum.IGrooveUnknownEnum, FieldGroupsEnum.IGrooveBSTREnum, TabGroupsEnum.IGrooveBSTREnum, Columns);
									}
								}

								g_IsDirty = false;
							}

