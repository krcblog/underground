						function AddTabButton_OnUpdateCommandState(i_UICommand)
						{
							if (g_IsEditingTab)
								i_UICommand.Enabled = false;
							else
								i_UICommand.Enabled = true;
						}

						function ModifyTabButton_OnUpdateCommandState(i_UICommand)
						{
							if (g_IsEditingTab || TabsListBox.GetSelectionIndex() == -1)
								i_UICommand.Enabled = false;
							else
								i_UICommand.Enabled = true;
						}

						function DeleteTabButton_OnUpdateCommandState(i_UICommand)
						{
							if (g_IsEditingTab || TabsListBox.GetSelectionIndex() == -1)
								i_UICommand.Enabled = false;
							else
								i_UICommand.Enabled = true;
						}

						function TabOKButton_OnUpdateCommandState(i_UICommand)
						{
							if (g_IsEditingTab)
								i_UICommand.Enabled = true;
							else
								i_UICommand.Enabled = false;
						}

						function TabCancelButton_OnUpdateCommandState(i_UICommand)
						{
							if (g_IsEditingTab)
								i_UICommand.Enabled = true;
							else
								i_UICommand.Enabled = false;
						}

						function OKButton_OnUpdateCommandState(i_UICommand)
						{
