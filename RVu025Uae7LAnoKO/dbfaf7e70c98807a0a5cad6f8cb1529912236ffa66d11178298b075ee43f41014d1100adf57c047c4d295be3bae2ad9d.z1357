												{
													DisplayOKMessageBox(Priv.GetResourcedString(IDS_FORMS_TOOL_SUBFORMS_GRP_FLD_WIZ_MIN_DATE_ERR), Priv.GetResourcedString(IDS_FORMS_TOOL_SUBFORMS_GRP_FLD_WIZ_MIN_DATE_ERR_TITLE));
