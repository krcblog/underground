								if (typeof LookupString != "undefined" && LookupString != null && LookupString != "")
								{
									LookupString = ConvertToXMLLookupString(LookupString);
									var LookupTargetDisplayName = GenerateLookupTargetDisplayName(LookupString);
									var Comment = GetValueFromQueryString("Comment", LookupString);
									i_Field.SetViewColumnLookupSettings(i_Field.IGrooveFormsToolDesignObject.Name, LookupString, LookupTargetDisplayName, Comment);
								}
								else
									i_Field.RemoveViewColumnLookupSettings();
							}

