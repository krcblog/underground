											return true;
									}
									else if (g_MinMaxType == "Date")
									{
										if (ValidateDate(MinValue))
										{
											if (MaxValue != "")
											{
												var MinDate = ParseDateOrDateTime(MinValue, GrooveIntlDateFormatStyle_Short, GrooveIntlDateFormatStyle_Short);
