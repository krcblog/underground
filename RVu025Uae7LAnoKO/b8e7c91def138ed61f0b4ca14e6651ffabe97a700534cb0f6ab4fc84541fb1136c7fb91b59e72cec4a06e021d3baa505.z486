class RFile(object):
    seen = {}

    def __init__(self, name):
        self.name = name

    __hash__ = None

    def __eq__(self, other):
        if other in self.seen:
            text = self.seen[other]
        else:
            text = self.seen[other] = other.read()
            other.close()
        if not isinstance(text, str):
            text = text.decode('ascii')
        return self.name == other.name == text


